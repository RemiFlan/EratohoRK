;///////////////////////
;ACT50, 손으로 애무를 강요//
;///////////////////////
[SKIPSTART]
	#애무強制/수음強制/페니스밴드수음強制
	#죠교대상(MASTER)に、손による애무를 강요する
	#죠교자に페니스があると必ず수음強制になる(페니스があると통상の애무強制は起こらない)
	#죠교자が페니스밴드장착中は페니스밴드수음強制になる
	#TFLAG:징계・플래그が立っていると発生しない
	#턴방침が"하드", "이상"だと発生しない
	#밀어넘어지는중には発生しない
	#TEQUIP(밧줄/삼각목마)장착中には発生しない
	#성교중/성교봉사중には発生しない
	#죠교대상が従うか否かはこの時点では未定
	#REACT분류が거부/날뛴다/逃げるの場合は拒絶扱いとなる

	*地の文はACT파생毎に分岐。各ACT파생内では分岐なし
	*ACT파생0,애무の時のみ쾌Ｂ/쾌ＡSOURCE増加
	*その他の場合は쾌Ｃ限定
[SKIPEND]
@KOJO_MALE_ACT_K39_50 ;TFLAG:ACT파생(0=애무強制/1=수음強制/2=페니스밴드수음強制)
#LOCALSIZE  1
#LOCALSSIZE 1
VARSET LOCAL
VARSET LOCALS

;ACT파생1と2は考慮しない
SIF GROUPMATCH(TFLAG:ACT파생, 1, 2)
	RETURN 0

;ACT50_0, 애무強制 //「自分の성기を손으로 애무しろと命令する」
;=================
IF !TFLAG:ACT파생
	;연속실행
	;========
	IF TFLAG:ACT == TFLAG:전ACT
		;죠교자がイキそう
		;================
		IF TCVAR:TARGET:가버려
			;호의、호감도で派生
			IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
				CALL HTMLPRINTW(@"「ひゃぁっ！ そんな…指でッ！ 摘んじゃ、ダメェ%HTML_PS2_K39()%」")
				CALL HTMLPRINTW(@"「イっちゃう%HTML_AH_K39()%　イっちゃうからぁ！ あぁ、だめぇぇっ…ッ…！」")
			ELSEIF AFFECTION("익애")
				{
				CALL HTMLPRINTW(
				@"「んっ%HTML_AH_K39()%　あぁっ%HTML_AH_K39()%" + 
				@"　そんなとこまで…弄られ…ッ…%HTML_PS2_K39()%」")
				}
;OriginalString : 「%CNAME(MASTER, TARGET)%に、触られて……私、こんなにっ……！」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%에, 손대어져……나, 이렇게……！」
			ELSE
				CALL HTMLPRINTW(@"「んっ%HTML_AH_K39()%　あっ%HTML_AH_K39()%　私、もう……」")
				CALL HTMLPRINTW(@"「もう、少しで…ッ…！ あぁっ%HTML_PS2_K39()%」")
			ENDIF
			RETURN 1
		ENDIF

		;その他
		;======
		;호의、호감도で派生
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
			CALL HTMLPRINTW(@"「あん%HTML_AH_K39()%　あっ%HTML_AH_K39()%　あぁっ！ なんか激しいよぉ…ひぁっ！」")
			CALL HTMLPRINTW(@"「や、弱いとこばっかり……弄るんだか、らぁ…ッ%HTML_PS2_K39()%」")
		ELSEIF AFFECTION("익애")
			CALL HTMLPRINTW(@"「はっ、んんっ！ いやらしい손つきですね……あぁっ%HTML_PS2_K39()%」")
			CALL HTMLPRINTW(@"「いつのまに、こんなに……っふあ%HTML_AH_K39()%　そこ、弄られたらッ！」")
		ELSE
			CALL HTMLPRINTW(@"「んっ%HTML_AH_K39()%　上손、ですよ……」")
			CALL HTMLPRINTW(@"「あっ%HTML_AH_K39()%　そこ、もっと触ってください……」")
		ENDIF
		RETURN 1
	ENDIF

	;성교봉사중 //TEQUIP:성교봉사중(정상위/대면좌위/배면좌위)かつ죠교자が"정욕"かつTCVAR:가버려が立っている場合限定で発生
	;========== //후배위/역강간/Ａ섹스では発生しない
	IF TEQUIP:성교봉사중
		LOCAL :0 = GET_KFLAG_K39("상사상애", (FLAG:주인님))
		LOCALS:0 '= PALAM:쾌Ｖ > 5000 ? "ッ！" # "…" 
		{
		CALL HTMLPRINTW(
		@"「あ\@ PALAM:쾌Ｖ > 5000 ? ゛ # \@……ああ……あ\@ PALAM:쾌Ｖ > 5000 
		? ゛ # \@っ、あ\@ PALAM:쾌Ｖ > 5000 ? ゛あぁ # \@っ%HTML_PS2_K39()%」")
		}
		{
;OriginalString : 「\@ LOCAL:0 
		PRINTFORMW 「\@ LOCAL:0 
		? ね、ねぇ # あ、あの 
		\@……%RAND_SPLIT(
		@"お願い\@ !LOCAL:0 ? です # \@。こっちも、弄って\@ !LOCAL:0 ? ください # \@…%LOCALS:0%" + 
		@"/ココが切ない\@ LOCAL:0 ? の # んです \@…%LOCALS:0%　ちょっと激しくシても、いい\@ !LOCAL:0 ? です # \@から……")%」
		}
		RETURN 1
	ENDIF

	;기타・조건없음 //確率で汎用アクションへ
	;================
	IF 구상記入チェック(50)
		;호의、호감도で派生
		IF GET_KFLAG_K39("상사상애", (FLAG:주인님))
;OriginalString : %CALLNAME%は%CALLNAME:MASTER%의 손を取り自らの身体に押し当てた…
			PRINTFORMDW %CALLNAME%(은)는 %CALLNAME:MASTER%의 수를 잡아 스스로의 신체에 꽉 눌렀다…
;OriginalString : 「ね、分かるでしょ……もうこんなに熱くなってるの…」
			PRINTFORMW 「그런데, 알겠죠……이제 이렇게 뜨거워져…」
;OriginalString : 「触って…弄り回して…何もかも忘れさせて……」
			PRINTFORMW 「손대어…만지작거려 돌려…모두 잊게 해……」
		ELSEIF AFFECTION("익애")
;OriginalString : 「私、%CNAME(MASTER, TARGET)%に触ってほしくて、もう참다できないんです…」
			PRINTFORMW 「 나,%CNAME(MASTER, TARGET)%에 손대기를 원해서, 이제 참을 수 없습니다…」
;OriginalString : 「お願いです………%CNAME(MASTER, TARGET)%の손で、私を溶かして……」
			PRINTFORMW 「부탁입니다………%CNAME(MASTER, TARGET)%의 손으로, 나를 녹여……」
		ELSE
;OriginalString : 「私の体はどうでしょう。%CNAME(MASTER, TARGET)%から見て魅力的でしょうか？」
			PRINTFORMW 「 나의 몸은 어떻습니까.%CNAME(MASTER, TARGET)%(으)로부터 봐 매력적입니까？」
;OriginalString : 「遠慮はいりませんよ。好きなだけ触ってくださいね…」
			PRINTFORMW 「사양은 필요 없습니다. 좋아할 뿐(만큼) 손대어 주세요…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF


