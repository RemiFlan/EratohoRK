				{
				CALL HTMLPRINTW(
				@"「だめっ、だめぇ%HTML_AH_K39()%　イっちゃうっ" + 
				@"%HTML_AH_K39()%　イっちゃうからぁ…ッ…！」")
				}
			ELSEIF AFFECTION("익애")
				{
				CALL HTMLPRINTW(
				@"「んぁっ%HTML_AH_K39()%　あっ%HTML_AH_K39()%　" + 
				"はぁっ…だ、だめですっ！ そんなに했다ら…」")
				}
