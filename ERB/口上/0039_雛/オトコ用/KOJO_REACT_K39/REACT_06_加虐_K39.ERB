@PRINT_KOJO_REACT_K39_COMMON_260
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC STRING
#DIMS DYNAMIC AHEART
#DIMS DYNAMIC MSTR
FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
AHEART    '= HTML_AH_K39()
STRING    '= RAND_SPLIT("ふふ/うふ/ふふっ/うふっ/くすっ/クスクス") + "…"
MSTR      '= CNAME(MASTER, TARGET)

;죠교대상の마조끼5以上かつ호의호감도が一定以上 // 25%の確率でネストを抜け下層に流れるように
IF ABL:MASTER:마조끼 >= 6 && FAV_RATING && PERCENT(75)
	SELECTCASE AUTORAND(2, 265)
		CASE 0
;OriginalString : 「そんな声聴かされると、ゾクゾクしちゃって……」
			PRINTFORMW 「그런 성청 빌려주어지면(자), 두근두근 해 버려서……」
			;ACT볼개그でない場合は派生
			{
			CALL HTMLPRINTW(@"「もうっ、堪らなくなっちゃう%HTML_AH2_K39()% \@ 
			!IS_NOWACTNAME("볼개그") ? %STRING% # \@」")
			}
			;ACT볼개그の場合
			SIF IS_NOWACTNAME("볼개그")
				CALL HTMLPRINTW("「ほら、涎が垂れてるわよ%HTML_AH2_K39()% %STRING%」")
		CASE 1
;OriginalString : 「甘い声で鳴いちゃって、もうどうしようもないわね……」
			PRINTFORML 「단 소리로 울어 버려서, 이제 어쩔 수 없네요……」
;OriginalString : 「――なんでこんな人を好きなったのかしら」
			PRINTFORMW 「――어째서 이런 사람을 좋아해 되었는지 해들 」
			{
			CALL HTMLPRINTW(@"「\@ RAND:3 
			? とか、言ってみたりして…%HTML_AH2_K39()% %STRING% 
			# でもこうなったのも、私にも責任があるのよね… \@」")
			}
	ENDSELECT
	RETURN 1
ENDIF

