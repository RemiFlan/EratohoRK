			{
			CALL HTMLPRINTW(@"「\@ FAV_RATING 
			? ほら # さあ 
			\@、もっと鳴いて\@ FAV_RATING 
			? ？ # … 
			\@もっと私を愉しませて\@ !FAV_RATING ? ください # \@%SYMBOL%」")
			}
		CASE 1
;OriginalString : 「そんな声で鳴かれると興奮し\@ FAV_RATING ? ちゃうんだけど # てしまいます \@…」
			PRINTFORMW 「그런 소리로 울어지면(자) 흥분해\@ FAV_RATING ? 낳았지만 # 라고 끝냅니다 \@…」
			{
;OriginalString : 「\@ FAV_RATING 
			PRINTFORMW 「\@ FAV_RATING 
			? ねぇ# あの 
			\@それ…、わざとやって\@ FAV_RATING 
			? るんじゃないの # ませんか \@？」
			}
		CASE 2
