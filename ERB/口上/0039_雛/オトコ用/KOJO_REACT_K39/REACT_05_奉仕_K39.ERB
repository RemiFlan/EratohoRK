;/////////////////////////////////////////
;REACT50, 손으로 애무를 강요・個別リアクション
;/////////////////////////////////////////
[SKIPSTART]
想定されるCOM…
	애무系反応
		COM:부끄러워한다
	봉사系反応
		COM:거부한다/소극적으로 한다/적극적으로 한다/애정을 담아 한다/태연하게 한다
	その他反応
		COM:자위를 시작한다

・地の文はREACT분류とACT파생で分岐
・REACT분류:受け入れるはREACTION_MESSAGEで処理される
・REACT분류が消極的従う/平然と従う/積極的従う/受け入れる/용서를 빌다である場合は受容扱い
・REACT분류が거부/날뛴다/逃げるの場合は拒絶했다扱いになる
[SKIPEND]
@KOJO_MALE_REACT_K39_50 ;TFLAG:ACT파생(0=애무強制/1=수음強制/2=페니스밴드수음強制)
;変数宣言と代入
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC STRING
#DIMS DYNAMIC AHEART
#DIMS DYNAMIC RHEART
#DIMS DYNAMIC MSTR
#DIMS DYNAMIC YOU
#DIMS DYNAMIC TGT
FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
AHEART    '= HTML_AH_K39()
RHEART    '= RAND:3 ? HTML_HEARTB(, COLOR2_K39("PINK")) # HTML_HEART(, COLOR2_K39("PINK"))
STRING    '= RAND_SPLIT("ふふ/うふ/ふふっ/うふっ/くすっ/クスクス") + "…"
MSTR      '= CNAME(MASTER, TARGET)
YOU       '= CALLNAME:MASTER
TGT       '= CALLNAME:TARGET


;REACT50, 자위를 시작한다/부끄러워한다/싫어한다 // REACTで完結시킨다
;=========================================
IF IS_NOWCOMNAME("자위를 시작한다/부끄러워한다/싫어한다")
	CALL TEXTDISPLAY_CATEGORY_5_REJECT_K39
	RETURN 1
ENDIF

;REACT50, 소극적으로 한다(50%) // 負荷2、派生1の場合は弾く
;========================
IF 구상記入チェック(50) && IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT부하 < 2 && TFLAG:REACT파생 < 1
	IF 0
	ELSEIF FAV_RATING
;OriginalString : 「や…だめっ、こんな……焦ら당했다ら…」
		PRINTFORMW 「이나…안돼, 이런……초조하게 해지면(자)…」
;OriginalString : 「ねぇ、もっと…もっと触ってよ……」
		PRINTFORMW 「응, 좀 더…좀 더 손대어……」
	ELSEIF AFFECTION("익애")
;OriginalString : 「あっ…ん…嫌、焦らさないで……」
		PRINTFORMW 「아…응…싫고, 초조하게 하지 마……」
;OriginalString : 「もどかしくて、おかしくなってしまいます……」
		PRINTFORMW 「안타깝고, 이상해져 버립니다……」
	ELSEIF AFFECTION("眷愛")
;OriginalString : 「んっ…焦らされてるみたいで、切なくなってしまいます…」
		PRINTFORMW 「응…초조하게 해지고 있는 것 같아, 안타깝게 되어 버립니다…」
	ELSEIF AFFECTION("嫌悪")
;OriginalString : 「あの、もっと真面目にやってもらえますか？」
		PRINTFORMW 「저, 좀 더 성실하게 해 줄 수 있습니까？」
	ELSE
;OriginalString : 「私に触るのは嫌ですか…？」
		PRINTFORMW 「 나에게 손대는 것은 싫습니까…？」
	ENDIF
	RETURN 1
ENDIF

;REACT50, 적극적으로 한다(50%) // 負荷2は弾く
;========================;
IF 구상記入チェック(50) && IS_NOWCOMNAME("적극적으로 한다") && TFLAG:REACT부하 < 2
	IF FAV_RATING
		CALL HTMLPRINTW(@"「あっ%AHEART% あぁっ%AHEART% ふぁっ%AHEART% いいよぉ…ッ…！」")
;OriginalString : 「ねぇ、もっとっ！ もっとシてぇっ！」
		PRINTFORMW 「응, 좀 더！ 좀 더 시라고！」
	ELSEIF AFFECTION("익애")
		CALL HTMLPRINTW(@"「はっ…あっ%AHEART% あんっ%AHEART% いいです、気持ちいいですよ」")
		CALL HTMLPRINTW(@"「んっ%AHEART% もっと、もっとシてください…」")
	ELSEIF AFFECTION("眷愛")
		CALL HTMLPRINTW(@"「あっ%AHEART% んんっ%AHEART% 적극적으로シてくれるんですね」")
;OriginalString : 「いいですよ…もっと、%MSTR%の好きなように…」
		PRINTFORMW 「좋아요…좀 더,%MSTR%(이)가 좋아하는 것 같게…」
	ELSE
		CALL HTMLPRINTW(@"「あ…んっ%AHEART% なかなかいいですよ」")
;OriginalString : 「さぁ、もっと私に触ってください…」
		PRINTFORMW 「, 좀 더 나에게 손대어 주세요…」
	ENDIF
	;;;;;;;;;;;;;;;;;;;
	;RETURNせずに下層へ
	;;;;;;;;;;;;;;;;;;;

	;快ｘが規定以上
	IF TCVAR:가버려 || PALAM:쾌Ｃ > 7500 || PALAM:쾌Ｂ > 7500 || PALAM:쾌Ａ > 5000
		;空行を入れる
;OriginalString : 
		PRINTL
		;쾌Ｃ条件
		IF TCVAR:가버려 || PALAM:쾌Ｃ > 7500
			{
			CALL HTMLPRINTW(@"「あ゛っ%AHEART% \@ FAV_RATING 
			? やだソコっ # ソコ弄られたらッ 
			\@%AHEART% あんっ%AHEART% あぁっ%HTML_PS2_K39()%」")
			}
		;쾌Ｂ条件
		ELSEIF PALAM:쾌Ｂ > 7500
			{
			CALL HTMLPRINTW(@"「ふぁっ%AHEART% \@ FAV_RATING 
			? 乳首、민감になってるのに…あんっ # いま……乳首、弄られたらッ 
			\@%AHEART% %RAND_SPLIT("あんっ、/")%あぁっ%HTML_PS2_K39()%」")
			}
		;쾌Ａ条件
		ELSE
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あ/ん/ふ/ぁ")%ぁっ%AHEART% " + 
			@"%RAND_SPLIT("おしり…、きもちいい/やだ私……おしりでこんなに感じて")%…" + 
			@"%RAND_SPLIT("っ/ッ/")%%HTML_PS2_K39()%」")
			}
		ENDIF
;OriginalString : 
		PRINTL
		{
		CALL HTMLPRINTW(@"「\@ RAND:2 
		? は%RAND_SPLIT("あ/ぁ/っ/ーっ")%%RHEART% は%RAND_SPLIT("あ/ぁ/ーっ")%%RHEART% 
		# ハァ%RHEART% ハァ%RHEART% 
		\@ もう\@ FAV_RATING 
		? 、%MSTR%の指で溶かされそう 
		# 私、%MSTR%に溶かされそうです \@……」")
		}
	ENDIF
	;RETURNは終端で
	RETURN 1
ENDIF


