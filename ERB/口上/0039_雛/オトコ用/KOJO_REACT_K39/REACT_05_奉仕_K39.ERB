;;;;;;;;;;;;;;;
;派生0と2を統合
;;;;;;;;;;;;;;;

;소극적으로 한다 // 負荷2、派生1はCOMに流す
;==========
IF IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT파생 < 1 && TFLAG:REACT부하 < 2
	;성교봉사중
	IF TEQUIP:성교봉사중
		IF FAV_RATING
;OriginalString : 「ねぇっ！ こんなのはイヤっ！」
			PRINTFORMW 「응！ 이런 건은 이야！」
;OriginalString : 「もっと突いてよ…もっとおっぱい\@ DERI2 ? 吸ってよ # 触って \@！ これじゃ、切なくって…」
			PRINTFORMW 「 좀 더 찔러…좀 더 젖가슴\@ DERI2 ? 들이마셔요 # 손대어 \@！ 이래서야, 안타까워서…」
		ELSE
;OriginalString : 「これでは切なくなってしまいます…」
			PRINTFORMW 「이것으로는 안타깝게 되어 버립니다…」
;OriginalString : 「お願いです、もっと、もっと적극적으로」
			PRINTFORMW 「부탁입니다, 좀 더, 좀 더 적극적으로」
		ENDIF
		{
;OriginalString : %TGT%は\@ GROUPMATCH(TEQUIP:성교봉사중, 1, 4) > 0 
		PRINTFORMDW %TGT%하\@ GROUPMATCH(TEQUIP:성교봉사중, 1, 4) > 0 
		? %YOU%に가슴を押し付けながら # 消極的な%YOU%に不満気な様子で 
		\@、もどかしげに腰を揺すっている…
		}
		RETURN 1
	ENDIF

	;その他(60%) // 確立でCOMに流す
	;-----------
	IF 구상記入チェック(60)
		IF FAV_RATING
;OriginalString : 「んっ、あっ！ ねぇ、なんで消極的なの？」
			PRINTFORMW 「응, 아！ 응, 어째서 소극적인의？」
;OriginalString : 「もっとちゃんとやってよ…」
			PRINTFORMW 「 좀 더 분명하게 해…」
		ELSEIF AFFECTION("익애")
;OriginalString : 「んんっ…く…お願い、焦らさないで……」
			PRINTFORMW 「응응…구…부탁해요, 초조하게 하지 마……」
;OriginalString : 「これでは、余計に切なくなってしまいます……」
			PRINTFORMW 「이것으로는, 불필요하게 안타깝게 되어 버립니다……」
		ELSEIF AFFECTION("眷愛")
;OriginalString : 「嫌々やってるような感じですね…」
			PRINTFORMW 「혐들 하고 있는 것 같은 느낌이군요…」
		ELSE
;OriginalString : 「あまり私の가슴に触れたくないみたいですね…」
			PRINTFORMW 「그다지 나의 가슴에 닿고 싶지 않은 것 같네요…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;태연하게 한다・적극적으로 한다
;======================
IF IS_NOWCOMNAME("태연하게 한다/적극적으로 한다") && TFLAG:REACT부하 < 2
	;성교봉사중
	IF TEQUIP:성교봉사중
		CALL QUAKE(4)
		{
		CALL HTMLPRINTW(@"「%RAND_SPLIT("ふ/あ/ん")%ぁっ%AHEART% " + 
		@"%RAND_SPLIT("あ/は/ん")%ぁ%RAND_SPLIT("っ/")%っ%AHEART% あんっ%AHEART% " + 
		@"あ%RAND_SPLIT("゛/")%っ%AHEART% %RAND_SPLIT("あ/ふ")%ぁっ%EHEART%」")
		}
;OriginalString : %TGT%は%YOU%に貫かれ하며 \@ DERI2 ? 先端の蕾を吸わ # 가슴を揉みしだか \@れ、甘い吐息を漏らしている…
		PRINTFORMDW %TGT%은(는) %YOU%에 관철해진 채로\@ DERI2 ? 첨단의 꽃봉오리를 흡원 # 가슴를 주무른동인가 \@, 단 한숨을 흘리고 있다…
		RETURN 1
	ENDIF

	;;;;;;;;;;;;;;;;;;
	;以下、確率でCOMへ
	;;;;;;;;;;;;;;;;;;

	;상사상애
	IF 구상記入チェック(80) && FAV_RATING ;80%
		;죠교자の쾌Ｂが高い // 죠교자が정욕
		IF PALAM:쾌Ｂ >= 7500 || IS_CONDITION(TARGET, "정욕")
			{
			CALL HTMLPRINTW(@"「あっ%AHEART% あぁん%AHEART% " + 
			@"や…そんなに優しく\@ DERI2 ? 吸わ # 触れら \@れたら私、もう……、抑えられなくなっちゃう…」")
			}
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("ふ/あ")%ぁ%RAND_SPLIT("っ/")%%AHEART% " + 
			@"%RAND_SPLIT("は/あ/")%ぁっ%RAND_SPLIT("っ/")%%AHEART% " + 
			@"%RAND_SPLIT("あ/ん/は")%んっ%AHEART% あっ%AHEART% あぁっ%EHEART%」")
			}
		;その他
		ELSE
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("ふふ/くすっ")%、そうやっておっぱい\@ 
			DERI2 ? 吸 # 触 
			\@ってる時の%MSTR%ってホント子供みたい%RAND_SPLIT("ね/")%%SYMBOL%」")
			}
			{
			CALL HTMLPRINTW(@"「子供にしては…ん%RAND_SPLIT("っ/")%%AHEART% \@ 
			DERI2 ? 舌 # 指 \@使いがイヤラシ過ぎるけど」")
			}
		ENDIF
		RETURN 1
	ENDIF

	;익애(호감도2000)
	IF 구상記入チェック(70) && AFFECTION("익애") ;70%
		SELECTCASE RANDREACT(2, 421)
			CASE 0
				CALL HTMLPRINTW(@"「あぁっ%AHEART% いいです、気持ちいいですっ%AHEART% んんっ%EHEART%」")
				CALL HTMLPRINTW(@"「あっ%AHEART% もっとっ、もっと\@ DERI2 ? 吸 # 触 \@ってください……」")
			CASE 1
				{
				CALL HTMLPRINTW(@"「ひあぁっ%AHEART% だ、駄目です%AHEART% そんなに優しく\@ 
				DERI2 ? 吸わ # 触れら \@れたら%EHEART%」")
				}
				{
				CALL HTMLPRINTW(@"「もう私……%RAND_SPLIT(
				@"自分が、抑えられなくなりそうで/%MSTR%のこと、本気で…好きに")%っ！」")
				}
		ENDSELECT
		RETURN 1
	ENDIF

	;眷愛(호감도1000)
	IF 구상記入チェック(70) && AFFECTION("眷愛") ;70%
		IF PALAM:쾌Ｂ >= 6500
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あ/ん")%っ%AHEART% なんか、イヤラシイ\@ DERI2 ? 舌 # 指 \@使いですね」")
;OriginalString : 「\@ DERI2 ? 先が # 私… \@、切なくなってしまいます…」
			PRINTFORMW 「\@ DERI2 ? 앞이 # 나… \@, 안타깝게 되어 버립니다…」
		ELSE
			CALL HTMLPRINTW(@"「あらあら、赤ちゃんみたいですね%SYMBOL%」")
;OriginalString : 「いいですよ。私の\@ DERI2 ? 乳首、もっと吸 # 가슴、もっと触 \@ってください…」
			PRINTFORMW 「좋아요. 나의 \@ DERI2 ? 유두, 좀 더 흡 # 가슴, 좀 더 촉 \@라고 주세요…」
		ENDIF
		RETURN 1
	ENDIF

	;その他(50%) // あまり손の入っていない箇所は確率落としてCOMと相互に補完し合うように
	IF 구상記入チェック(50)
;OriginalString : 「んっ！ なかなかいいですよ」
		PRINTFORMW 「응！ 꽤 좋아요」
;OriginalString : 「丁寧に、優しくお願いしますね…」
		PRINTFORMW 「정중하게, 상냥하고 부탁드릴게요…」
		RETURN 1
	ENDIF
ENDIF


