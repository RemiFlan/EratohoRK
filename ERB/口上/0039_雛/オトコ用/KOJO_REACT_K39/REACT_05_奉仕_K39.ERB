ENDIF

;REACT51, 소극적으로 한다(55%)
;========================
IF 구상記入チェック(55) && IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT파생 < 1 && TFLAG:REACT부하 < 2
	SELECTCASE RANDREACT(4, 410)
		CASE 0
			CALL HTMLPRINTW(@"「んっ%AHEART% くっ…あまり焦らさないで\@ FAV_RATING ? よ # \@……」")
;OriginalString : 「これじゃ私…切なくって……」
			PRINTFORMW 「이래서야 나…안타까워서……」
		CASE 1
			CALL HTMLPRINTW(@"「あっ%AHEART% もっと…、적극적으로して\@ FAV_RATING ? よ # くれないと \@…」")
;OriginalString : 「私、もどかしく\@ FAV_RATING ? っ # \@て……」
			PRINTFORMW 「 나, 안타깝고\@ FAV_RATING ? 읏 # \@(이)라고……」
		CASE 2
			{
			CALL HTMLPRINTW(@"「あんっ%AHEART% んっ…\@ FAV_RATING 
			? なによそれ…焦れったい…… # あまり했다くないんですか…？ \@」")
			}
		CASE 3
			CALL HTMLPRINTW(@"「んっ%AHEART% はんっ%AHEART% %RAND_SPLIT("あ/ん/は")%っ……」")
;OriginalString : 「\@ FAV_RATING ? ねぇ…、そんなに焦らさないでよぉ # お願いです…あまり焦らさないで \@……」
			PRINTFORMW 「\@ FAV_RATING ? 저기…, 그렇게 초조하게 하지 말아 # 부탁입니다…그다지 초조하게 하지 마 \@……」
	ENDSELECT
	RETURN 1
ENDIF

;REACT51, 적극적으로 한다(60%)
;========================
IF 구상記入チェック(60) && IS_NOWCOMNAME("적극적으로 한다") && TFLAG:REACT부하 < 2
	SELECTCASE RANDREACT(6, 420)
		CASE 0
			{
			CALL HTMLPRINTW(@"「や%AHEART% ダメっ！ そこっ、舌で転が\@ 
			FAV_RATING ? しちゃぁ # 당했다ら \@っ…」")
			}
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あ/ん")%%AHEART% そ、そうやって！ 弱いとこばっかり…ッ！」")
		CASE 1
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あ/ん/あぁ")%んっ%AHEART% あっ%AHEART% 舌が、きもちいい\@ 
			!FAV_RATING ? です # \@ッ…%EHEART%」")
			}
			CALL HTMLPRINTW(@"「もっとっ%AHEART% もっとぉ%AHEART% あぁっッ…%EHEART%」")
		CASE 2
			{
			CALL HTMLPRINTW(@"「やっ%AHEART% %RAND_SPLIT("あ/ん/あぁ")%んっ%AHEART% き、きもちいい\@ 
			FAV_RATING ? よぉ # です \@っ…%EHEART%」")
			}
		CASE 3
			{
			CALL HTMLPRINTW(@"「ふぁっ%AHEART% \@ FAV_RATING 
			? あん # いいですっ 
			\@%AHEART% そこ……もっとっ%EHEART%」")
			}
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("は/あ/ん/")%んっ%AHEART% 蕩けるッ！ 蕩けちゃ\@ 
			FAV_RATING ? う # います \@…っ%EHEART%」")
			}
		CASE 4
			CALL HTMLPRINTW(@"「あぁっ%AHEART% そ、そんなとこまで……%RAND_SPLIT("あ/ふ/ん")%ぁっ%EHEART%」")
			{
			CALL HTMLPRINTW(@"「やっ%AHEART% \@ FAV_RATING 
			? やだ私…へ、変になっちゃう # わ、わたし……おかしくっ 
			\@…ッ…%EHEART%」")
			}
		CASE 5
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("は/あ/ん")%んっ%AHEART% " + 
			@"%RAND_SPLIT("あ/ふ")%ぁっ%AHEART% 적극적으로、シてくれる\@ 
			FAV_RATING ? の # んです \@ね……」")
			}
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("ふ/あ/ん")%ぁっ%AHEART% いい\@ 
			FAV_RATING ? よぉ # です 
			\@っ%AHEART% もっと、もっとッ…%EHEART%」")
			}
	ENDSELECT
	RETURN 1
ENDIF


