			CALL HTMLPRINTW(@"「んんっ%AHEART%　や、やだ…そんなに激しく…ッ！」")
			CALL HTMLPRINTW(@"「んっ%AHEART%　あぁっ…あっ%AHEART%　そ、そこっ！ あぁ…」")
