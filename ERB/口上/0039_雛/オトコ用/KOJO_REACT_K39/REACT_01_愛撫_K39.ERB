;///////////////////////////////////
;REACT27, 밀어 넘어트린다・個別リアクション
;///////////////////////////////////
[SKIPSTART]
想定されるCOM…
	회화系反応
		COM:마음 약하게 응답한다/무뚝뚝하게 응답한다/상냥하게 응답한다/강하게 응답한다/용서를 빌다/기분좋게 해
	고통系反応
		COM:무서워한다

・地の文はREACT분류(受け入れる/적극적으로 한다, 태연하게 한다/용서를 빌다/소극적으로 한다)で分岐
[SKIPEND]
@KOJO_MALE_REACT_K39_27 ;TFLAG:ACT파생（=0,밀어 넘어트린다/=1,밀어넘어짐終了）
#DIM  DYNAMIC FAV_RATING
#DIMS DYNAMIC SYMBOL
#DIMS DYNAMIC STRING
#DIMS DYNAMIC MSTR
FAV_RATING = GET_KFLAG_K39("상사상애")
SYMBOL    '= HTML_PS_K39()
STRING    '= RAND_SPLIT("ふふ/うふ/ふふっ/うふっ/くすっ/クスクス") + "…"
MSTR      '= CNAME(MASTER, TARGET)

;밀어넘어짐終了なら1を返して終わり
SIF TFLAG:ACT파생
	RETURN 1

;REACT27, ACT파생0:밀어 넘어트린다
;==========================
IF !TFLAG:ACT파생
	;REACT분류が受け入れる
	;---------------------
	IF IS_COMGRONAME("受け入れる")
		;REACT부하による派生
		SELECTCASE GET_COM_RANK()
			CASE "自然な행동", "無理のない행동"
				{
;OriginalString : 「押し倒されて嬉しそうな顔してる\@ FAV_RATING 
				PRINTFORMW 「밀어 넘어뜨려져 기쁜 듯한 얼굴 하고 있다\@ FAV_RATING 
				? 남자って、ちょっとどうなの？ って気もするけど 
				# 男の人ってどうなんでしょう？ という気もしますけど \@…」
				}
				CALL HTMLPRINTW(@"「これはこれで悪くない\@ FAV_RATING ? わね # ですね \@%SYMBOL%」")
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
			CASE "無理を했다행동"
				{
;OriginalString : 「そうそう、それで良い\@ FAV_RATING 
				PRINTFORML 「그렇게 자주, 그래서 좋다\@ FAV_RATING 
				? のよ # んですよ 
				\@。%MSTR%だって満更でもない\@ FAV_RATING 
				? のよね # んですよね \@？」
				}
;OriginalString : 「これから私にされることに期待してる\@ FAV_RATING ? のよね # んですよね \@？」
				PRINTFORMW 「지금부터 나로 되는 것에 기대하고 있다\@ FAV_RATING ? 의군요 # 응(이)군요 \@？」
				CALL FADE_TO_CLEARLINE(15, @"「%STRING%」")
				CALL HTMLPRINTW(@"「%MSTR%のその表情を見れば判\@ FAV_RATING ? るわ # りますよ \@…%SYMBOL%」")
		ENDSELECT
		RETURN 1
	ENDIF

	;//消費기력の派生を設けたいが今は見送る
	;REACT분류が적극적으로 한다・태연하게 한다
	;---------------------------------
	IF IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;호의호감도で派生
		IF FAV_RATING
;OriginalString : 「随分大人しくなっちゃって…」
			PRINTFORMW 「상당히 얌전해져 버려서…」
			{
;OriginalString : 「%RAND_SPLIT(
			PRINTFORMW 「%RAND_SPLIT(
			"それって、あとは好きにしてって事？/" + 
			"じゃ、後は好きなようにさせてもらうわ。ふふ…/" + 
			"ふふっ…、いいわ。そのままじっとしてて……")%」
			}
		ELSE
;OriginalString : 「あら、얌전한んですね？」
			PRINTFORMW 「어머나, 얌전하네요？」
			CALL HTMLPRINTW(@"「では、あとは私の好きにさせてもらいますよ%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류が용서를 빌다
	;---------------------
	IF IS_COMGRONAME("용서를 빌다")
		IF FAV_RATING
;OriginalString : 「ほら、大人しくして…」
			PRINTFORMW 「이봐요, 얌전하게 해…」
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT(
			"可愛がってあげる/" + 
			"悪いようにはしないわ/" + 
			"動かれるとその…、色々とやりにくいし")%…%SYMBOL%」")
			}
		ELSE
			CALL HTMLPRINTW(@"「%RAND_SPLIT("さぁ、もう逃がしませんよ/大人しくしててくださいね？ ふふ…")%%SYMBOL%」")
		ENDIF
		RETURN 1
	ENDIF

	;REACT분류が소극적으로 한다
	;---------------------
	IF IS_COMGRONAME("소극적으로 한다")
		;COM10, 부끄러워한다
		IF IS_NOWCOMNAME("부끄러워한다")
			IF FAV_RATING
;OriginalString : 「私に押し倒されるのがそんなに恥ずかしいの？」
				PRINTFORMW 「 나에게 밀어 넘어뜨려지는 것이 그렇게 부끄러운거야？」
				CALL HTMLPRINTW(@"「ふふ、可愛い。키스했다くなっちゃう…%SYMBOL%」")
				CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
				;키스を捏造
				CALL SET_KISS
			ELSE
				CALL HTMLPRINTW(@"「あら、こんな事で부끄러워한다なんて…。可愛らしいですね%SYMBOL%」")
			ENDIF
			RETURN 1
		ENDIF

		;その他のCOMの場合
		IF FAV_RATING
;OriginalString : 「それ、ちょっと消極的過ぎない？」
			PRINTFORML 「그것, 조금 소극너무 적인 있고？」
;OriginalString : 「あとは私にお任せって事なの？」
			PRINTFORMW 「나머지는 나에게 맡겨라는 일이야？」
			CALL HTMLPRINTW(@"「まぁ…、それならそれで。私は別にいいけど%SYMBOL%」")
		ELSE
;OriginalString : 「あまり乗り気じゃないみたいですけど…」
			PRINTFORML 「그다지 내켜하는 마음이 아닌 것 같지만…」
;OriginalString : 「かといって、抵抗するつもりもないんですね？」
			PRINTFORMW 「라고 해서 저항할 생각도 없네요？」
			CALL HTMLPRINTW(@"「うふ%HTML_AH2_K39()% では、そういう事なら……」")
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;REACTで必ず完結시킨다
RETURN 1
