			PRINTFORMDL %RAND_SPLIT("자궁입까지 후벼파진다감각에/질내의 최안쪽까지 켜진다감각에")%,
		;애태움無し
		ELSE
			;汎用処理, COM20_2
			CALL PRINT_REACT_CATEGORY_10_COM20_2_K39
			{
;OriginalString : %RAND_SPLIT(
			PRINTFORMDL %RAND_SPLIT(
			"子宮입に届くほどの深く激しい突きこみに/" + 
			@"%YOU%の激しい허리기술に/" + 
			"荒々しい허리기술で抽送を叩きこまれ")%、
			}
		ENDIF
;OriginalString : %TGT%は%PLEASURE_DESCRIPTION(TARGET)%喘ぎ悶えている…
		PRINTFORMDW %TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%허덕여 괴로워하고 있다…
		RETURN 1
	ENDIF

	;互いに정욕
	;----------
	IF IS_CONDITION(MASTER, "정욕") && IS_CONDITION(TARGET, "정욕")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「…っあ%AHEART% %RAND_SPLIT("あ/ん/ふ/")%あぁっ%AHEART% もう、おかしくな…る……」")
		CALL QUAKE(4)
		{
		CALL HTMLPRINTW(@"「ひぁっ%AHEART% あぁっ%AHEART% " + 
		@"%RAND_SPLIT("あ/は")%あぁぅっ、あぁっ%AHEART% はぁっ…%EHEART%」")
		}
;OriginalString : 정욕に滾る二人は互いに狂おしく求め合い、獣のように激しく交わっている…
		PRINTFORMDW 정욕에 고두 명은 서로 서로 미칠 듯하게 요구해 , 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF

	;죠교대상が정욕
	;--------------
	IF IS_CONDITION(MASTER, "정욕")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「や、激しッ！ 壊れるっ、壊れちゃうっ%AHEART% あぅっ%AHEART% あぁっ%EHEART%」")
		CALL QUAKE(4)
		CALL HTMLPRINTW(@"「あっ%AHEART% あぁっ%AHEART% あっ%AHEART% こ、これダメぇっ\@ PALAM:쾌Ｖ >= 8500 ? ！！ # \@！」")
;OriginalString : 
		PRINTL
;OriginalString : 정욕に滾る%YOU%の%RAND_SPLIT("激/荒々")%しい突きこみに、
		PRINTFORMDL 정욕에 고%YOU%의 %RAND_SPLIT("격/황들")%강요하고 찔러 포함에,
;OriginalString : %TGT%は%PLEASURE_DESCRIPTION(TARGET)%激しく喘いでいる…
		PRINTFORMDW %TGT%은(는) %PLEASURE_DESCRIPTION(TARGET)%격렬하게 허덕이고 있다…
		RETURN 1
	ENDIF

	;죠교자が정욕
	;------------
	IF IS_CONDITION(TARGET, "정욕")
		CALL QUAKE(4)
		{
		CALL HTMLPRINTW(@"「ねぇもっと！ もっと突いてっ\@ 
		TENSION(TARGET, 8) == "高い" ? ！！！ # ！\@ あぅっ、あぁっ%EHEART%」")
		}
		CALl QUAKE(4)
		CALL HTMLPRINTW(@"「もっと%AHEART% %RAND_SPLIT("ぐちゃぐちゃ/めちゃめちゃ")%に…%RAND_SPLIT("っ/ッ/")%！」")
;OriginalString : 情炎に身を焦がす%TGT%は%YOU%を激しく求めつつ、
		PRINTFORMDL 정염에 몸을 태운다%TGT%은(는) %YOU%(을)를 격렬하게 요구하면서,
;OriginalString : その젖음唇から艷やかな旋律を奏でている…
		PRINTFORMDW 그 젖은 입술로부터 요염한 선율을 연주하고 있다…
		RETURN 1
	ENDIF

	;その他 // この箇所のみ確率で汎用処理に流す
	;------
	IF 구상記入チェック(65)
		CALL QUAKE(4)
		;상사상애
		IF GET_KFLAG_K39("상사상애")
			{
			CALL HTMLPRINTW(@"「んっ%AHEART% あ%RAND_SPLIT("ん/ン")%んっ%AHEART% " + 
			@"%RAND_SPLIT("あ/ふ/")%ぁっ%AHEART% 好きっ%AHEART% 大好きっ%EHEART%」")
			}
			CALL QUAKE(4)
;OriginalString : 「ねぇ、키스して…何もかも忘れさせて……」
			PRINTFORMW 「응, 키스 해…모두 잊게 해……」
			CALL CHK_FADECONFIG_OUTPUT_OF_KOJORAND_K39(, , "키스", 1)
		;익애
		ELSEIF AFFECTION("익애")
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("あ/ん/ふ/ああ/んあ/ふあ")%ぁっ%AHEART% " + 
			@"%RAND_SPLIT("は/あ/")%%RAND_SPLIT("う/うぅ/ぅ")%っ%AHEART% " + 
			@"あっ%AHEART% あっ%AHEART% あぁっ…%EHEART%」")
			}
			CALL QUAKE(4)
;OriginalString : 「お願い、키스して……%KOJO_RAND_K39(("키스"), 1)%」
			PRINTFORMW 「부탁해요, 키스 해……%KOJO_RAND_K39(("키스"), 1)%」
		;その他
		ELSE
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("ん/ン")%んっ%AHEART% " + 
			@"あ%RAND_SPLIT("ァッ/ぁっ/ぁッ")%%AHEART% 凄く、深くて…」")
			}
			CALL QUAKE(4)
			{
			CALL HTMLPRINTW(@"「%RAND_SPLIT("んあ/ああ/ふあ/ん/あ/ふ/")%ぁっ%AHEART% " + 
			@"はうぅっ！ あっ%AHEART% あっ、あぁっ…%EHEART%」")
			}
		ENDIF
		RETURN 1
	ENDIF
ENDIF

