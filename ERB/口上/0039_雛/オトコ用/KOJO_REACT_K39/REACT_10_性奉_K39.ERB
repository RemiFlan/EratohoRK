@KOJO_MALE_REACT_K39_95 ;TFLAG:ACT파생(0=통상/1=연속/2=Ｖ성교봉사系から/3=Ａ성교系から/4=Ｖ성교系から/5=Ａ성교봉사系から)
#DIMS DYNAMIC AHEART
#DIMS DYNAMIC EHEART
#DIMS DYNAMIC MSTR
#DIMS DYNAMIC YOU
#DIMS DYNAMIC TGT
AHEART '= HTML_AH_K39()
EHEART '= HTML_PS2_K39()
MSTR   '= CNAME(MASTER, TARGET)
YOU    '= CALLNAME:MASTER
TGT    '= CALLNAME:TARGET

;조교중初回かつ受容である場合
;============================
;ONCEREACTではなくONCEを使用
;ONCEは번호を統一し、分類全体でターンにつき一度のみ表示する
IF IS_COMGRONAME("消極的従う/平然と従う/積極的従う/受け入れる/용서를 빌다") && ONCE(295)
	CALL TEXTDISPLAY_CATEGORY_10_ONCE_K39
	RETURN RESULT
ENDIF

;애태움開始
;==========
IF TCVAR:MASTER:가버려 && TCVAR:MASTER:애태움도 > 1 && IS_COMGRONAME("積極的従う/平然と従う/受け入れる")
	CALL TEXTDISPLAY_CATEGORY_10_TEASE_K39
	RETURN RESULT
ENDIF

;쾌감을 참는다 // 55%
;===============
IF 구상記入チェック(55) && IS_NOWCOMNAME("쾌감을 참는다")
	CALL TEXTDISPLAY_CATEGORY_10_COM14_K39
	RETURN RESULT
ENDIF

;消極的に従った // 50%
;==============
IF 구상記入チェック(50) && IS_COMGRONAME("消極的従う")
	CALL TEXTDISPLAY_CATEGORY_10_REACTCATEGORY_1_K39
	RETURN RESULT
ENDIF

;쾌감을 받아들인다 // 60%
;================
IF 구상記入チェック(60) && IS_NOWCOMNAME("쾌감을 받아들인다")
	CALL TEXTDISPLAY_CATEGORY_10_COM15_K39
	RETURN RESULT
ENDIF

;허리를 흔든다
;========
