	;=================================================
	IF 구상記入チェック(0) && GET_COM_RANK() == "無理を했다행동"
		;実は적극			恥ずかしさに消え入りそうな声で了承했다
		IF COMCOR_POSI()
			PRINTFORMW 
		;実は嫌				悔しさに顔を歪めながらも了承했다
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF

