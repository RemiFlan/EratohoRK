;────────────────────────────────────
;EV101, 의상変更
;────────────────────────────────────
;・ショップから의상変更を行った場合に表示
;-------------------------------------------------
@KOJO_EVENT_K15_101(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=
;　一の位…（0, もう着てる、1, 着替えた、2, 恥ずかしくて着られない、3, 嫌がって着ない、9, 持ってない）
;　十の位…（0, 통상、10, 반라、20, 전라、30, 속옷無し）
;　百の位…코스프레のために保留
;　千の位…（0, 죠교자, 1000, 죠교대상）
;　例（죠교대상が恥ずかしがって전라にできなかった＝1022）
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が着替えた
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상되었다
		CASE 1
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
;OriginalString : 「赤ちゃんもいますし、暖かくしないとですね♪」
				PRINTFORML 「아기도 있고, 따뜻하게 하지 않으면이군요♪」
			ELSE
				IF RAND:2 == 0
;OriginalString : 「やっぱりいつもの服が一番落ち着きますね」
					PRINTFORML 「역시 평소의 옷이 제일 침착하네요」
				ELSE
;OriginalString : 「これなら%RAND_SPLIT("脱がす愉しみもあり/着衣えっちもでき")%ますもんね♪」
					PRINTFORML 「이것이라면%RAND_SPLIT("벗기는 즐거움도 있어/착의 엣치도 성과")%것이군요♪」
				ENDIF
			ENDIF
			RETURN 1
		;반라되었다
		CASE 11
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				;フレーバーだよ！
;OriginalString : 「シースルーのマタニティドレスって…ヘンタイっ♪」
				PRINTFORML 「시 스루의 마타니티드레스는…헨타이♪」
			ELSE
;OriginalString : 「ちょっと身軽になりま했다っ」
				PRINTFORML 「조금 홀가분하게 되었던」
			ENDIF
			RETURN 1
		;전라되었다
		CASE 21
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
;OriginalString : 「ちょっとスース―しますね…」
				PRINTFORML 「조금 스스―하네요…」
;OriginalString : 「ほら、お腹の子が風邪ひかないように…」
				PRINTFORML 「이봐요, 배의 아이가 감기 걸리지 않게…」
;OriginalString : 「いつもリリーの事、ぎゅってしててくださいね♪」
				PRINTFORML 「언제나 릴리의 일, 는 하고 있어 주세요♪」
			ELSE
				IF RAND:2 == 0
;OriginalString : 「%RAND_SPLIT("裸になっちゃいま했다/お股がスース―します")%…えへへっ」
					PRINTFORML 「%RAND_SPLIT("알몸에지금했다/가랑이가 스스―합니다")%…네에에 」
;OriginalString : 「これなら、いつでもどこでもエッチできますね♪」
					PRINTFORML 「이것이라면, 언제 어디서나 섹스할 수 있네요♪」
				ELSE
;OriginalString : 「それじゃ、最後の一枚も……えへへっ%RAND_SPLIT("、脱いじゃいま했다/")%♪」
					PRINTFORML 「그러면, 마지막 한 장도……네에에%RAND_SPLIT(", 탈는 지금했다/")%♪」
				ENDIF
			ENDIF
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が恥ずかしがって着替えられない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		;반라だって恥ずかしい
		CASE 12
;OriginalString : 「%RAND_SPLIT("あの/ですから…その/嫌じゃないんですけど")%、まだ恥ずかしくて…」
			PRINTFORML 「%RAND_SPLIT("저/그러니까…그/혐이 아닙니다만")%, 아직 부끄러워서…」
			RETURN 1
		;普段から전라はちょっと……
		CASE 22
;OriginalString : 「%RAND_SPLIT("えっと/無理言わないでください/その")%…まだ心の準備が……うぅ…」
			PRINTFORML 「%RAND_SPLIT("엣또/무리 말하지 말아 주세요/그")%…아직 마음의 준비가………」
			RETURN 1
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が嫌がって着替えられない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		;반라だって恥ずかしい
		CASE 13
;OriginalString : 「あの、まだ恥ずかしくて…」
			PRINTFORML 「저, 아직 부끄러워서…」
			RETURN 1
		;普段から전라はちょっと……
		CASE 23
;OriginalString : 「えっと…まだ心の準備が……うぅ…」
			PRINTFORML 「어와…아직 마음의 준비가………」
			RETURN 1
	ENDSELECT
ENDIF

