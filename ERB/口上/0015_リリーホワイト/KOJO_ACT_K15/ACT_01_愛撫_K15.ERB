	;죠교방침　1,노멀플레이、2,이상플레이、3,휴식、4,하드플레이、5,소프트플레이
	;행동예약/방침예약/ACT분류예약
	;금일의방침が애만들기
	IF IS_POLICY("금일", "애만들기") && PERCENT(90)
			;分類성교
			TFLAG:ACT분류예약 = 3
			WAIT
	ELSEIF AFFECTION("상사상애") && PERCENT(50)
		IF !TALENT:MASTER:처녀 && TEQUIP:눈가리개 && PERCENT(30)
			;역강간
			TCVAR:행동예약 = 103
			WAIT
		ELSEIF !TALENT:MASTER:처녀 && PERCENT(30)
			;分類성교
			TFLAG:ACT분류예약 = 3
			WAIT
		ELSEIF PERCENT(30)
			;입으로 애무
			TCVAR:행동예약 = 12
			WAIT
		ELSEIF PERCENT(30)
			;파이즈리한다
			TCVAR:행동예약 = 17
			WAIT
		ELSEIF PERCENT(30)
			;항문펠라
			TCVAR:행동예약 = 14
			WAIT
		ELSEIF PERCENT(30)
			;성기마찰한다
			TCVAR:행동예약 = 16
			WAIT
		ENDIF
	ELSEIF AFFECTION("죠교完了") && PERCENT(50)
		IF !TALENT:MASTER:처녀 && TEQUIP:눈가리개 && PERCENT(30)
			;역강간
			TCVAR:행동예약 = 103
			WAIT
		ELSEIF !TALENT:MASTER:처녀 && PERCENT(20)
			;分類성교
			TFLAG:ACT분류예약 = 3
			WAIT
		ELSEIF PERCENT(30)
			;입으로 애무
			TCVAR:행동예약 = 12
			WAIT
		ELSEIF PERCENT(30)
			;파이즈리한다
			TCVAR:행동예약 = 17
			WAIT
		ELSEIF PERCENT(30)
			;성기마찰한다
			TCVAR:행동예약 = 16
			WAIT
		ELSEIF PERCENT(30)
			;항문펠라
			TCVAR:행동예약 = 14
			WAIT
		ENDIF
	ELSEIF AFFECTION("目覚め") && PERCENT(50)
		IF PERCENT(30)
			;파이즈리한다
			TCVAR:행동예약 = 17
			WAIT
		ELSEIF PERCENT(30)
			;입으로 애무
			TCVAR:행동예약 = 12
			WAIT
		ELSEIF PERCENT(30)
			;항문펠라
			TCVAR:행동예약 = 14
			WAIT
		ELSEIF PERCENT(30)
			;성기마찰한다
			TCVAR:행동예약 = 16
			WAIT
		ENDIF
	ENDIF
	RETURN 1
ENDIF

