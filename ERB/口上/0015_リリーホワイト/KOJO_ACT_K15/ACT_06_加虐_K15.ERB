		;죠교자が자위중
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;Ｃ애무継続中
	;=================================================
	IF 구상記入チェック(0) && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2)
		;수음・애무継続中
		IF TEQUIP:죠교대상Ｃ사용 == 1
;OriginalString : 
			PRINTFORMW 
		;펠라・クンニ継続中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件無し
	;=================================================
	IF 구상記入チェック(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

