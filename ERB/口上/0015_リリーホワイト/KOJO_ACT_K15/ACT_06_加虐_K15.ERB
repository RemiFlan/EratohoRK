IF 구상記入チェック(0) && !TFLAG:ACT파생
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교중/성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TRUECHECK(TEQUIP:성교중, TEQUIP:성교봉사중) > 0
		;성교중
