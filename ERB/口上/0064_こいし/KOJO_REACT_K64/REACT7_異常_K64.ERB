;REACT74_0, 最後まで人は来なかった(無事)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック() && !TFLAG:ACT파생
	;混乱していた%CALLNAME:MASTER%だったが、最後まで人は来なかった
	;やっと%CALLNAME:TARGET%に回収され、%CALLNAME:MASTER%はほっと했다…
	;=================================================
	;처녀
	;=================================================
	IF 구상記入チェック() && TALENT:MASTER:처녀
		SELECTCASE RANDREACT(2, 2)
			CASE 0
;OriginalString : 「えへへー」
				PRINTFORMW 「네에에―」
;OriginalString : 「スリルあったでしょ？」
				PRINTFORMW 「스릴 있었겠죠？」
			CASEELSE
;OriginalString : 「あーあ、残念♪」
				PRINTFORMW 「아-아, 유감♪」
;OriginalString : 「%K64_CNAME()%の想い出に残る初体験が見たかったのに」
				PRINTFORMW 「%K64_CNAME()%의 추억에 남는 첫체험을 보고 싶었는데 」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック()
		SELECTCASE RANDREACT(2, 3)
			CASE 0
;OriginalString : 「誰も来なかったね」
				PRINTFORMW 「아무도 오지 않았지요」
;OriginalString : 「…本当はちょっと期待してたんじゃない？」
				PRINTFORMW 「…사실은 조금 기대하고 있었던  것이 아니다？」
			CASEELSE
;OriginalString : 「あれ、誰も来なかったんだ」
				PRINTFORMW 「저것, 아무도 오지 않았다」
;OriginalString : 「%K64_CNAME()%ってみんなから嫌われてるんじゃない？」
				PRINTFORMW 「%K64_CNAME()%라는 모두로부터 미움받고 있는  것이 아니다？」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;REACT75, 페니스밴드장착・個別リアクション
;────────────────────────────────────
;・TFLAG:ACT파생(0=장착/1=解除)
;-------------------------------------------------
;想定されるCOM…
;회화系反応
;	COM:침묵한다/마음 약하게 응답한다/무뚝뚝하게 응답한다/상냥하게 응답한다/강하게 응답한다/빈정거린다/더 하드하게 해/용서를 빌다/기분좋게 해
;고통系反応
;	COM:무서워한다
;その他反応
;	COM:자위를 시작한다
;-------------------------------------------------
@KOJO_REACT_K64_75
;-------------------------------------------------
;・固有の地の文なし。죠교대상の反応に応じてREACTION_MESSAGEで汎用処理
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT75_0, 죠교자페니스밴드장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && !TFLAG:ACT파생
	;=================================================
	;죠교자が처녀を喪失
	;=================================================
	;TARGETが처녀なら、처녀상실が起こる。この処理は@ACTION_APPLY2_75にて行われる
	;-------------------------------------------------
	IF 구상記入チェック(0) && TCVAR:TARGET:처녀상실
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM0, 침묵한다(REACT분류:なし)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("침묵한다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM1, 마음 약하게 응답한다(REACT분류:소극적으로 한다)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("마음 약하게 응답한다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM2, 무뚝뚝하게 응답한다(REACT분류:なし)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("무뚝뚝하게 응답한다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM3, 愛想良く応答する(REACT분류:적극적으로 한다)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("상냥하게 응답한다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM4, 강하게 응답한다(REACT분류:なし)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("강하게 응답한다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM5, 빈정거린다(REACT분류:태연하게 한다)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("빈정거린다")
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;COM6, 더 하드하게 해(REACT분류:受け入れる)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("더 하드하게 해")
		;-------------------------------------------------
		;「この恥知らずの豚め！」(징계・포인트加算)
		;-------------------------------------------------
		IF 구상記入チェック(0) && TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;派生1を書いた場合はREACT파생0,「ふーん」(죠교대상の意見を黙殺)扱い
		;書かなかった場合はREACT파생に関わらず、この箇所を補完します
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 구상記入チェック(0)
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;COM7, 용서를 빌다(REACT분류:용서를 빌다)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("용서를 빌다")
		;派生1の場合はREACT분류:なし
		;-------------------------------------------------
		;意見を出すなんていい度가슴だね！(징계・포인트加算)
		;-------------------------------------------------
		IF 구상記入チェック(0) && TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;派生1を書いた場合はREACT파생0,「ふーん」(죠교대상の意見を黙殺)扱い
		;書かなかった場合はREACT파생に関わらず、この箇所を補完します
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 구상記入チェック(0)
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;COM8, 기분좋게 해(REACT분류:なし)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("기분좋게 해")
		;-------------------------------------------------
		;「この恥知らずの豚め！」(징계・포인트加算)
		;-------------------------------------------------
		IF 구상記入チェック(0) && TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;派生1を書いた場合はREACT파생0,「ふーんと聞き流す」扱い
		;書かなかった場合はREACT파생に関わらず、この箇所を補完します
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 구상記入チェック(0)
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;COM33, 무서워한다(REACT분류:용서를 빌다)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("무서워한다")
		;-------------------------------------------------
		;派生：怯えに対する초조함
		;-------------------------------------------------
		IF 구상記入チェック(0) && TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;派生1を書いた場合はREACT파생0,「ふーんと聞き流す」扱い
		;書かなかった場合はREACT파생に関わらず、この箇所を補完します
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 구상記入チェック(0)
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	
	;=================================================
	;COM50, 자위를 시작한다(REACT분류:受け入れる)
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("자위를 시작한다")
		;-------------------------------------------------
		;勝손にオナニーするな(징계・포인트加算)
		;-------------------------------------------------
		IF 구상記入チェック(0) && TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
		
		;派生1を書いた場合はREACT파생0,「勝손にオナニーしてるけどいいか」扱い
		;書かなかった場合はREACT파생に関わらず、この箇所を補完します
		;-------------------------------------------------
		;その他
		;-------------------------------------------------
		IF 구상記入チェック(0)
;OriginalString : 
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	RETURN 0	;条件にヒットしなければ汎用リアクションへ
ENDIF

;派生0,장착を参考に필요な分岐を追加して使用して下さい
;条件なしの記述はやめましょう
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT75_1, 죠교자페니스밴드解除
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:ACT파생 == 1
	RETURN 0	;条件にヒットしなければ汎用リアクションへ
ENDIF
;REACT75, 페니스밴드장착
