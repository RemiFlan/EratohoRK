			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

