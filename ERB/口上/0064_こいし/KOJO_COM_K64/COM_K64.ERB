	;=================================================
	;ACT분류,애무
	;=================================================
	IF 구상記入チェック(0) && MENUMATCH(TFLAG:ACT, "애무")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TRUECHECK(MENUMATCH(TFLAG:ACT, "성봉"), TEQUIP:TARGET:성교봉사중) > 0
		IF V_SEX(MASTER)	;Ｖ성교봉사중
			PRINTFORMW 
