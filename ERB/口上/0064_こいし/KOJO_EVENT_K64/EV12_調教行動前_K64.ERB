IF 구상記入チェック(0) && TRUECHECK(NOWEX:TARGET:Ｃ절정, NOWEX:TARGET:Ｖ절정, NOWEX:TARGET:Ａ절정, NOWEX:TARGET:Ｂ절정) > 0
	;=================================================
	;죠교자사정
	;=================================================
	IF 구상記入チェック(0) && NOWEX:TARGET:사정
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;各部位の절정が1つ以上の場合に真になる箇所
	;=================================================
	;다중절정
	;=================================================
	IF 구상記入チェック(0) && TRUECHECK(NOWEX:TARGET:Ｃ절정, NOWEX:TARGET:Ｖ절정, NOWEX:TARGET:Ａ절정, NOWEX:TARGET:Ｂ절정) > 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
