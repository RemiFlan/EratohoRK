;========================================================================
;회화系 ACT0～9
;========================================================================
;・TFLAG:ACT(0=지금 기분을 묻다/1=자신에게의 마음을 묻다/2=성에 관한 이야기를 하다/3=상냥하게 위로한다/4=엄하게 위협한다/5=의상 변경을 요구한다/6=합의를 요구한다/7=징계한다고 위협한다/8=고함치다/9=기분나쁘게 웃다)
;・TFLAG:ACT분류 == 0に該当
;・ACT9, 기분나쁘게 웃다は이상系ACT분류から派生する場合がある
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "회화")で拾う事が可能
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では구상側のRESULT(終端のRETURN 1など)によって구상が表示당했다のか否か、
;・また箇所毎の特有の挙動(미약等구상をスキップなど)を制御しています
;・@구상記入チェックの場合代入処理を省略できますが、RESULT値は구상側で決めうちする필요があります
;・特別な理由がない限り、テンプレとしてはRETURN 1を記述しておきます
;-------------------------------------------------

;────────────────────────────────────
;ACT0, 지금 기분을 묻다
;────────────────────────────────────
;・今の気分について聞いてくる
;・죠교대상の反応に応じてREACT以降でACT파생への代入が行われる
;・턴방침が"이상"なら発生しない
;-------------------------------------------------
@KOJO_ACT_K64_0
;-------------------------------------------------
;関数@TARGET_DESCRIPTIONによってTEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)
;@EQUIPACTION_DESCRIPTIONによってTEQUIP(안면승마/안면승마항문/자위중)で地の文は변화する可能性がある
;全て埋める필요はないので、その他・条件なしを最初に埋めた上で、あとは好みで分岐を増減してください
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT0, 지금 기분을 묻다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック()
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック() && FIRSTACT(0)
		PRINTFORMW 「응, 지금 어떤 기분？」
		CALL HTMLPRINTW(@"「나는 누나와 달리 마음을 읽을 수 없기 때문에, 분명하게 가르쳐%K64_SYMBOL("BHeartW")%」")
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;PENIS(TARGET)/!PENIS(TARGET)/TEQUIP:죠교자페니스밴드等で分岐してもよい
	;=================================================
	;성교중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:TARGET:성교중
		SELECTCASE TEQUIP:TARGET:성교중	;体位による派生
			CASE 1	;정상위				체중을 실어 %CALLNAME:MASTER%의 질내에 가득
				PRINTFORMW 
			CASE 2	;후배위				뒤에서 %CALLNAME:MASTER%의 질내에 가득
				PRINTFORMW 
			CASE 3	;기승위				%CALLNAME:MASTER%의 암컷 구멍에 끝까지 밀어넣어
				PRINTFORMW 
			CASE 4	;대면좌위			밀착한 %CALLNAME:MASTER%의 질을 애무하며
				PRINTFORMW 
			CASE 5	;배면좌위			%CALLNAME:MASTER%에게 삽입하여 껴안고
				PRINTFORMW 
			CASE 6	;항문섹스		%CALLNAME:MASTER%의 의 항문에 넣어
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;죠교대상のＣへの애무が行われている状況。TEQUIP관리なので可能性は割りとある
	;=================================================
	;죠교대상Ｃ사용
	;=================================================
	IF 구상記入チェック(0) && GROUPMATCH(TEQUIP:TARGET:죠교대상Ｃ사용, 1, 2)
		IF TEQUIP:TARGET:죠교대상Ｃ사용 == 1	;수음・애무継続中
			PRINTFORMW 
		ELSE									;펠라・クンニ継続中
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/!PENIS(MASTER)/TEQUIP:페니스밴드等で分岐してもよい
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:TARGET:성교봉사중
		SELECTCASE TEQUIP:TARGET:성교봉사중	;体位による派生
			CASE 1	;정상위시킨다			大きく開いた股の間に迎え入れ
				PRINTFORMW 
			CASE 2	;후배위시킨다			背後から질내に삽입され
				PRINTFORMW 
			CASE 3	;역강간				ぬるぬるの膣できゅうきゅうと絞り上げ
				PRINTFORMW 
			CASE 4	;대면좌위시킨다			体を密着させて膣奥に歓迎し
				PRINTFORMW 
			CASE 5	;배면좌위시킨다			背を預けて堪能し
				PRINTFORMW 
			CASE 6	;항문섹스시킨다	腸液にぬめる肛門で締め付け
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자が자위중
	;=================================================
	IF 구상記入チェック(0) && GETBIT(TEQUIP:TARGET:자위중, 1)
		IF TEQUIP:TARGET:죠교자페니스밴드	;죠교자が페니스밴드身に付けている
			PRINTFORMW 
		ELSE								;その他
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마中
	;=================================================
	IF 구상記入チェック(0) && TRUECHECK(TEQUIP:TARGET:안면승마, TEQUIP:TARGET:안면승마항문) > 0
		PRINTFORMW 「후후…괴롭다？ 맛있다？」
		PRINTFORMW 「응, 어떤 기분인가 가르쳐♪」
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상が자위중
	;=================================================
	IF 구상記入チェック() && GETBIT(TEQUIP:TARGET:자위중, 0)
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「자위 볼 수 있는 것은, 어떤 기분？」
			CASEELSE
				PRINTFORMW 「볼 수 있으면서 자위하는 것은, 어떤 기분？」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=============================================
	;삼각목마사용중
	;=============================================
	IF 구상記入チェック() && TEQUIP:TARGET:삼각목마
		PRINTFORMW 「어때？ 마 산호개 즐겁다？」
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック()
		PRINTFORMW 「응, 지금 어떤 기분？」
		RETURN 1
	ENDIF
ENDIF

