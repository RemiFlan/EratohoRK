;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT68_2, 관장液追加
;━━━━━━━━━━━━━━━━━━━━━━━━
;既に注入済みの場合にこの箇所に派生
;-------------------------------------------------
IF 구상記入チェック(0) && TFLAG:ACT파생 == 2
	;地の文↓
	;%CALLNAME:TARGET%は%TARGET_DESCRIPTION("하면서、")% %CALLNAME:MASTER%의 항문に
	;さらに관장液を注入しようと했다…
	
	[SKIPSTART]
	液量で書き分けるならコメントアウトを解除
	下層のPRINTFORMWを削除して下さい
	SELECTCASE TEQUIP:TARGET:관장그릇＋플러그	;관장液分量派生
		CASE 5		;液量5
;OriginalString : 
			PRINTFORMW 
		CASE 4		;液量4
;OriginalString : 
			PRINTFORMW 
		CASE 3		;液量3
;OriginalString : 
			PRINTFORMW 
		CASEELSE	;その他
;OriginalString : 
			PRINTFORMW 
	ENDSELECT
	[SKIPEND]
	;書き分けないのであればここで
;OriginalString : 
	PRINTFORMW 
	RETURN 1
ENDIF
