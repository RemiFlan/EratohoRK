ENDIF

;-------------------------------------------------
;플랑님페니스밴드연성
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 17
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;플랑님고문도구 연성전
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG == 22
	RETURN LOCAL
ENDIF

;-------------------------------------------------
;플랑님ＳＭ상품 구입
;-------------------------------------------------
;기입 체크
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG > 22 && ARG < 32
