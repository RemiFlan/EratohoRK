	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
;OriginalString : 
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;PENIS(TARGET)/!PENIS(TARGET)/TEQUIP:죠교자페니스밴드等で分岐してもよい
	;=================================================
	;성교중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:성교중
		;体位による派生
		SELECTCASE TEQUIP:성교중
			;정상위				체중을 실어 %CALLNAME:MASTER%의 질내에 가득
			CASE 1
;OriginalString : 
				PRINTFORMW 
			;후배위				뒤에서 %CALLNAME:MASTER%의 질내에 가득
			CASE 2
;OriginalString : 
				PRINTFORMW 
			;기승위				%CALLNAME:MASTER%의 암컷 구멍에 끝까지 밀어넣어
			CASE 3
;OriginalString : 
				PRINTFORMW 
			;대면좌위			밀착한 %CALLNAME:MASTER%의 질을 애무하며
			CASE 4
;OriginalString : 
				PRINTFORMW 
			;배면좌위			%CALLNAME:MASTER%에게 삽입하여 껴안고
			CASE 5
;OriginalString : 
				PRINTFORMW 
			;항문섹스		%CALLNAME:MASTER%의 의 항문에 넣어
			CASE 6
;OriginalString : 
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;죠교대상のＣへの애무が行われている状況。TEQUIP관리なので可能性は割りとある
	;=================================================
	;죠교대상Ｃ사용
	;=================================================
	IF 구상記入チェック(0) && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2)
		;수음・애무継続中
		IF TEQUIP:죠교대상Ｃ사용 == 1
;OriginalString : 
			PRINTFORMW 
		;펠라・クンニ継続中
		ELSE
;OriginalString : 
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/!PENIS(MASTER)/TEQUIP:페니스밴드等で分岐してもよい
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:성교봉사중
		;体位による派生
		SELECTCASE TEQUIP:성교봉사중
			;정상위시킨다				大きく開いた股の間に迎え入れ
			CASE 1
;OriginalString : 
				PRINTFORMW 
			;후배위시킨다				背後から질내に삽입され
			CASE 2
;OriginalString : 
				PRINTFORMW 
			;역강간					ぬるぬるの膣できゅうきゅうと絞り上げ
			CASE 3
;OriginalString : 
				PRINTFORMW 
			;대면좌위시킨다				体を密着させて膣奥に歓迎し
			CASE 4
;OriginalString : 
				PRINTFORMW 
			;배면좌위시킨다				背を預けて堪能し
			CASE 5
;OriginalString : 
				PRINTFORMW 
			;항문섹스시킨다		腸液にぬめる肛門で締め付け
			CASE 6
;OriginalString : 
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자が자위중
	;=================================================
	IF 구상記入チェック(0) && GETBIT(TEQUIP:자위중, 1)
		;죠교자が페니스밴드身に付けている
		IF TEQUIP:죠교자페니스밴드
;OriginalString : 
			PRINTFORMW
		;その他
		ELSE
;OriginalString : 
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;안면승마中
	;=================================================
	IF 구상記入チェック(0) && TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0
;OriginalString : 
		PRINTFORMW 
		RETURN 1
