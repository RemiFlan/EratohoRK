;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・何か脱いだ FIRSTDATUI(15)
;------------------------------------------------
;・자위・봉사系の予備動作になりやすい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;죠교자が[정욕]상태
	;=================================================
	IF 구상記入チェック(100) && T_COND("정욕")
;OriginalString : 「후후후…お待ちかねのショータイムよ♪」
		PRINTFORMW 「후후후…학수 고대의 쇼 타임이야♪」
;OriginalString : 유카리はそう言ってじっくりと見せ付けるように服を脱ぎだ했다
		PRINTFORMDW 보라색은 그렇게 말해 차분히 과시하듯이 옷을 탈희생타 했다
		RETURN 1
	ENDIF
	
	;아라이멘도正
	IF ALI(0)
