;
;	[ここに구상主名を入れる]구상 탈의・追加アクション구상ファイル
;
;	죠교자が죠교指示以外に行う행동のファイル
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;//暫定仕様@/L

;────────────────────────────────────
;탈의アクション구상
;────────────────────────────────────
;・죠교자アクションの前に강제탈의や자주탈의が発生했다ときに表示
;・この구상の時点ではまだTEQUIPは更新されていない
;・初回分岐は「初めて見た/見せた」とは限らない。
;　ショップメニューで脱がせたことがあるかもしれないし、죠교の流れで完全に脱がせずに노출だけしている場合もある。
;　もしかすると조수だったときに見たことがあるかもしれない。そういう意味では簡易的なものである。
;　厳密な意味での「初見」플래그を用意するかどうかは要望次第ということで。
;-------------------------------------------------
@KOJO_MALE_DATUI_K44
;-------------------------------------------------
;関連変数…
;강제탈의：TFLAG:102　자주탈의：TFLAG:103
;=(-1, 강제탈의に抵抗、1, 겉옷、2, 하반신겉옷、3, 上衣、4, 상반신全部、5, 하반신全部、
;　6, 악세서리ー、7, 악세서리ー以外全て、8, 전라まで、9, 속옷だけ)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의に抵抗당했다 FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 < 0
	PRINTFORMW 「, 이거 참 무엇으로 저항해」
	PRINTFORMW 「벗지 않으면 시작할 수 없지 않다」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;二人とも何か脱いだ FIRSTDATUI(10)
;------------------------------------------------
;・主に섹스系행동の予備動作で発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;죠교자が[정욕]상태
	;=================================================
	IF 구상記入チェック(100) && T_COND("정욕")
		PRINTFORMW 「아-도-빨리 벗으세요」
;OriginalString : 「私だってこんなに濡れてんだから…%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「나도 이렇게 젖고 있는 것이니까…%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック(100)
		PRINTFORMW 「이봐요 이봐요, 와 벗는다♪」
		PRINTFORMW 「응？조금 기대해 버린다？♪」
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・악세서리ーのみ FIRSTDATUI(20)
;------------------------------------------------
;・主に신발안。他には장갑や소매など
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상記入チェック(0) && TFLAG:자주탈의 == 6 && !IS_NOWASSIACTNAME(, "탈의")
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・まんまんうｐ FIRSTDATUI(30)
;------------------------------------------------
;・今回の탈의で성기が見えるよう되었다場合に表示
;・ぱんつ見せを経由せず一気に脱ぐ場合あり
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;すでに가슴が見えてる
	;=================================================
	IF 구상記入チェック(100) && B_DATUICHECK(0) == 1
		PRINTFORMW 「알고 있다고…콧치도 보고 싶지요？」
;OriginalString : 「あーあ鼻息荒くしちゃって…%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「아-아콧김 난폭하게 해 버려…%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック(100)
		PRINTFORMW 「이봐요 팬츠 벗었어♪」
;OriginalString : 「そんなにジロジロ一生懸命見ないの%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「그렇게 빤히 열심히 보지 않는거야%HTML_HEARTB()%」")
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・おっぱいうｐ FIRSTDATUI(40)
;------------------------------------------------
;・今回の탈의でおっぱいが見えるよう되었다場合に表示
;・ブラ見せを経由せず一気に脱ぐ場合あり
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;すでに성기が見えてる
	;=================================================
	IF 구상記入チェック(100) && P_DATUICHECK(0) == 1
		PRINTFORMW 「젖가슴도 보고 싶다고？ 이봐요ﾍﾟﾛﾝ」
;OriginalString : 「もう…わかりやすいチンポねぇ%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「이제(벌써)…알기 쉬운 자지군요%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件なし
	;=================================================
	IF 구상記入チェック(100)
;OriginalString : 「こんなちっちゃい가슴見て反応するなんて…%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「이런 작다가슴봐 반응하다니…%HTML_HEARTB()%」")
		PRINTFORMW 「유두라면 뭐든지 좋은 것일까♪」
		RETURN 1
	ENDIF
ENDIF

[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・ぱんつ見せ FIRSTDATUI(50)
;------------------------------------------------
;・今回の탈의で팬티（キャラによって드로와즈・트렁크）が見えるよう되었다場合に表示
;・하반신속옷の無いキャラは書く필요なし
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・ブラ見せ FIRSTDATUI(60)
;------------------------------------------------
;・今回の탈의でブラ（キャラによって스포츠브라・サラシ）が見えるよう되었다場合に表示
;・상반신속옷の無いキャラは書く필요なし
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・전라に FIRSTDATUI(70)
;------------------------------------------------
;・「전라まで脱いだ」だけではなく、「今脱いだので전라되었다」という意味
;・「겉옷しか残っていない상태で겉옷を脱いだ」なども含まれる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:자주탈의 && ZENRACHECK(0) && !IS_NOWASSIACTNAME(, "탈의")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・악세서리ーのみ FIRSTDATUI(80)
;------------------------------------------------
;・主に신발안。他には장갑や소매など
;・脱が했다うちに入らない！となりがちなのでこれだけは部位別テンプレを用意
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:강제탈의 == 6
ENDIF
[SKIPEND]

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・ちんこ見せろ FIRSTDATUI(90)
;------------------------------------------------
;・今回の탈의で남성기が見えるよう되었다場合に表示
;・ぱんつ見せろを経由せず一気に脱がせる場合あり
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 3 && PENIS(MASTER)
	PRINTFORMW 「응은 빨리 자지 내세요♪」
;OriginalString : 「…%HTML_HEARTB()%　なかなか逞しいじゃない%HTML_HEARTB()%」")
	CALL HTMLPRINTW(@"「…%HTML_HEARTB()% 꽤 씩씩하지 않아%HTML_HEARTB()%」")
	RETURN 1
ENDIF

[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・ぱんつ見せろ FIRSTDATUI(120)
;------------------------------------------------
;・今回の탈의で하반신속옷が見えるよう되었다場合に表示
;・죠교대상が하반신속옷の無いキャラ限定の구상であれば書く필요がない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:강제탈의 && P_DATUICHECK(1) == 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・전라に FIRSTDATUI(140)
;------------------------------------------------
;・「전라まで剥いだ」だけではなく、「今剥いだので전라되었다」という意味
;・「겉옷しか残っていない상태で겉옷を剥いだ」なども含まれる
;・「おっぱい見せろ」～「ぱんつ見せろ」を書いた場合は記入する필요はない（上層の分岐に食われてここまで落ちてこない為）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:강제탈의 && ZENRACHECK(1)
ENDIF
[SKIPEND]

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・何か脱いだ FIRSTDATUI(150)
;------------------------------------------------
;・자위・봉사系の予備動作になりやすい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	PRINTFORMW 「-라고 와♪」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・何か剥いだ FIRSTDATUI(160)
;------------------------------------------------
;・애무系の予備動作になりやすい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의
	PRINTFORMW 「이봐요 이봐요 벗은 벗었다♪」
	RETURN 1
ENDIF

[SKIPSTART]
;========================================================================
;追加アクション구상
;========================================================================
;・죠교자が추가행동を取ったときに汎用的に表示
;・書く場合、アクション구상の後に続けて違和感のない形で書かれることが望ましい
;-------------------------------------------------
;────────────────────────────────────
;EXTRA1, Ｃ애무
;────────────────────────────────────
;・追加で애무または수음を行う
;・죠교대상に페니스がない場合は애무、ある場合は수음
;-------------------------------------------------
@KOJO_MALE_ACT_EXTRA_K44_1
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;EXTRA1_0, 애무する
;-------------------------------------------------
;・죠교대상に페니스がない場合
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && !PENIS(MASTER)
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;EXTRA1_1, 수음する
;-------------------------------------------------
;・죠교대상に페니스がある場合
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && PENIS(MASTER)
ENDIF

;────────────────────────────────────
;EXTRA2, Ｖ애무
;────────────────────────────────────
;・追加でヴァッジ애무を行う
;・죠교대상が남자の場合発生しない
;・죠교대상が처녀の場合は発生しない
;-------------------------------------------------
@KOJO_MALE_ACT_EXTRA_K44_2
;-------------------------------------------------
IF 구상記入チェック(0)
ENDIF

;────────────────────────────────────
;EXTRA3, Ａ애무
;────────────────────────────────────
;・追加で항문애무を行う
;-------------------------------------------------
@KOJO_MALE_ACT_EXTRA_K44_3
;-------------------------------------------------
IF 구상記入チェック(0)
ENDIF

;────────────────────────────────────
;EXTRA4, Ｂ애무
;────────────────────────────────────
;・追加で乳首責めを行う
;-------------------------------------------------
@KOJO_MALE_ACT_EXTRA_K44_4
;-------------------------------------------------
IF 구상記入チェック(0)
ENDIF

;────────────────────────────────────
;미약등사용구상
;────────────────────────────────────
;・죠교자が미약や로션を使用했다ときに汎用的に表示
;・「アクション구상」で記入チェックを=2と했다場合は表示されない
;・書く場合、アクション구상の後に続けて違和感のない形で書かれることが望ましい
;-------------------------------------------------
@KOJO_MALE_USE_K44
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;로션/미약同時使用
;------------------------------------------------
;・主に성교系コマンドから発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && GETBITAND(TFLAG:미약등사용구상, 0, 1)
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;로션使用
;------------------------------------------------
;・玩具・성교系コマンド等から発生する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && GETBIT(TFLAG:미약등사용구상, 0)
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;미약使用
;------------------------------------------------
;・애무系コマンド等から発生する
;・키스で発生했다場合は입移しとなるため、そのケースは키스のアクション구상内で処理するほうがよい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && GETBIT(TFLAG:미약등사용구상, 1)
ENDIF
[SKIPEND]
