	;=================================================
	;Ａ성교から
	;=================================================
	;IF 구상記入チェック(0) && TFLAG:ACT파생 == 3
	;ENDIF

	;=================================================
	;Ｖ성교系から
	;=================================================
	;IF 구상記入チェック(0) && TFLAG:ACT파생 == 4
	;ENDIF

	;=================================================
	;Ａ성교봉사から
	;=================================================
	IF 구상記入チェック(100) && TFLAG:ACT파생 == 5
		PRINTFORMW 「엉덩이도 좋지만…」
		CALL HTMLPRINTW(@"「이번은 여기로%HTML_HEARTB()% 가득 찔러%HTML_HEARTB()%」")
		RETURN 1
	ENDIF

	;=================================================
	;その他・条件無し
	;=================================================
	IF 구상記入チェック(100)
		CALL HTMLPRINTW(@"「이봐요빨리%HTML_HEARTB()%」")
		CALL HTMLPRINTW(@"「뒤로부터 와아…%HTML_HEART()% 교미하고 싶은으로 합시다…%HTML_HEARTB()%」")
		PRINTFORMDW 스와자는 귀여운 엉덩이를 후리 후리 시키면서 유혹해 왔다…
		RETURN 1
	ENDIF
ENDIF

