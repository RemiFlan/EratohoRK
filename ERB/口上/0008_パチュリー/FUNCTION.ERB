@EFFECT_SHOOT_K8
;엘리스구상よりお借りしてます
SELECTCASE NOWEX:MASTER:사정
	;드라이사정
	CASE 1
		CALL QUAKE(4)
	;대량사정
	CASE 3
		CALL FLASH(2)
	;その他
	CASEELSE
		CALL FLASH(1)
ENDSELECT

@RANDLIPS_K8(ARG)
#FUNCTIONS
#DIMS CONST STARTER = "あむっ", "はむっ", "かぷっ"
#DIMS CONST ENDER = "じゅるるるるるるるる", "ちゅーーーーーーーー"
SELECTCASE ARG
	CASE 0
		RETURNF @"%STARTER:(RAND:3)%……ん……んっ……ちゅぷ"
	CASE 1
		RETURNF "むちゅっ……ちゅぅ……ぷちゅっちゅるるっ"
	CASE 2
		RETURNF @"じゅるるっ…じゅるっ……%ENDER:(RAND:2)%"
	CASE 3
		RETURNF "ん……ちゅぷっ……こくっ……ちゅ"
ENDSELECT
