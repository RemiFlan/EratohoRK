;☆暫定仕様@/L

;────────────────────────────────────
;EV21, 大만족구상
;────────────────────────────────────
;・죠교자の대만족보너스を취득했다場合に表示
;-------------------------------------------------
@KOJO_EVENT_K1_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;주인님さま大만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
;대요정구상を参考にさせて頂きま했다
;が、うちのへたれいむさんではパワー不발だったよ…。自機キャラとしてはこれでいいのかもしれない
IF 구상記入チェック(25) && !TEQUIP:성교봉사중
	IF !FLAG:오토모드
		;PRINTFORMDW %CALLNAME:TARGET%さま大만족
		PRINTFORML
		PRINTFORML 「다음은 네가 만족하는 차례군요. 해 주었으면 한 일, 있는 것일까？」
		IF TEQUIP:MASTER:볼개그
			PRINTFORMDL %CALLNAME:TARGET%은(는) %CALLNAME:MASTER%의 구가를 풀면서 묻어 왔다
			TEQUIP:MASTER:볼개그 = 0
		ENDIF
		PRINTFORML
		PRINTFORMDW 아무래도%CALLNAME:TARGET%은(는) %CALLNAME:MASTER%의 요구에 응해 주는 것 같다…
		IF TFLAG:징계・플래그 && TEQUIP:MASTER:볼개그
;OriginalString : [0] んー！んー！
			PRINTDL [0] 응―！응―！
;OriginalString : [1] むー！むー！
			PRINTDL [1] ―！―！
			INPUT
			PRINTFORMW 「미안해요 자주(잘) 도취되어 듣지 않았어요」
			PRINTFORMDW %CALLNAME:TARGET%님의 웃는 얼굴은, 눈부시다.
			RETURN 1
		ENDIF
		;성교봉사要望はみだりに組み込むべきではない
;OriginalString : [0] もっと만족して欲しい
		PRINTDL [0] 좀 더 만족해 주었으면 한다
;OriginalString : [1] 입付けたい
		PRINTDL [1] 입맞추고 싶다
;OriginalString : [2] おっぱい
		PRINTDL [2] 젖가슴
;OriginalString : [3] ご봉사させてください
		PRINTDL [3] 봉사시켜 주세요
;OriginalString : [4] 踏んでくださいお願いします
		PRINTDL [4] 밟아 주세요 부탁합니다
;OriginalString : [5] 자위が見たい
		PRINTDL [5] 자위를 보고 싶다
		;ここが機能していない
		SIF EQUIP:악세서리 == 0
;OriginalString : [6] 소매を着けてください
			PRINTDL [6] 소매를 입어 주세요
		;初回分岐やゲームバランスへの영향が強い為条件付与
		;女당신の場合【굴복:22】질내사정당했다
		;男당신の場合【굴복:42】항문に사정されるを見る
		; || は使うべきではないのだがどう했다ものか
		SIF PENIS(TARGET) && (TALENT:MASTER:남자 == 0 && GETBIT(CFLAG:각인, 22)) || (TALENT:MASTER:남자 && GETBIT(CFLAG:각인, 42))
;OriginalString : [21] 안에 내!
			PRINTDL [21] 안에 내！
;OriginalString : [100] 特に要望は無い
		PRINTDL [100] 특히 요망은 없다
		INPUT
		IF RESULT == 0
			PRINTFORML （하나 하나 사랑스럽네요 진한 개）
			PRINTFORMDW %CALLNAME:TARGET%은(는) %CALLNAME:MASTER%의 가슴에 얼굴을 묻어 소매치기 소매치기 해 왔다
		ELSEIF  RESULT == 1
			;【굴복:11】키스を취득済みならば
			IF GETBIT(CFLAG:각인, 11)
				;解除処理
				IF TEQUIP:삼각목마
					PRINTFORML 「이대로는 닿지 않네요」
					TEQUIP:삼각목마 = 0
				ENDIF
				;解除処理
				IF TEQUIP:안면승마 || TEQUIP:안면승마항문
					PRINTFORMDL %CALLNAME:TARGET%은(는) %CALLNAME:MASTER%의 얼굴에 다시 향해 눈감았다
					TEQUIP:안면승마 = 0
					TEQUIP:안면승마항문 = 0
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%는 수긍해 눈감았다
				ENDIF
				;へたれな당신からは키스できない
				PRINTFORMDW 입술을 내며 입맞춤을 기다린다%CALLNAME:TARGET%에 %CALLNAME:MASTER%(은)는 넋을 잃고 보고 굳어져 버린다…
				PRINTFORML 
				PRINTFORMDL ………
				PRINTFORMW ………
				PRINTFORML 
				;描写不발。키스後とも受け取ることができてしまう
				CALL FADEOUT(4, "등")
				PRINTFORMW 「언제까지 굳어져」
				CALL FADEOUT(8, "「아니 뭐, 응」")
				PRINTFORMW 「역시 너는 너」
				;結局레이무から키스する
				TCVAR:행동예약 = 15
			;初키스は弾く
			ELSE
				PRINTFORML 「바보 아직. 아직, 저희들에게는 빨라요」
				PRINTFORMDW 수줍다%CALLNAME:TARGET%는 사랑스럽지만, 상황은 키스할 경황은 아니다
			ENDIF
		ELSEIF RESULT == 2
			PRINTFORMD %CALLNAME:TARGET%는 
			;도구解除処理
			IF TEQUIP:상반신겉옷
				PRINTFORMD 윗도리를 벗어
				TEQUIP:상반신겉옷 = 0
				TEQUIP:상반신속옷 = 0
			ENDIF
			;도구解除処理
			IF TEQUIP:상반신속옷
				PRINTFORMD 사라시를 풀면(자),
				TEQUIP:상반신겉옷 = 0
				TEQUIP:상반신속옷 = 0
			ENDIF
			;도구解除処理
			IF TEQUIP:MASTER:볼개그
				TEQUIP:MASTER:볼개그 = 0
			ENDIF
				
			PRINTFORMDL %CALLNAME:MASTER%의 눈앞에 아름답게 갖추어진 유방을 노골적으로 나타냈다
			;おっぱい吸わせる
			TCVAR:행동예약 = 57
			TFLAG:ACT파생 = 2
		ELSEIF RESULT == 3
			PRINTFORML 「좋아요. 네가 기분 좋게 하며」
			;도구解除処理
			IF TEQUIP:MASTER:볼개그 || TEQUIP:페니스밴드 || TEQUIP:삼각목마
				TEQUIP:MASTER:볼개그 = 0
				TEQUIP:페니스밴드 = 0
				TEQUIP:삼각목마 = 0
			ENDIF
			;밧줄は解除しない処理
			IF TEQUIP:MASTER:밧줄
				PRINTFORML 「그대로도 할 수 있네요」
			ENDIF
			;성교봉사分岐は矛盾の元になりそうなので一旦最上部で排除 死に分岐
			IF TEQUIP:성교봉사중
				PRINTFORMDW %CALLNAME:TARGET%은(는) %CALLNAME:MASTER%에 몸을 맡김 산등성이다 해 왔다
				;성교봉사 정상위 죠교자←죠교대상
				TCVAR:행동예약 = 95
			ELSE
				;初心さの補強
				PRINTFORMDW %CALLNAME:TARGET%는 이불에 누워%CALLNAME:MASTER%의 행위를 가슴을 크게 울리게 한 기다려 짓고 있다…
				;봉사 クンニ 죠교자←죠교대상
				TCVAR:행동예약 = 51
			ENDIF
		ELSEIF RESULT == 4
			PRINTFORML 「너의 변태는 죽어도 나을 것 같지도 않네요」
			;애무 발コキ
			TCVAR:행동예약 = 18
		ELSEIF RESULT == 5
			;しどろもどろに OUTは弾けるように速め INは絞り出すように遅め 非처녀になっても처녀性は大事
			;このバリアントに%CALLNAME:MASTER%攻め要素は不要な気がしなくもないが、레이무さんが可愛い
			
			;OUTは연속で使わないように注意。FADEにFORM機能は無い模様。調整の필요性を実感。OUTとINの推敲
			PRINTFORML 
			CALL FADEIN(4, "「와…」")
			CALL FADEIN(12, "「알았어요」")
			;주인님とは呼ばせない。旦那様も少し違う気がする。
			CALL FADEOUT(4, "「(이)닷！」")
			CALL FADEIN(8, "「남편,」")
			;凄いカタコト
			CALL FADEIN(8, "「」")
			PRINTFORML 
			PRINTFORMDW 딱딱하다%CALLNAME:TARGET%의 응대에 상관하지 않고%CALLNAME:MASTER%(은)는 계속을 재촉했다
			PRINTFORML 
			CALL FADEOUT(4, "「내가」")
			CALL FADEIN(8, "「내가 스스로」")
			CALL FADEIN(8, "「하는, 하는 것을…」")
			CALL FADEOUT(4, "「엣」")
			PRINTFORML 
			PRINTFORMDW %CALLNAME:MASTER%(은)는 개작년과(뿐)만에 %CALLNAME:TARGET%에 무엇을 하는지 묻었다…
			PRINTFORML 
			CALL FADEIN(12, "「무엇 말하게 하는거야…」")
			PRINTFORML 
			CALL FADEIN(8, "「이, 인,」")
			CALL FADEOUT(12, "「에―」")
			CALL FADEIN(8, "「자위！를 봐！만족！해 주세요！」")
			CALL FADEOUT(12, "「아아아아아……」")
			PRINTFORML 
			;下行との擦り合わせのため変更
			;PRINTFORMDW %CALLNAME:TARGET%は行為前から既に피폐しきっている…
			PRINTFORMD %CALLNAME:TARGET%는 행때문 전부터 이미 피폐 해
			;%CALLNAME:TARGET%피폐化 ぐったり하면서まんまんくちゅくちゅする姿ってエロいと思うの
			;機能していない為コメントアウト
			;TCVAR:TARGET:상태변화 = 1
			;PRINTFORMD %CALLNAME:TARGET%は
			;도구解除処理
			IF TEQUIP:눈가리개
				PRINTFORMD 아이마스크를 잡아 닫으면서
				TEQUIP:눈가리개 = 0
			ENDIF
			PRINTFORMDW 납죽 엎드림이 되어 고개 숙이고 있다…
			PRINTFORMDW 무방비인 하반신은 성적이면서도 어딘가 미소주위 것이 있었다…
			;せっかくなので素敵アングルで자위してもらう
			TEQUIP:안면승마 = 1
			;죠교자が자위
			TCVAR:행동예약 = 42
		ELSEIF RESULT == 6 && EQUIP:악세서리 == 0
			[SKIPSTART]
			IF 소매オナホに사정済みの条件が判明次第開放
				PRINTFORMDW %CALLNAME:MASTER%(은)는 스며들고 것 할 수 있던 소매를 입는 듯%CALLNAME:TARGET%에 부탁했다
				PRINTFORMDW %CALLNAME:TARGET%는 소매를 안주, 하부에 모인 국물을 확인하고 있다…
				PRINTFORML 
				PRINTFORMW 「위%RAND_SPLIT("/아/읏")%…하고 있어 소매의 모퉁이로부터 가는 체 물러날 수 있었던 정액이, 배어 나오고 있다」
			ELSE
			[SKIPEND]
				PRINTFORML 「이상한 곳에 구애받아요…뭐 좋아요…」
				PRINTFORMDL 흰 소매가%CALLNAME:TARGET%의 겨드랑이를 마음껏 북돋운다
				EQUIP:악세서리 = 2
			;ENDIF
		;コマンド出現条件と同じ条件を付け손打ちの排斥
		;女%CALLNAME:MASTER%의 場合【굴복:22】질내사정당했다 男%CALLNAME:MASTER%의 場合【굴복:42】항문に사정されるを要求
		ELSEIF RESULT == 21 && PENIS(TARGET) && (TALENT:MASTER:남자 == 0 && GETBIT(CFLAG:각인, 22)) || (TALENT:MASTER:남자 && GETBIT(CFLAG:각인, 42))
			;새드追加描写
			IF TALENT:새드
				CALL FADEOUT(20, "「에에. 제대로 알고 있어요」")
				CALL FADEIN(12, "「난폭하게당했다있는이네요」")
				IF ITEM:밧줄
					;PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%にお札を貼りつけた
					;可愛らしさの補強
					PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%조금 지폐를 붙였다
					TEQUIP:밧줄 = 1
				ENDIF
			ELSE
				PRINTFORMW 「예. 알았어요」
			ENDIF
			;おちんちんの準備ができていない場合
			IF TCVAR:TARGET:발기도 < 1000
				PRINTFORML 「그 전에 이것, 크게 해 주세요」
				PRINTFORMDW %CALLNAME:TARGET%는 %CALLNAME:MASTER%\@ TALENT:새드 ? 의 목 안에 음경을 쑤셔넣어 할 수 있었다 # 에입그래서봉사시키고사정준비를시켰다 \@
				EXP:MASTER:구음경험 ++
				;가버려な場合 このまま입内でいいや
				IF TCVAR:TARGET:가버려
					PRINTFORML 「아-안돼 이것, 이대로 입안에 사정해요」
					;이라마치오
					TCVAR:행동예약 = 56
					RETURN 1
				ELSE
					PRINTFORML 「이제 좋아요. 더 이상 하면 사정해버릴 그렇게」
					BASE:TARGET:사정 = MAXBASE:TARGET:사정
					TCVAR:TARGET:발기도 = 1500
					TCVAR:TARGET:가버려 = 1
				ENDIF
			ENDIF
			;당신にまんまんがある場合
			IF TALENT:MASTER:남자 == 0
				;도구解除処理
				IF TEQUIP:바이브 || TEQUIP:페니스밴드 || TEQUIP:삼각목마
					TEQUIP:페니스밴드 = 0
					TEQUIP:바이브 = 0
					TEQUIP:삼각목마 = 0
				ENDIF
				;女당신플레이ヤーには物발りないと思いますが통상男당신メインの구상なのでこの位で勘弁して下さい
				IF TALENT:새드
					;圧迫感を持たせる為ハートや音符は使わない。区切りが발りなくなるので為改行
					;PRINTFORML 「それじゃ、あんたの抜き穴使ってあげるからお尻をこっちに向けて自分で広げなさい」
					PRINTFORML 「좋아요. 너의 빼기구멍 사용해 주기 때문에, 엉덩이를 여기를 향하며」
					PRINTFORML 「스스로 넓히세요」
					;성교 후배위 죠교자→죠교대상
					TCVAR:행동예약 = 31
				ELSE
					PRINTFORML 「그러면, 합시다」
					;PRINTFORMDW %CALLNAME:TARGET%は%CALLNAME:MASTER%의 頬に입付けた
					PRINTFORMDW %CALLNAME:TARGET%은(는) %CALLNAME:MASTER%의 뺨에 입맞추면(자) 상냥하고 이불에 밀어 넘어뜨려 지워져 왔다
					;성교 정상위 죠교자→죠교대상
					TCVAR:행동예약 = 30
				ENDIF
			;당신にまんまんがない場合
			ELSE
				IF TEQUIP:MASTER:하반신겉옷 || TEQUIP:MASTER:하반신속옷
					TEQUIP:MASTER:하반신겉옷 = 0
					TEQUIP:MASTER:하반신속옷 = 0
				ENDIF
				;도구解除処理
				IF TEQUIP:항문비즈 || TEQUIP:항문바이브 || TEQUIP:관장그릇＋플러그 || TEQUIP:삼각목마
					TEQUIP:항문비즈 = 0
					TEQUIP:항문바이브 = 0
					TEQUIP:관장그릇＋플러그 = 0
					TEQUIP:삼각목마 = 0
				ENDIF
				PRINTFORML 「그러면 파랑엉덩이를 여기를 향하세요」
				;항문섹스 죠교자→죠교대상
				TCVAR:행동예약 = 35
			ENDIF
		ELSE
			PRINTFORMW 「생각해 떠오르지 않았던 것일까 해들 」
		ENDIF
	ENDIF
	;この後に続く%CALLNAME:TARGET%さま大만족を消去し上部に設置 消去できていない模様
	RETURN 2
ENDIF

;────────────────────────────────────
;EV22, 상태변화구상
;────────────────────────────────────
;・상태이상が付いたときに表示
;・TCVAR:상태변화が죠교자、TCVAR:MASTER:상태변화が죠교대상
;・상태변화が起こると3ターン지속する
;-------------------------------------------------
@KOJO_EVENT_K1_22(ARG)
;-------------------------------------------------
;*************************************************
;EV22_0, 죠교자상태변화
;*************************************************
;━━━━━━━━━━━━━━━━━━━━━━━━
;주인님さま[피폐]化	소모時    疲労のため消極的행동が우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && IS_CONDITION(TARGET, "피폐")

ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;주인님さま[쇠약]化	強소모時  疲労のため消極的행동が우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && IS_CONDITION(TARGET, "쇠약")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;주인님さま[무기력]化	무기력時  倦怠のため消極的행동が우선
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && IS_CONDITION(TARGET, "무기력")
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;주인님さま[몽롱]化	強절정時  快感のため享楽的행동が우선
