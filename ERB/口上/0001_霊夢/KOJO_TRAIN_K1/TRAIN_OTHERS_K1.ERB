;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG:3
	SETBIT すべて順に表示, 3
	SELECTCASE ARG:3
		;반항각인Lv2
		CASE 2
		;表示しない
		CASEELSE
			CLEARBIT すべて順に表示, 3
	ENDSELECT
	SIF GETBIT(すべて順に表示, 3) && 記入チェック == 1
		RETURN 1
ENDIF

