;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・ぱんつ見せろ FIRSTDATUI(120)
;------------------------------------------------
;・今回の탈의で하반신속옷が見えるよう되었다場合に表示
;・죠교대상が하반신속옷の無いキャラ限定の구상であれば書く필요がない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 && P_DATUICHECK(1) == 2
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(100)
		IF TCVAR:MASTER:발기도 >= 1000
			PRINTFORMW 「 이제 딱딱해질 것 같구나？속옷을 밀어 올리고 있어, 환원 빌려라」
			PRINTFORMW 「곧,%RAND_SPLIT("편하게 해 주기 때문에/부드럽게 해 주어요")%」
		ELSE
			PRINTFORMW 「 아직, (듣)묻지 말라고 않는 것 같구나？」
			PRINTFORMW 「곧 딱딱하게 해, 그리고 부드럽게 해 주어요」
		ENDIF
		
		;후타나리당신でそちんの場合 레이무さんは比較대상知らない
		;大丈夫、えのきには勝ってるわ。しめじには、いや、まあ、その
		;후타나리레이무さんだいなりおとこ당신
		;PRINTFORMW 「その속옷、可愛いわね」
		;PRINTFORML 「でも、속옷の中身はもっと可愛いわよ」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・ブラ見せろ FIRSTDATUI(130)
;------------------------------------------------
;・今回の탈의でブラ상반신속옷が見えるよう되었다場合に表示
;・죠교대상が남자だと表示しないので、남자限定구상では書く필요が無い
;・죠교대상が상반신속옷の無いキャラ限定の구상であれば書く필요がない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:강제탈의 && B_DATUICHECK(1) == 2 && !TALENT:MASTER:남자
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTDATUI(130)
		PRINTFORMW 「그 속옷, 사랑스럽네요」
	ENDIF
	
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・전라に FIRSTDATUI(140)
;------------------------------------------------
;・「전라まで剥いだ」だけではなく、「今剥いだので전라되었다」という意味
;・「겉옷しか残っていない상태で겉옷を剥いだ」なども含まれる
;・「おっぱい見せろ」～「ぱんつ見せろ」を書いた場合は記入する필요はない（上層の分岐に食われてここまで落ちてこない為）
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 && ZENRACHECK(1)
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(100)
		IF TCVAR:MASTER:발기도 >= 1000
			PRINTFORMW 「이것으로 전부군요」
		;皮の鎧
		ELSE
			PRINTFORMW 「벌써 한 장 있던 원이군요」
;OriginalString : 아웃8, "くすっ")
			CALL FADEOUT(8, "키득")
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・何か脱いだ FIRSTDATUI(150)
;------------------------------------------------
;・자위・봉사系の予備動作になりやすい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;죠교자が[정욕]
	;=================================================
	IF 구상記入チェック(0) && IS_CONDITION(TARGET, "정욕")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・何か剥いだ FIRSTDATUI(160)
;------------------------------------------------
;・애무系の予備動作になりやすい
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && TFLAG:강제탈의
	;=================================================
	;죠교자が[정욕]
	;=================================================
	IF 구상記入チェック(0) && IS_CONDITION(TARGET, "정욕")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

