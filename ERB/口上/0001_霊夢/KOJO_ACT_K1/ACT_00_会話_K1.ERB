;────────────────────────────────────
;ACT6, 합의를 요구한다
;────────────────────────────────────
;・初めてを貰ってもいいかと聞く
;・初めて＝처녀or동정(両方の場合もある)
;・既に합의を得ている場合は発生しない
;・죠교대상が처녀でも동정でもない場合は発生しない
;・TFLAG:징계・플래그が立っていると発生しない
;・턴방침が"이상"だと発生しない
;・話題『합의』が既出となり、以後、同一조교중では抑制される
;-------------------------------------------------
@KOJO_ACT_K1_6
;-------------------------------------------------
;関数@TARGET_DESCRIPTIONでTEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)で地の文は변화する
;あとは好みで分岐を増減してください
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT6, 합의를 요구한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0)
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상이남자の場合
	;=================================================
	IF 구상記入チェック(0) && TALENT:MASTER:남자
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상が후타나리の場合
	;=================================================
	IF 구상記入チェック(0) && TALENT:MASTER:후타나리
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상が여자の場合
	;=================================================
	IF 구상記入チェック(0) && !PENIS(MASTER)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;性別毎の書き分けをしないなら、ここだけ書いておきましょう
	;남자、후타나리、여자分岐を全て埋めた場合は不要です。削除しましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT7, 징계한다고 위협한다
;────────────────────────────────────
;・これから징계・すると言い出す
;・このACTが発生している時点でTFLAG:징계・플래그は立っている
;・턴방침が"소프트", "이상"だと発生しない
;・TEQUIP:밀어넘어지는중時には発生しない
;-------------------------------------------------
@KOJO_ACT_K1_7
;-------------------------------------------------
;ここでは地の文の변화はない。分岐の増減はお好みで
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT7, 징계한다고 위협한다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0)
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다」のいずれか
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「기분좋게 해」（この恥知らず！）
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("기분좋게 해")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「더 하드하게 해」（この豚め！）
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("더 하드하게 해")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「용서를 빌다」（許してもらえませんで했다）
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("용서를 빌다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「자위를 시작한다」（人の話を聞け！）
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("자위를 시작한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT8, 고함치다
;────────────────────────────────────
;・大声で고함치다
;・턴방침が"소프트", "노멀"では発生しない
;・TFLAG:징계・플래그に関わらず、発生する可能性がある（징계・中にさらに고함치다可能性等）
;-------------------------------------------------
@KOJO_ACT_K1_8
;-------------------------------------------------
;地の文は"%CALLNAME:TARGET%は%CALLNAME:MASTER%を大声で怒鳴った…"のみで派生はない
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT8, 고함치다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0)
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが「무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다」のいずれか
	;=================================================
	IF 구상記入チェック(0) && IS_PREVCOMNAME("무뚝뚝하게 응답한다/강하게 응답한다/싫어한다/날뛴다/거부한다")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT9, 기분나쁘게 웃다
;────────────────────────────────────
;・不気味な微笑を浮かべる
;・이상系から発生する場合もある
;・금일의방침が"노닥노닥"だと発生しない
;・話題『嗤う』が既出となり、以後、同一조교중では抑制される
;-------------------------------------------------
@KOJO_ACT_K1_9
;-------------------------------------------------
;関数@TARGET_DESCRIPTIONによってTEQUIP(성교중/죠교대상Ｃ사용/성교봉사중/자위중 & 2)で地の文は변화する
;기타・조건없음を最初に埋めた上で、あとは好みで分岐を増減してください
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT9, 기분나쁘게 웃다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100)
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;PENIS(TARGET)/!PENIS(TARGET)/TEQUIP:죠교자페니스밴드等で分岐してもよい
	;=================================================
	;성교중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:성교중
		;体位による派生
		SELECTCASE TEQUIP:성교중
			;정상위				체중을 실어 %CALLNAME:MASTER%의 질내에 가득
			CASE 1
				PRINTFORMW 
			;후배위				뒤에서 %CALLNAME:MASTER%의 질내에 가득
			CASE 2
				PRINTFORMW 
			;기승위				%CALLNAME:MASTER%의 암컷 구멍에 끝까지 밀어넣어
			CASE 3
				PRINTFORMW 
			;대면좌위			밀착한 %CALLNAME:MASTER%의 질을 애무하며
			CASE 4
				PRINTFORMW 
			;배면좌위			%CALLNAME:MASTER%에게 삽입하여 껴안고
			CASE 5
				PRINTFORMW 
			;항문섹스		%CALLNAME:MASTER%의 의 항문에 넣어
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;죠교대상のＣへの애무が行われている状況。TEQUIP관리なので可能性は割りとある
	;=================================================
	;죠교대상Ｃ사용
	;=================================================
	IF 구상記入チェック(0) && GROUPMATCH(TEQUIP:죠교대상Ｃ사용, 1, 2)
		;수음・애무継続中
		IF TEQUIP:죠교대상Ｃ사용 == 1
			PRINTFORMW 
		;펠라・クンニ継続中
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;PENIS(MASTER)/!PENIS(MASTER)/TEQUIP:페니스밴드等で分岐してもよい
	;=================================================
	;성교봉사중
	;=================================================
	IF 구상記入チェック(0) && TEQUIP:성교봉사중
		;体位による派生
		SELECTCASE TEQUIP:성교봉사중
			;정상위시킨다				大きく開いた股の間に迎え入れ
			CASE 1
				PRINTFORMW 
			;후배위시킨다				背後から질내に삽입され
			CASE 2
				PRINTFORMW 
			;역강간					ぬるぬるの膣できゅうきゅうと絞り上げ
			CASE 3
				PRINTFORMW 
			;대면좌위시킨다				体を密着させて膣奥に歓迎し
			CASE 4
				PRINTFORMW 
			;배면좌위시킨다				背を預けて堪能し
			CASE 5
				PRINTFORMW 
			;항문섹스시킨다		腸液にぬめる肛門で締め付け
			CASE 6
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자が자위중
	;=================================================
	IF 구상記入チェック(0) && GETBIT(TEQUIP:자위중, 1)
		;죠교자が페니스밴드身に付けている
		IF TEQUIP:죠교자페니스밴드
			PRINTFORMW
		;その他
		ELSE
			PRINTFORMW
		ENDIF
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(100)
		PRINTFORMW 「―！%CALLNAME:TARGET%에 맡겨！」
		RETURN 1
	ENDIF
ENDIF
