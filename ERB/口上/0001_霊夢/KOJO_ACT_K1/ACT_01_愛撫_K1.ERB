	;後ろから
	;=================================================
	IF 구상記入チェック(0) && TCVAR:MASTER:위치전후 == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;前から
	;=================================================
	IF 구상記入チェック(0) && TCVAR:MASTER:위치전후 == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		RETURN 1
	ENDIF
ENDIF

