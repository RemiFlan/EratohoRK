	;상반신服装が要押し退け			服をまくりあげて
	IF BRACHECK(1, 1)
		PRINTFORMW
	;その他
	ELSE
		PRINTFORMW
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT11_2, 乳の揉み合い
;━━━━━━━━━━━━━━━━━━━━━━━━
;・가슴の揉み合いを要求する。この時点での죠교대상の反応は未定
;・乳首系の条件から漏れた場合、죠교자の욕망+Ｂ감각３以上なら、1/2で가슴の揉み合いになる
;-------------------------------------------------
IF 구상記入チェック(0) && TFLAG:ACT파생 == 2
	;=================================================
	;상반신服装が邪魔			服の上から
	;=================================================
	IF 구상記入チェック(0) && BRACHECK(1)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;상반신겉옷が要押し退け		겉옷をまくり上げ
	;=================================================
	IF 구상記入チェック(0) && UPPERJACKET(1) == 1
		;속옷もまくり上げた
		IF UPPERUNDER(1) == 1
			PRINTFORMW 
		;その他
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;상반신겉옷が要押し退け		속옷の裾から손を忍ばせ
	;=================================================
	IF 구상記入チェック(0) && UPPERUNDER(1) == 1
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;기타・조건없음
	;=================================================
	IF 구상記入チェック(0)
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF

