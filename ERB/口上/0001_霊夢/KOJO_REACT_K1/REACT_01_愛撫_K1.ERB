		;;消費기력による派生
		;SELECTCASE GET_COM_RANK()
		;	;自然な행동
		;	CASE "自然な행동"
		;		PRINTFORMW 
		;	;無理のない행동
		;	CASE "無理のない행동"
		;		PRINTFORMW 
		;	;無理を했다행동
		;	CASE "無理を했다행동"
		;		PRINTFORMW 
		;ENDSELECT
		;RETURN 1
		;=================================================
		;펠라に対し消極的 反応が薄い場合
		;=================================================
		IF 구상記入チェック(50) && IS_COMGRONAME("소극적으로 한다") && IS_NOWCOMNAME("싫어한다/쾌감을 참는다") && TFLAG:REACT파생 < 1
