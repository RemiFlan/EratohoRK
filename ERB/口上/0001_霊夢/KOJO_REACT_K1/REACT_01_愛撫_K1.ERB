			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「엉덩이 만지작거릴 때 자지도군요 시켜 버려서」
;OriginalString : 아웃4, "  くす")
					CALL FADEOUT(4, "  쿠쿡")
