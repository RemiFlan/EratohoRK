;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,3 はっきりと今の気持ちを告げた
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류が적극적으로 한다の場合
;-------------------------------------------------
IF 구상記入チェック(100) && TFLAG:ACT파생 == 3
	;=================================================
	;임신 역강간で子供出来ちゃってどんな気持ちどんな気持ち もうすぐ生まれそう
	;=================================================
	IF 구상記入チェック(0) && TALENT:임신
		RETURN 1
	ENDIF
	;自然な행동
	;=================================================
	;REACT부하0, 一緒に居られることが嬉しいと솔직に입に했다
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "自然な행동"
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「-응」
				PRINTFORML
				PRINTFORMW 「…더운 물은. 기대해도 좋아요」
			CASEELSE
				PRINTFORMW 「 나도」
				PRINTFORMW 「쭉 앞의 일이지만…내가 할머니가 되어도, 그 말을 (듣)묻고 싶어요」
			ENDSELECT
		RETURN 1
	ENDIF
	
	;無理のない행동
	;=================================================
	;REACT부하1, はっきりと今の気持ちを告げた
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理のない행동"
		PRINTFORMW 「정말？」
		PRINTFORMDW %CALLNAME:TARGET%는 기대가 깃들인 눈으로 %CALLNAME:MASTER%(을)를 응시하고 있다
		RETURN 1
	ENDIF
	
	;無理を했다행동
	;=================================================
	;REACT부하2, 機嫌を損ねないよう、取り繕って答えた
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理を했다행동"
		PRINTFORMW 「무리하고 있지」
		PRINTFORML
		PRINTFORMW 「영력은 있어」
		PRINTFORMW 「그 말, 정말로 해 주어요」
		RETURN 1
	ENDIF
	
	;REACT부하分岐を全て埋めたら不要な箇所
	;逆に上層の分岐を書かないのであればここで補完させましょう
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック(0)
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,4 まあまあ悪くはない
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류が태연하게 한다の場合
;-------------------------------------------------
IF 구상記入チェック(100) && TFLAG:ACT파생 == 4
	;自然な행동
	;=================================================
	;REACT부하0, そう悪く思ってはいないと答えた
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "自然な행동"
		;%CALLNAME:MASTER%が죠교される時、주인님もまた죠교されている。%CALLNAME:TARGET%さんが無理を했다積極的행동
		IF AFFECTION("익애")
;OriginalString : 아웃8, "「そ、そう…」")
			CALL FADEOUT(8, "「그, 그렇게…」")
			PRINTFORML 「곳에서, 좋은 차과자가 있는데…」
;OriginalString : 아웃8, "そわそわ")
			CALL FADEOUT(8, "안절부절")
		PRINTFORMDW %CALLNAME:TARGET%는 당돌하게 차과자를 권해 왔다
			
		ELSE
		ENDIF
		PRINTFORMW 「긍정적으로 파악해 두어요」
		PRINTFORMW 「…차과자, 있어요？」
		PRINTFORMDW %CALLNAME:TARGET%는 당돌하게 차과자를 권해 왔다
		RETURN 1
	ENDIF
	
	;無理のない행동
	;=================================================
	;REACT부하1, 「まあまあ悪くはない」と言った
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理のない행동"
		PRINTFORMW 「확실치 않네요―」
		;ここに안면승마分岐を作ろう
		RETURN 1
	ENDIF
	
	;無理を했다행동
	;=================================================
	;REACT부하2, 無理を했다행동
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理を했다행동"
		[SKIPSTART]
		コメントアウトしておきます@/L
		;実は積極			視線を外し、悪くはないよと呟いた
		IF COMCOR_POSI()
		;実は嫌				精一杯の強がりで、まあまあ悪くないと答えた
		ELSE
			PRINTFORMW 
		ENDIF
		[SKIPEND]
		PRINTFORMW 「그런 일 말해, 무리하고 있지」
		PRINTFORMW 「 나에게는 솔직해질 수 없다？」
		RETURN 1
	ENDIF
	
	;REACT부하分岐を全て埋めたら不要な箇所
	;逆に上層の分岐を書かないのであればここで補完させましょう
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック(0)
		RETURN 1
	ENDIF
ENDIF

