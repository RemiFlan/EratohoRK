;========================================================================
;죠교アクション・コマンド구상
;========================================================================

;────────────────────────────────────
;탈의アクション구상
;────────────────────────────────────
;・죠교자アクションの前に강제탈의や자주탈의が発生했다ときに表示
;・この구상の時点ではまだTEQUIPは更新されていない
;・初回分岐は「初めて見た/見せた」とは限らない。
;　ショップメニューで脱がせたことがあるかもしれないし、죠교の流れで完全に脱がせずに노출だけしている場合もある。
;　もしかすると조수だったときに見たことがあるかもしれない。そういう意味では簡易的なものである。
;　厳密な意味での「初見」플래그を用意するかどうかは要望次第ということで。
;-------------------------------------------------
@KOJO_NO9_MS_DATUI_K14
;-------------------------------------------------
;関連変数…
;강제탈의：TFLAG:102　자주탈의：TFLAG:103
;=(-1, 강제탈의に抵抗、1, 겉옷、2, 하반신겉옷、3, 上衣、4, 상반신全部、5, 하반신全部、
;　6, 악세서리ー、7, 악세서리ー以外全て、8, 전라まで、9, 속옷だけ)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의に抵抗당했다 FIRSTDATUI(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:강제탈의 < 0
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && FIRSTDATUI(0)
		PRINTFORMW 「뭐야, 거역할 생각？ 아무튼에서도…」
		PRINTFORMW 「저항한다면 억지로 벗게 할 때까지」
		RETURN 1
	ENDIF
	
	;=================================================
	;自分は脱いだ
	;=================================================
	IF 구상記入チェック(100) && TFLAG:자주탈의
		PRINTFORMW 「사양이 없는 사람…」
		PRINTFORMW 「뭐, 나는 탈이 시켜 주는데」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	;죠교자BASE:초조함派生
	SELECTCASE TENSION(TARGET, 7)
		CASE "高い"
			PRINTFORMW 「또 그렇게 거역하군요」
			PRINTFORMW 「소야, 귀부인……, 나를 초조하게 하는 재능이 있어요」
		CASE "一般"
			PRINTFORMW 「……귀찮은 사람」
			PRINTFORMW 「 나에게 억지로알몸에 벗겨지는 것이 소망？」
		CASE "低い"
			PRINTFORMW 「%CNAME(MASTER, TARGET)%, 어쩌면…」
			PRINTFORMW 「입은 채로 하는 것이, 좋아하기도 하다 의 것인지 해들…？」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;二人とも何か脱いだ FIRSTDATUI(10)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・主に섹스系행동の予備動作で発生する
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:강제탈의 > 0 && TFLAG:자주탈의 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;죠교자が[정욕]かつ전라되었다
	;=================================================
	IF 구상記入チェック(100) && T_COND("정욕") && ZENRACHECK(0)
		PRINTFORMW 「이런 것, 이제 필요없어요！」
		PRINTFORM 「, 태어난 채로의 모습으로 즐깁시다…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;죠교자が죠교대상を익애
	;=================================================
	IF 구상記入チェック(0) && AFFECTION("익애")
		PRINTFORMW 「, 벗고 벗으며」
		PRINTFORM 「후후…, 마음껏 즐깁시다…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;-------------------------
	;その他
	;-------------------------
	;죠교자BASE:만족派生
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			PRINTFORMW 「역시 이러하지 않으면」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%의 체온을 느끼고 싶고. 뭐, 나의 신체는…」
			PRINTFORM 「조금 서늘할지도이지만. 그런데…
			CALL PRINT_SYMBOL_K14
		CASE "一般"
			PRINTFORMW 「입은 채로 하는 것도 나쁘지 않은데…」
			PRINTFORMW 「역시 방해군요？ 후후…」
		CASE "低い"
			PRINTFORMW 「준비는 좋을까…？」
			PRINTFORM 「그러면, 조속히 초\@ AFFECTION("익애") ? 짊어진다 # 원이야 \@
			IF AFFECTION("익애")
				CALL PRINT_SYMBOL_K14
			ELSE
				PRINTW 」
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・악세서리ーのみ FIRSTDATUI(20)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・主に신발안。他には장갑や소매など
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:자주탈의 == 6 && !IS_NOWASSIACTNAME(, "탈의")
	;=================================================
	;전라되었다（脱ぐ前は신발안以外裸とかそういう感じ）
	;=================================================
	IF 구상記入チェック(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !TEQUIP:눈가리개
		SELECTCASE RANDDATUI(2, 20)
			CASE 0
				PRINTFORMW 「응, 봐 주세요. 나의 알몸 아무쪼록 해들…？」
			CASE 1
				PRINTFORMW 「알몸에 양말이라든지(분)편이\@ AFFECTION("상사상애") ? 좋았다？ # %CNAME(MASTER, TARGET)%의 기호였는지 해들 \@」
				PRINTFORM 「아무튼에서도…, 이제 벗어 버렸다 것은 방법 없어요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 「 이제 %RAND_SPLIT("여분/안녕")%것은 필요하지 않네요」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・まんまんうｐ FIRSTDATUI(30)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・今回の탈의で성기が見えるよう되었다場合に表示
;・ぱんつ見せを経由せず一気に脱ぐ場合あり
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && TALENT:TARGET:주인님 && FIRSTDATUI(30)
		PRINTFORMW 「어때？ 나의 누구에게도 보인 일이 없는 부분이야」
		PRINTFORMW 「그 의미를 눈치채지 않을 수록,%CNAME(MASTER, TARGET)%가 바보같지 않으면 좋지만, 이군요」
		RETURN 1
	ENDIF
	
	;=================================================
	;すでに가슴が見えてる
	;=================================================
	IF 구상記入チェック(100) && B_DATUICHECK(0) == 1
		PRINTFORMW 「그런 눈으로 볼 수 있으면(자)…」
		PRINTFORM 「여기도 보이지 않는 것에는 가지 않네요…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;ちんこうｐ
	;=================================================
	IF 구상記入チェック(100) && PENIS(TARGET)
		SELECTCASE RANDDATUI(2, 30)
			CASE 0
				PRINTFORMW 「이봐요, 봐 주세요……」
				PRINTFORMW 「이것으로%CNAME(MASTER, TARGET)%(을)를 학째 싶어서, 이제 이렇게 되어 버린다의」
			CASE 1
				PRINTFORMW 「, 자주(잘) 봐 두세요」
				PRINTFORM 「코레로%CNAME(MASTER, TARGET)%가 어떻게 하셔버리는 의 것인지, 상상하면서. 그런데…
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		;フェードアウト
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? ふふふ # フフフ \@…」")
		CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「\@ AFFECTION("상사상애") ? 후후후 # 후후후 \@…」")
		CLEARLINE 1
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	PRINTFORMW 「이봐요, 사양하지 말아줘」
	PRINTFORM 「 좀 더 자주(잘) 봐도 좋아…
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;・今回の탈의でおっぱいが見えるよう되었다場合に表示
;・ブラ見せを経由せず一気に脱ぐ場合あり
;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・おっぱいうｐ FIRSTDATUI(40)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 3 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && TALENT:TARGET:주인님 && !TALENT:거유 && FIRSTDATUI(40)
		PRINTFORMW 「크다고만큼이 아니지만, 형태에는 자신이 있어…？」
		PRINTFORMW 「어머나, 사양하는 일 없어요. 좀 더 잘 봐도 좋아요」
		PRINTFORMW 「 나의 이런 모습을 볼 수 있는 일에, 감사하면서！」
		PRINTFORMDW %CALLNAME%(은)는 이익 그렇게, 그렇게 말해 발했다…
		RETURN 1
	ENDIF
	
	;=================================================
	;すでに성기が見えてる	50%
	;=================================================
	IF 구상記入チェック(50) && P_DATUICHECK(0) == 1
		PRINTFORMW 「응 소야…, 나의 가슴 아무쪼록 해들？」
		RETURN 1
	ENDIF
	
	;ONCEで表示했다ら以後その조교중では表示させない
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(100) && ONCEDATUI(40)
		IF 0
		;죠교자が거유
		ELSEIF TALENT:거유
			PRINTFORM 「\@ TALENT:MASTER:작은체형 ? 눈을 빛내어져 버려 사랑스러워요 # 눈초리가 추잡해요 \@…
			CALL PRINT_SYMBOL_K14
			PRINTFORMW 「큰 젖가슴을 좋아하는 사람은, 아이 같은 곳 있네요」
			;죠교대상が작은체형
			SIF TALENT:MASTER:작은체형
				PRINTFORMW 「은%CNAME(MASTER, TARGET)%…, 아직 아이였어. 후후…」
		;죠교자が빈유
		ELSEIF TALENT:빈유
			PRINTFORMW 「작은 가슴이 좋아라든지, 진짜%CNAME(MASTER, TARGET)%도 괴짜군요？」
			PRINTFORMW 「이봐요, 이것을 보고 싶었었지요？」
		ELSE
			PRINTFORMW 「머뭇머뭇 하지 않고, 어차피라면 좀 더 당당히 보면？」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%가 무엇을 생각하고 있을까 라고, 벌써 전망이야」
			;1/3の確率で真
			SIF !RAND:3
				PRINTFORMW 「볼 수 있었다고 줄어드는 것이 아니고, 군요」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・ぱんつ見せ FIRSTDATUI(50)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・今回の탈의で팬티（キャラによって드로와즈・트렁크）が見えるよう되었다場合に表示
;・하반신속옷の無いキャラは書く필요なし
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:자주탈의 && P_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && TALENT:TARGET:주인님 && FIRSTDATUI(5)
		PRINTFORMW 「어때？ 이것\@ KFLAG:TARGET:300 == 2 ? 도 # , \@나의 수제야」
		PRINTFORMW 「사람에 따라서는 속옷 모습을 좋아하기도 하다다운데…」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%의 경우는 어떤가 해들. 응？」
		KFLAG:TARGET:300 = 1
		RETURN 1
	ENDIF
	
	;ONCEで表示했다ら以後その조교중では表示させない
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(100) && ONCEDATUI(50)
		;2/3で真
		IF RAND:3
			PRINTFORMW 「응 아무쪼록 해들？」
			PRINTFORM 「이번은,
			CALL WHICH_LINGERIE(COLOR2_K14("ALICE"))
			PRINTFORMW (으)로 해 보았지만」
			PRINTFORMW 「이봐요, 어떻게야！？ 무엇인가%RAND_SPLIT("멋이 있는 칭찬말/감상")%의 하나 정도,%RAND_SPLIT("言いなさいよっ！/言えない訳？")%」
		;1/3
		ELSE
			PRINTFORMW 「응, 자주(잘) 봐…」
			PRINTFORMW 「 이제 %RANDTEXT_K14("滲みになってる", "少し濡れちゃってる")%의, 아는 것일까？」
			;1/3で真
			IF !RAND:3
				PRINTFORM 「%CNAME(MASTER, TARGET)%(을)를 학일 생각하면, 이렇게 되어버린다 것이군요…
				CALL PRINT_SYMBOL_K14
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「フフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후…」")
				CLEARLINE 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・ブラ見せ FIRSTDATUI(60)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・今回の탈의でブラ（キャラによって스포츠브라・サラシ）が見えるよう되었다場合に表示
;・상반신속옷の無いキャラは書く필요なし
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:자주탈의 && B_DATUICHECK(0) == 2 && !IS_NOWASSIACTNAME(, "탈의") && !TEQUIP:눈가리개
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && TALENT:TARGET:주인님 && FIRSTDATUI(60)
		PRINTFORMW 「이봐요, 꽤 사랑스러운 브라겠지？ 이것\@ KFLAG:TARGET:300 == 1 ? 도 # , \@나의 손수만들기야」
		PRINTFORMW 「그 중%CNAME(MASTER, TARGET)%에도 갖춤의 것을 만들어 주어도 괜찮네요」
		KFLAG:TARGET:300 = 2
		RETURN 1
	ENDIF
	
	;ONCEで表示했다ら以後その조교중では表示させない
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(100) && ONCEDATUI(60)
		PRINTFORM 「어때？ \@ TIME ? 오늘 밤 # 금일 \@하
		CALL WHICH_LINGERIE(COLOR2_K14("ALICE"))
		PRINTFORMW 브라로 해 보았어」
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「이것, 생각보다는 자신작이지만…라고」
				PRINTFORMW 「이봐요, 얼마나-로 해？ 칭찬한다면 지금 가운데야！」
			CASE "一般"
				PRINTFORMW 「마음에 들어 받을 수 있었는지 해들. 라고  할까…」
				PRINTFORMW 「입다물지 말고, 무엇인가 말하세요」
			CASE "低い"
				PRINTFORMW 「…무엇 입다물어. 무엇인가 말하는 것, 있겠지요？」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;자주탈의・전라に FIRSTDATUI(70)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・「전라まで脱いだ」だけではなく、「今脱いだので전라되었다」という意味
;・「겉옷しか残っていない상태で겉옷を脱いだ」なども含まれる
;------------------------------------------------
IF 구상記入チェック(100) && TFLAG:자주탈의 && ZENRACHECK(0) && !IS_NOWASSIACTNAME(, "탈의")
	IF AFFECTION("상사상애") && !TEQUIP:눈가리개
		PRINTFORMW 「응, 나를 봐…나의 알몸, 깨끗할까…？」
	ELSE
		PRINTFORMW 「, 준비는OK라는 곳일까」
	RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;강제탈의・악세서리ーのみ FIRSTDATUI(80)
;━━━━━━━━━━━━━━━━━━━━━━━━
;・「전라まで脱いだ」だけではなく、「今脱いだので전라되었다」という意味
