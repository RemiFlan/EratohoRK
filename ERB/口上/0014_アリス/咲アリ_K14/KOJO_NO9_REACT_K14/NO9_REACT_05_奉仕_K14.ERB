;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && IS_COMGRONAME("受け入れる") && !IS_NOWCOMNAME("이루는대로")
	CALL PRINT_COMGRO_4_K14
	RETURN 1
ENDIF

;-----------------------------
;ACT파생毎の処理
;-----------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생1, 손으로 애무強制
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:ACT파생
	;=================================================
	;REACT분류が소극적으로 한다
	;=================================================
	IF 구상記入チェック(100) && IS_COMGRONAME("소극적으로 한다")
		;以下、COM毎の分岐
		;COM날뛴다はREACT파생0でない場合、暴れは通らない
		;COMで싫어한다ACT妨害になる場合は弾く
		;-------------------------------------------------
		;싫어한다/날뛴다 RANDREACT(0)
		;-------------------------------------------------
		IF 구상記入チェック(100) && (IS_NOWCOMNAME("싫어한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2) || (IS_NOWCOMNAME("날뛴다") && TFLAG:REACT파생 > 0)
			SELECTCASE RANDREACT(2, 0)
				CASE 0
					PRINTFORMW 「이봐요, 그렇지 않아서……」
					PRINTFORMW 「진짜, 속상하고 이상해져 그래요！」
				CASE 1
					PRINTFORMW 「이제 와서 싫어해도 어쩔 도리가 없다고 것에, 완전히…」
					PRINTFORMW 「이제 되니까…, 나의 손의 움직임에 맞추세요」
			ENDSELECT
			RETURN 1
		ENDIF
		
		;派生「消極的なのが気に入らない」の場合は弾く。負荷2は弾く。確率を絞る
		;-------------------------------------------------
		;소극적으로 한다
		;-------------------------------------------------
		IF 구상記入チェック(50) && IS_NOWCOMNAME("소극적으로 한다") && !TFLAG:REACT파생 && TFLAG:REACT부하 < 2
			IF AFFECTION("상사상애")
				PRINTFORMW 「아……팥고물……아…」
				PRINTFORMW 「응……좀 더 강하게 비비어……」
			ELSEIF AFFECTION("익애")
				PRINTFORMW 「뭐야, 어쩐지 부실 같네요…」
				PRINTFORMW 「이봐요, 좀 더 분명하게 해…」
			ELSEIF AFFECTION("권애")
				PRINTFORMW 「뭐야, 나의 가슴에 손대고 싶지 않은거야…？」
			ELSEIF AFFECTION("嫌悪")
				PRINTFORMW 「이봐요,！ 좀 더 정중하게 하세요！」
				PRINTFORMW 「진짜 서투름대변…」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%의 기억의 나쁨에는 기가 막혀요…」
			ELSE
				PRINTFORMW 「의욕이 없는 것일까… 사용할 수 없네요」
			ENDIF
			RETURN 1
		ENDIF
		RETURN 1
	ENDIF
	
	;無理を했다행동時は弾く。確率を絞る
	;=================================================
	;적극적으로 한다
	;=================================================
	IF 구상記入チェック(70) && IS_NOWCOMNAME("적극적으로 한다/태연하게 한다") && TFLAG:REACT부하 < 2
		;죠교자쾌Ｂ派生
		SELECTCASE PALAM:쾌Ｂ
			CASE IS >= 8500
				;背後から
				IF TCVAR:MASTER:위치전후 == 3
					PRINTFORM 「응구！ 응응, 아…
						CALL AUTOHEARTCB_K14
					PRINTFORMW 　, 어젯밤！ 나, 이제…」
;OriginalString : (COLOR_K14("사쿠야"), "「…わかったわ。ほら엘리스、こう？」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…알았어요. 이봐요엘리스, 이렇게？」")
					PRINTFORM 「아…
						CALL AUTOHEARTCB_K14
					PRINTFORMW 　(이)나, 기다려！ 아, 안돼！」
					CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「…って。本当に待ったら怒る癖に」")
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「まぁ、待つつもりもないんだけど、ね")
						CALL PRINT_SYMBOLM_K14
				;その他
				ELSE
					PRINTFORM 「응！ (이)나……아
					CALL AUTOHEARTCB_K14
					PRINTFORMW 　팥고물……！」
					PRINTFORMW 「그렇게 되면(자)……도, 이제…나！」
					CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「괜찮아、わかってるわ」")
;OriginalString : (COLOR_K14("사쿠야"), "「いいわよ、엘리스。このまま……」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「좋아요,엘리스이대로……」")
				ENDIF
			CASE IS >= 4000
				;背後から
				IF TCVAR:MASTER:위치전후 == 3
					PRINTFORMW 「응아！ , 사쿠야……그렇게 되면(자), 소리가 나와버린다」
;OriginalString : (COLOR_K14("사쿠야"), "「ふふ、いいじゃない。それに私も、貴女の声が聞きたいわ」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후후, 좋지 않아. 거기에 나도, 당신의 소리를 듣고 싶어요」")
;OriginalString : (COLOR_K14("사쿠야"), "「というか、後ろからされる方が興奮するのかしら？」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「라고 할까, 뒤로부터 되는 (분)편이 흥분할까？」")
					PRINTFORMW 「몰라요, 그런 것…」
;OriginalString : (COLOR_K14("사쿠야"), "「ま、いいわ。そういう事にしておいてあげても、ね」")
