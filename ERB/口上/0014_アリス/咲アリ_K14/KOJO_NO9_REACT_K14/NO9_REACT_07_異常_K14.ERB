;-------------------------------------------------
@KOJO_NO9_MS_REACT_K14_75
;-------------------------------------------------
;・固有の地の文なし。죠교대상の反応に応じてREACTION_MESSAGEで汎用処理
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생0, 장착
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:ACT파생
	;TARGETが처녀でない場合はFIRSTREACT等を使うといいのかも知れない
	;まぁ손間なんでお好みでと言ったところ
	;=================================================
	;죠교자が처녀を喪失
	;=================================================
	IF 구상記入チェック(100) && TCVAR:처녀상실
		PRINTFORMW 「 그렇지만 역시, 이것……」
;OriginalString : (COLOR_K14("사쿠야"), "「やっぱり、なによ…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「역시, 뭐야…」")
		PRINT 「
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "すっっごく痛いわ", 1)
		PRINTW ！」
;OriginalString : (COLOR_K14("사쿠야"), "「……自業自得、ね」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「……자업자득이, 군요」")
		RETURN 1
	ENDIF
	
	SELECTCASE TRAINNAME:SELECTCOM
		CASE "침묵한다"
			PRINTFORMW 「뭐야, 입다물어 버려서. 트마라나이원이군요」
			RETURN 1
		CASE "마음 약하게 응답한다"
			;派生：애원する
			IF IS_COMGRONAME("용서를 빌다")
				PRINTFORMW 「이런 일 그만두어는, 지금부터가 좋으면 진한 것에」
			;派生：마음 약하게 응답한다
			ELSE
				PRINTFORMW 「어머나, 상당히 기운이 없지 않다」
				PRINTFORMW 「경쟁이 없어서, 조금 퇴색해버리는 원이군요」
			ENDIF
			RETURN 1
		CASE "무뚝뚝하게 응답한다", "강하게 응답한다", "皮肉をいう"
			PRINTFORMW 「%CNAME(MASTER, TARGET)%가 사랑스럽지 않은 것은 알았지만…」
			PRINTFORMW 「언제에도 더욱 더 사랑스럽지 않네요！」
			RETURN 1
		CASE "상냥하게 응답한다"
			PRINTFORMW 「후후, 드물고 애교가 있지 않다」
			PRINTFORMW 「무엇인가를 기대해, 는 (뜻)이유인가 해들. 응？」
			RETURN 1
		CASE "더 하드하게 해", "기분좋게 해"
			PRINTFORMW 「그런 일 말해 버려서, 정말로 좋은 것일까？」
			PRINTFORMW 「후회해도 몰라요\@ TFLAG:REACT파생 == 1 ? ！ # 후후… \@」
			RETURN 1
		CASE "용서를 빌다"
			PRINTFORMW 「이 상황으로 허락해라고 말을 들어도, 무엇을 허락하면 좋은 것일까. 응？」
			RETURN 1
		CASE "무서워한다"
			PRINTFORMW 「후후…, 그런 느낌으로 무서워할 수 있으면(자)……」
			PRINTFORMW 「불필요하게 학째 싶어져버리는 원」
			RETURN 1
		CASE "자위를 시작한다"
			;派生：勝손にオナニーするな
			IF TFLAG:REACT파생 == 1
				PRINTFORMW 「은？ 뭐 제멋대로인 금년 비치는 것？」
				PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 조금은 참는 일을 기억하는 것이 좋아요……」
			;派生：勝손にオナニーしてるけどいいか
			ELSE
				PRINTFORMW 「그렇게 무슨맥략도 없고, 당돌하게 시작할 수 있어도」
				PRINTFORMW 「뭐, 좋아하는 대로 해 주세요. 화낼 생각도 안 돼요…」
			ENDIF
			RETURN 1
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

