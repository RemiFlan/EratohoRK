				;行削除
				CLEARLINE 1
				CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「あっ、んんっ！ やっ！ 엘리스、そんなとこ…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗, 응읏！ 얏！ 엘리스, 그런 곳…」")
				PRINTFORMW 「부끄러워하는 일은 없어, 사쿠야. 좀 더 솔직하게 받아들여…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;今の状況に対して부끄러워한다
	;=================================================
	IF 구상記入チェック(100) && !TFLAG:REACT파생
