				CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「へぇへぇ。それは結構な事ね」")
				PRINTFORMW 「뭐야, 사랑스럽지 않네요」
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「")
				CALL PRINTFONTSTYLE(COLOR2_K14("SAKUYA"), "それはこっちの台詞よ", 1)
;OriginalString : (COLOR_K14("사쿠야"), "」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "」")
			CASE "一般"
				PRINTFORML 「심한 일 말해요…나, 이것이라도 열심히 해 」
