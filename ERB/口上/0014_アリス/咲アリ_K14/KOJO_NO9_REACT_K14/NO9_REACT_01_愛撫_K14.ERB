	;快感に対して부끄러워한다
	;=================================================
	IF 구상記入チェック(100) && TFLAG:REACT파생 == 1
		;ACT파생が乳首吸いの場合
		IF TFLAG:ACT파생 == 1
			PRINTFORMDL 앨리스는 수치에 몸부림사쿠야를 억누르는 것처럼,
			PRINTFORMDW 사쿠야의 %OPPAI_DESCRIPTION(MASTER)%의 첨단의 뇌를 혀끝에서 연주해, 입에 넣게 하면서 애무하고 있다…
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("乳首吸い"), 1)%")
			CLEARLINE 1
		;その他
		ELSE
			PRINTFORMDW 앨리스는 수치에 몸부림사쿠야를 응시하면서, 섬세한 손가락 값어치사쿠야의 %OPPAI_DESCRIPTION(MASTER)%(을)를 애무하고 있다…
		ENDIF
		PRINTL
		;죠교자만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「あっ、あんっ！ や…おねがい、엘리스。みないで…っ」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아, 아읏！ 야…부탁,엘리스보지 마…읏」")
				PRINTFORMW 「싫어요. 이런 사랑스러운 사쿠야, 분별없게 볼 수  없는 것…」
				PRINTFORMDW 앨리스는 수치에 눈동자를 물기를 띠게 하는 사쿠야의 머리를 어루만지면(자), 얼굴을 대고 상냥하게 입맞추고 했다…
				;키스を捏造
				CALL SET_KISS
			CASE "一般"
				CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「はっ、あぁっ！ やだ、엘리스っ！ こ、こんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「핫, 아앗！ 싫다,엘리스읏！ 이, 이런 건…」")
				PRINTFORM 「후후, 아직도 시초야. 좀 더 부끄러운 일 해 주기 때문에…
				CALL PRINT_SYMBOL_K14
			CASE "低い"
				CALL QUAKE(1)
;OriginalString : (COLOR_K14("사쿠야"), "「あっ、んんっ！ やっ！ 엘리스、そんなとこ…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「앗, 응읏！ 얏！ 엘리스, 그런 곳…」")
				PRINTFORMW 「부끄러워하는 일은 없어, 사쿠야. 좀 더 솔직하게 받아들여…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;条件無しで補完させずにREACT파생0で決めうちする
	;=================================================
	;今の状況に対して부끄러워한다
	;=================================================
	IF 구상記入チェック(100) && !TFLAG:REACT파생
		;ACT파생が乳首吸いの場合
		IF TFLAG:ACT파생 == 1
			PRINTFORMDL 앨리스는 수치에 몸부림사쿠야를 억누르는 것처럼,
			PRINTFORMDW 사쿠야의 %OPPAI_DESCRIPTION(MASTER)%의 첨단의 뇌를 혀끝에서 연주해, 입에 넣게 하면서 애무하고 있다…
			;フェードアウト
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"     %KOJO_RAND_K14(("乳首吸い"), 1)%")
			CLEARLINE 1
		;それ以外
		ELSE
			PRINTFORMDW 앨리스는 수치에 몸부림사쿠야를 응시하면서, 섬세한 손가락 값어치사쿠야의 %OPPAI_DESCRIPTION(MASTER)%(을)를 애무하고 있다…
		ENDIF
		PRINTL
		;죠교자만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「사쿠야는 의외로 우브인 것？ 적합하지 않은 생각도 들지만, 그러한 것도 나쁘지 않아요…♪」
;OriginalString : (COLOR_K14("사쿠야"), "「う、ウルサイわね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄럽네요…」")
				PRINTFORM 「후후, 사랑스럽다. , 이 앨리스씨가 좀 더 부끄러운 일 해 주어요…
			CASE "一般"
				PRINTFORMW 「응 사쿠야, 가르쳐 주세요. 도대체 무엇이 그렇게 부끄러운거야？」
;OriginalString : (COLOR_K14("사쿠야"), "「そ、そんな事言える訳…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 일 말할 수 있는 것…」")
				PRINTFORM 「그렇게, 그렇지만…지금부터 좀 더 부끄러운 일 해 주기 때문에, 각오 해 주세요…
			CASE "低い"
				PRINTFORMW 「응 사쿠야, 나에게 손대어지는 것이 그렇게 부끄럽다？」
;OriginalString : (COLOR_K14("사쿠야"), "「き、聞かないでよ。そんな事…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「드, 듣지 말아. 그런 일…」")
				PRINTFORM 「후후, 좋아요. 더 이상은 (듣)묻지 않고 두어 준다. 지금은, 이군요…
		ENDSELECT
		;SELECTCASEを抜けてからまとめて閉じる
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
RETURN 0
ENDIF
