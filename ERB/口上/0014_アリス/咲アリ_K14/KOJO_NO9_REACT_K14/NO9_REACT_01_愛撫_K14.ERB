				CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「ちょ、ちょっと待ちなさいよ！」")
;OriginalString : (COLOR_K14("사쿠야"), "「なんで私が悪いみたいな流れになるのか、判らないんだけど」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「어째서 내가 나쁜 것 같은 흐름이 되는지, 알지 않지만」")
				PRINTFORMW 「……완전히. 정말로 심한 이야기군요」
				
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「")
				CALL PRINTFONTSTYLE(COLOR2_K14("SAKUYA"), "人の話を聞きなさいよ", 1)
;OriginalString : (COLOR_K14("사쿠야"), "」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "」")
			CASE "低い"
				PRINTFORML 「어머나, 말해 주어요？」
;OriginalString : (COLOR_K14("사쿠야"), "「なによ、悪い？」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐야, 나쁘다？」")
				PRINTFORMW 「별로？ 다만…, 그러한 사쿠야도, 허리 사용이 허술해 싫증 마지막 없지만, 이군요」
				CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「言ってくれるわね…」")
				PRINTFORMW 「뭐야, 나쁘다？」
				CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「…………」")
				PRINTFORMW 「…………」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;快感を感じながら
	;=================================================
	IF 구상記入チェック(100) && TFLAG:REACT파생 == 1
		;만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「그러한 것은 사쿠야인것 같다고는 생각하는데……거짓말은 좋지 않네요？」
;OriginalString : (COLOR_K14("사쿠야"), "「わ、私は嘘なんか！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「나, 나는 거짓말 따위！」")
