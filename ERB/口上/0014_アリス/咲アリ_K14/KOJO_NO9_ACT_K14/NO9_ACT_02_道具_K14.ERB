;========================================================================
;도구系 ACT20～26
;========================================================================
;・TFLAG:ACT(20=로터/21=바이브/22=항문바이브/23=항문비즈/24=Ｃ애무도구/25=유두캡/26=페니스밴드)
;・TFLAG:ACT분류 == 2に該当
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "도구")で拾う事が可能
;・죠교자が죠교대상に도구を使う。この구상の時点で죠교자はすでに손を出している
;・미약・로션を使用するケースがある
;・도구類の장착、解除等はTFLAG:ACT파생で定義される
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では구상側のRESULT(終端のRETURN 1など)によって구상が表示당했다のか否か、
;・また箇所毎の特有の挙動(미약等구상をスキップなど)を制御しています
;・@구상記入チェックの場合代入処理を省略できますが、RESULT値は구상側で決めうちする필요があります
;-------------------------------------------------
;도구系実行の際に강제탈의が入る仕様が不満
;非常に面白くない。これは改善されるべき
;-------------------------------------------------
;関連関数…
;FIRSTACT
;初回ACT判定関数
;TFLAG:ACT（TFLAG:90）を自動で参照、現在実行されているACTごとに번호が独立して保存される
;
;ONCEACT
;조교중未読判定関数　죠교前に初期化される
;その他の仕様はほぼFIRSTACTと同様
;-------------------------------------------------

;────────────────────────────────────
;ACT20, 로터
;────────────────────────────────────
;・Ｃ中心に애무する
;・애무系から発生する場合もある
;・TFLAG:징계・플래그が立っていると発生しない
;・턴방침が"하드", "이상"だと発生しない
;・TEQUIP(클리캡/오나홀/페니스밴드/삼각목마)장착中には発生しない
;・성교중/성교봉사중には発生しない
;・죠교대상に페니스がある場合は発生しない
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_20
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT20, 로터
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:ACT파생
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック() && FIRSTACT(0)
;OriginalString : (COLOR_K14("사쿠야"), "「な、なによ！ それ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「뭐, 뭐야！ 그것！」")
		PRINTFORMW 「어머나, 소야도 모르구나？ 우선 연성은 해 보았지만, 나도 자주(잘)은 모르는거야…」
		PRINTFORMW 「과 아무튼 그러한 (뜻)이유, 그리고…소야로 시험하게 해 주는 일로 해요♪」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;ONCEACT
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		SELECTCASE POLICY("금일") ;금일の죠교방침派生処理
			CASE "통상"
				PRINTFORMW
			CASE "항문"
				PRINTFORMW
			CASE "징계・"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "イチャイチャ"
				PRINTFORMW
			CASEELSE ;その他
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;これは要らないかな とりあえず枠だけ
	;=================================================
	;가버려
	;=================================================
	IF 구상記入チェック(0) && TCVAR:MASTER:가버려
		SELECTCASE TCVAR:MASTER:애태움도
			CASE IS > 1 ;애태움中
				PRINTFORMW
			CASE 1 ;애태움終了
				PRINTFORMW
			CASEELSE ;애태움無し
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;연속실행
	;=================================================
	;これが成立する可能性は限りなく低い。何処かで何か仕込むしか
	;TEQUIPで관리されてる訳じゃないしねぇ
	;-------------------------------------------------
	IF 구상記入チェック(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신服装が要押し退け
		;-------------------------------------------------
		IF 구상記入チェック(100) && PANTSCHECK(1, 1)
			;하반신속옷が要押し退け
			IF LOWERUNDER(1) == 1
				;속옷をずらして
				IF GETBIT(TEQUIP:착의노출, 2)
					PRINTFORMDW 앨리스는 비켜 놓았다%INNER_B(MASTER)%(으)로부터 들여다 본 소야의 %CLITORIS_DESCRIPTION(MASTER)%음핵을 로터로 계속 꾸짖고 있다…
;OriginalString : (COLOR_K14("사쿠야"), "「んぅっ、んっ…や、だめぇっ、いやぁぁっ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏…야, 안돼엣, 싫어엇！」")
					;開発済み
					IF ABL:MASTER:Ｃ감각 > 5
						PRINTFORMW 「그런 일 말해도, 소리에 단 것이 섞이는 것은 숨길 수 있지 않아요？ 후후」
					;開発中
					ELSE
						PRINTFORMW 「직접은 자극이 너무 강한 의 것인지 해들？ 소야의 일이니까 곧바로 익숙해지겠지요지만. 후후」
					ENDIF
				;布地越しに
				ELSE
					PRINTFORMDW 앨리스는%INNER_B(MASTER)%의 위로부터 소야의 %CLITORIS_DESCRIPTION(MASTER)%음핵을 로터로 계속 애무했다…
;OriginalString : (COLOR_K14("사쿠야"), "「んぅっ、んっ…んく…あっ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏…응쿠…앗！」")
					;開発済み
					IF ABL:MASTER:Ｃ감각 > 5
						PRINTFORMW 「 이제 이래서야 소야에는 어딘지 부족한 느낌 까, 군요♪」
					;開発途上
					ELSE
						PRINTFORMW 「 아직 그다지 익숙해지지 않는 느낌 까…그렇지만 속옷 위로부터라면 나쁘지 않을 것입니다？」
					ENDIF
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;穿いてない
		;---------------------
		PRINTFORMDW 앨리스는 소야의 노출이 되었다%VAGINA_DESCRIPTION(MASTER)%(을)를 로터로 계속 애무했다…
;OriginalString : (COLOR_K14("사쿠야"), "「んぅっ、んっ…やぁっ！ いやぁぁっ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「응웃, 읏…야앗！ 싫어엇！」")
		;開発済み
		IF ABL:MASTER:Ｃ감각 > 5
			PRINTFORMW 「그런 일 말해도, 소리에 단 것이 섞이는 것은 숨길 수 있지 않아요？ 후후」
		;開発途上
		ELSE
			PRINTFORMW 「직접은 자극이 너무 강한 의 것인지 해들？ 소야의 일이니까 곧바로 익숙해지겠지요지만. 후후」
		ENDIF
		RETURN 1
	ENDIF
	
	;微妙なところ 枠だけ置いておく
	;=================================================
	;유두캡/클리캡장착中
	;=================================================
	;IF 구상記入チェック(0) && TRUECHECK(TEQUIP:유두캡, TEQUIP:클리캡) > 0
	;ENDIF
	
	;=================================================
	;하반신服装が要押し退け
	;=================================================
	IF 구상記入チェック(100) && PANTSCHECK(1, 1)
		;하반신속옷が要押し退け
		IF LOWERUNDER(1) == 1
			;속옷をずらして
			IF GETBIT(TEQUIP:착의노출, 2)
				PRINTFORMDL 앨리스는%INNER_B(MASTER)%(을)를 비켜 놓아 소야의 %VAGINA_DESCRIPTION(MASTER)%(을)를 노출시키면(자),
				PRINTFORMDW 로터를 꽉 눌러%CLITORIS_DESCRIPTION(MASTER)%음핵을 요시작했다…
;OriginalString : (COLOR_K14("사쿠야"), "「ふぁっ！ あぁっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후앗！ 아앗！」")
				PRINTFORMW 「후후, 굉장히 이케나이일 하고 있는 기분이 되어요, 이것」
				PRINTFORMW 「어때？ 역시 직접 자극되는 것은 기분 좋은 것일까？」
				;開発済み
				IF ABL:MASTER:Ｃ감각 > 5
					CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「あっ、あっ")
					CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
					PRINTFORMW 「후후, (들)물을 것도 없었다 같구나♪」
				;開発中
				ELSE
;OriginalString : (COLOR_K14("사쿠야"), "「や、やだっ엘리스！ こんなの…ん、んんっ！」")
					CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「시, 싫닷엘리스！ 이런 건…응, 응읏！」")
					PRINTFORMW 「 아직 익숙해지지 않는 것 같구나？ 그렇지만 괜찮아, 곧바로 좋아져요♪」
				ENDIF
			;布地越しに
			ELSE
				PRINTFORMDW 앨리스는%INNER_B(MASTER)%넘어로 로터를 꽉 눌러%CLITORIS_DESCRIPTION(MASTER)%소야의 음핵을 요시작했다…
;OriginalString : (COLOR_K14("사쿠야"), "「んっ！ あっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏！ 앗！」")
				PRINTFORMW 「이런 자극이 강한 것은, 속옷 넘어(분)편이 좋은 것 같아요？」
				;開発済み
				IF ABL:MASTER:Ｃ감각 > 5
					PRINTFORMW 「보통은, 이군요. 소야에는 조금 어딘지 부족할지도이지만♪」
				;開発中
				ELSE
					PRINTFORMW 「조금씩 길들여 가 주어요, 소야♪」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	
	;-------------------------
	;穿いてない
	;-------------------------
	PRINTFORMDW 앨리스는 노출이 된 소야의 %VAGINA_DESCRIPTION(MASTER)%에 로터를 꽉 누르고%CLITORIS_DESCRIPTION(MASTER)%음핵을 요시작했다…
;OriginalString : (COLOR_K14("사쿠야"), "「ふぁっ！ あぁっ！」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「후앗！ 아앗！」")
	PRINTFORMW 「어때？ 좀 더 강 시력이 좋다？ 그렇지 않으면 약한 것이 좋은 것일까？」
	IF ABL:MASTER:Ｃ감각 > 5 ;開発済み
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「あっ、あっ")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
		PRINTFORMW 「후후, 알았다. 좀 더 강하게 시라고 주어요♪」
	ELSE ;開発中
;OriginalString : (COLOR_K14("사쿠야"), "「や、やだっ엘리스！ こんなの…ん、んんっ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「시, 싫닷엘리스！ 이런 건…응, 응읏！」")
		PRINTFORMW 「그렇게…그러면, 조금 상냥하게 시라고 주어요♪」
	ENDIF
	RETURN 1
ENDIF
;NO9_ACT20, 로터

;────────────────────────────────────
;ACT21, 바이브
;────────────────────────────────────
;・죠교대상が남자の場合は発生しない
;・TFLAG:징계・플래그がたっていると発生しない
;・턴방침が"이상"だと発生しない
;・TEQUIP(삼각목마/페니스밴드)장착中だと発生しない
;・성교중には発生しない
;・죠교대상が처녀でも発生する可能性がある
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_21
;-------------------------------------------------
;・TFLAG:ACT파생(0=바이브장착/1=解除)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT21_0, 바이브장착
;━━━━━━━━━━━━━━━━━━━━━━━━
;삽입に至らない、바이브による애무とかあってもいい
;-------------------------------------------------
IF 구상記入チェック(100) && !TFLAG:ACT파생
	;=================================================
	;初回
	;=================================================
	IF 구상記入チェック(100) && FIRSTACT(0)
		IF TEQUIP:눈가리개
			PRINTFORMDW %CALLNAME%(은)는 살그머니%CALLNAME:MASTER%의 눈 가림을 제외해 주었다…
			TEQUIP:눈가리개 = 0
			PRINTL
		ENDIF
		PRINTFORMW 「응, 소야는 이것 본 적 있는 것일까？」
;OriginalString : (COLOR_K14("사쿠야"), "「――見た事ないけど。何に使うのか、想像はつくわ…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「――본 일 없지만. 무엇에 사용하는지, 상상은 붙어요…」")
		PRINTFORMW 「후후, 유석…소쇄인 가정부는 추찰이 좋네요. 뭐, 이 형태, 그 밖에 보고 잘못하자도 없겠지만♪」
;OriginalString : (COLOR_K14("사쿠야"), "「…で。それで何をしようって言うのよ」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「…그래서. 그래서 무엇을 하자고 말하는거야」")
		
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "「사쿠야…前言撤回するわ。まだわからないの？ 貴女に使うに決まってるじゃない")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「ちょ、ちょっと待って！ さすがに私もそれだけは嫌！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「조, 조금 기다려！ 과연 나도 그것만은 싫어！」")
		PRINTFORMW 「어째서？」
;OriginalString : (COLOR_K14("사쿠야"), "「どうしても！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아무래도！」")
		PRINTFORMW 「구별하고가 없는 것, 말하지 말고 주세요. 모처럼 소야를 위해 마력으로 연성했으니까」
;OriginalString : (COLOR_K14("사쿠야"), "「何が折角よ！ そんなもの、頼んだ覚えはないわよ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무엇이 모처럼！ 그런 것, 부탁한 기억은 없어요！」")
		
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "「大丈夫、大丈夫♪ 사쿠야ならきっとすぐ慣れるわ。病み付きになっちゃうかも")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTFORMW 「, 관념해. 얌전하고 없음 있고, 소야. 후후…」
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;仮設
	;=================================================
	;ONCEACT
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		SELECTCASE POLICY("금일") ;금일の죠교방침派生処理
			CASE "통상"
				PRINTFORMW
			CASE "항문"
				PRINTFORMW
			CASE "징계・"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "イチャイチャ"
				PRINTFORMW
			CASEELSE ;その他
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	
	;とりあえず枠だけ設置
	;=================================================
	;가버려
	;=================================================
	IF 구상記入チェック(0) && TCVAR:MASTER:가버려
		SELECTCASE TCVAR:MASTER:애태움도
			CASE IS > 1 ;애태움中
				PRINTFORMW
			CASE 1 ;애태움終了
				PRINTFORMW
			CASEELSE ;애태움無し
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;연속실행
	;=================================================
	;※これは成立しないのでREACTなりでサルベージする필요アリ
	;---------------------------------------------
	IF 구상記入チェック(100) && TFLAG:ACT == TFLAG:전ACT
		PRINTFORMDW 준구바이브래이터를%VAGINA_DESCRIPTION(MASTER)%에 파묻힌 소야는,\@ ABL:MASTER:Ｖ감각 < 5 ? 질내의 이물감에 # 쾌락에 \@몸부림, 허덕이고 있다…
		CALL QUAKE(1)
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「あっ、あっ")
		;ハートを表示して括弧を閉じてWAIT
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("SAKUYA"), "　んんっ、はっ、あぁっ！」")
		;Ｖ開発済み
		IF ABL:MASTER:Ｖ감각 > 5
			PRINTFORMW 「완전히 마음에 드는 것같구나？ 어쩐지 보고 있는 내가 흥분해버릴까도, 이것…」
		;開発途上
		ELSE
			PRINTFORMW 「생각했던 것보다 마음에 들어 줄 수 있고 있는 것 같구나. 후후…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;항문바이브/항문비즈장착中(二穴責め)
	;=================================================
	IF 구상記入チェック(100) && TRUECHECK(TEQUIP:항문바이브, TEQUIP:항문비즈) > 0
		PRINTFORMW 「전이 외로운 것 같구나, 소야. 여기에도 갖고 싶은 것일까？ 후후…」
