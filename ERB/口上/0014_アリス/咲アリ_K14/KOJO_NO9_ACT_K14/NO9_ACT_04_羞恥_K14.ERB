;
;	엘리스구상(대상限定) 수치系アクション구상ファイル
;
;		수치系アクション구상をまとめたファイル
;
;	EXTRA모드でNO:9사쿠야をMASTERに선택했다場合のみ表示
;
;Version		Reason
;1.70			@구상記入チェック仕様に。その他細かな修正
;
;_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/_/#

;========================================================================
;수치系 ACT40～45
;========================================================================
;・TFLAG:ACT(40=자위/41=비패개장/42=자위과시/43=수치플레이/44=야외플레이/45=방뇨/46=코스프레(미구현))
;・TFLAG:ACT분류 == 4に該当
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "수치")で拾う事が可能
;・ACTによっては거부が可能であったりなかったり
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では구상側のRESULT(終端のRETURN 1など)によって구상が表示당했다のか否か、
;・また箇所毎の特有の挙動(미약等구상をスキップなど)を制御しています
;・@구상記入チェックの場合代入処理を省略できますが、RESULT値は구상側で決めうちする필요があります
;-------------------------------------------------
;関連変数…
;TFLAG:징계・플래그
;TCVAR:MASTER:상태변화(죠교대상の상태)
;TCVAR:상태변화(죠교자の상태)
;TFLAG:ACT
;TFLAG:전ACT
;PREVCOM 전회COM
;-------------------------------------------------
;関連関数…
;FIRSTACT
;처음ACT判定関数
;TFLAG:ACT（TFLAG:90）を自動で参照、現在実行されているACTごとに번호が独立して保存される
;
;ONCEACT
;조교중未読判定関数　죠교前に初期化される
;その他の仕様はほぼFIRSTACTと同様
;-------------------------------------------------

;────────────────────────────────────
;NO9_ACT40, 자위
;────────────────────────────────────
;・オナニーしてみせろと命令する
;・この段階で命令に従うか否かは未定
;・REACT분류が소극적으로 한다/태연하게 한다/적극적으로 한다/受け入れる以外の場合は、全てREACT분류"거부"扱い
;・턴방침が"이상"だと発生しない
;・밀어넘어지는중である場合は発生しない
;・すでに자위중である場合は発生しない
;・TEQUIP(밧줄/삼각목마)장착中には発生しない
;・성교중/성교봉사중には発生しない
;・このACTに派生はない
;-------------------------------------------------
;服を全て着た상태でも起こり得る
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_40
;-------------------------------------------------
;・죠교대상に페니스がない場合
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT40, 자위
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !PENIS(MASTER)
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(100) && FIRSTACT(0)
		PRINTFORMW 「그런데, 소야에는 여기서 자위를 해 받아요」
;OriginalString : (COLOR_K14("사쿠야"), "「なっ！？ ここでしろって言うの？ 貴女の目の前で？」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫！？ 여기서 해라고 말해？ 당신의 눈앞에서？」")
		PRINTFORMW 「그래요？ 그 이외의 무언가에 들린 것이라면, 나의 말투가 나빴다는 일이군요」
;OriginalString : (COLOR_K14("사쿠야"), "「そんな、人前でなんて…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런, 남의 앞에서는…」")
		PRINTL
		PRINTFORMW 「후후, 유석의 소야도 처음의 일같구나？」
		PRINTFORMW 「 그렇지만 신경쓰는 일 없어요. 언제나 하고 있는 것 같게, 평상시 대로시라고 주면 그것으로 좋은거야」
		PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「簡単に言ってくれるわね…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「간단하게 말해 주어요…」")
		PRINTFORMW 「이라도 간단한 일이겠지？ 굳이 어려운 일 해라고 하고 있는 작정(생각)은 없는 것」
		PRINTFORMW 「, 수다는 여기까지. 소야가 어떤 바람으로 하는지, 나에게 보이게 해 주세요…」
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが가게해줘！
	;=================================================
	IF 구상記入チェック(100) && IS_PREVCOMNAME("가게해줘！")
		[SKIPSTART]
		;-------------------------------------------------
		;전ACTが자위（現在の仕様だと死に分岐）
		;-------------------------------------------------
		IF 구상記入チェック(0) && IS_PREVACTNAME("자위")
			PRINTFORMDW 앨리스는 이나 키라고！(와)과 조르는 소야에, 한층 더 자위를 계속하듯이 명했다…
			PRINTL
			PRINTFORMW 「이봐요, 스스로 시 비치기 때문에, 사람을 믿지 않고 스스로 이도 참 어떻게야？」
			PRINTFORMDW 앨리스의 말은 닿지 않고, 소야는 열중해 자신을 위로하고 있다…
			PRINTL
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
			CLEARLINE 1
			CALL QUAKE(1)
			CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「んんっ、あっ")
			CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　やだっ、とまらない！ とまらないのっ！」")
			PRINTFORMW 「지금은 무슨 말을 해도 헛됨같구나…」
			RETURN 1
		ENDIF
		[SKIPEND]
		
		;---------------------
		;その他
		;---------------------
		PRINTFORMDW 앨리스는 이나 키라고！(와)과 조르는 %CALLNAME:MASTER%에, 자위를 하듯이 명했다…
		PRINTL
		;2/3の確率で表示
		IF RAND:3
			;죠교자BASE만족派生
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORMW 「좋아요？ 마음대로 이 오세요」
					PRINTFORMW 「우선, 나는 아무것도 해 줄 생각은 없으니까…」
					CALL FADEIN(13, "「스스로 어떻게든 하는 일이군요？」")
				CASE "一般"
					PRINTFORMW 「그렇게 이 오고 싶으면, 마음대로 하면？」
					PRINTFORMW 「 나는 여기서 감상시켜 주기 때문에」
				CASE "低い"
					PRINTFORMW 「좋아요. 좋아하는 대로 이 오세요. 다만…스스로, 군요？」
			ENDSELECT
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
			CLEARLINE 1
		;1/3の確率で表示
		ELSE
			PRINTFORMW 「이봐요, 언제라고도 이는 좋아？」
			PRINTFORMW 「소야가 스스로 위로해 이곳, 나에게 보이게 해 주세요」
;OriginalString : (COLOR_K14("사쿠야"), "「やだっ！ 言わないでよ、そんな事…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「싫닷！ 말하지 말아, 그런 일…」")
			PRINTFORMW 「후후, 사랑스러운 일 말하군요」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが기분좋게 해
	;=================================================
	IF 구상記入チェック(100) && IS_PREVCOMNAME("기분좋게 해")
		PRINTFORMDW 앨리스는 기분 좋게 해와 조르는 소야에, 자위를 하듯이 명했다…
		PRINTL
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「기분 좋아지고 싶으면, 스스로 어떻게든 하며」
				PRINTFORMW 「지금은 아무것도 할 생각 없어. 미안한데…」
			CASE 1
				PRINTFORMW 「…기분 좋게 해, 군요. 아무튼 좋은데」
				PRINTFORMW 「우선, 스스로 어떻게든 해 주세요. 후후」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;前COMが자위를 시작한다
	;=================================================
	IF 구상記入チェック(100) && IS_PREVCOMNAME("자위를 시작한다")
		PRINTFORMW 「귀부인이 이렇게 자위 정말 좋아했던이라니？ 소야」
		;フェードアウト
		CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
		CLEARLINE 1
		
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「んんっ、あっ")
		CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"), "　や、とまらなくなっちゃうぅっ！」")
		
		PRINTFORMW 「사양이 없는 사람…후후. 좋아요, 기분이 끝날 때까지 계속해 주세요」
		
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "「私はじっくり鑑賞させて貰う事にするわ…")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		RETURN 1
	ENDIF
	
	;=================================================
	;연속실행
	;記入チェック
	LOCAL:1 = 1
	;=================================================
	IF 구상記入チェック() && TFLAG:ACT == TFLAG:전ACT
		PRINTFORMDW 앨리스는 자위에 빠지는 소야에 한층 더 계속하듯이 명했다…
		;前COM派生処理
		SELECTCASE TRAINNAME:PREVCOM
			CASE "부끄러워한다"
				PRINTFORMW 「부끄러워도 결국은  그만둘 수 없구나？」
				PRINTFORMW 「뭐, 귀부인이 부끄러워하는 일자체, 조금 깜짝이지만」
				PRINTFORMW 「…라고, 조금 과언 까. 응？ 후후」
			CASE "소극적으로 한다"
				PRINTFORMW 「뭐야, 그다지 시 싶지 않은거야？」
;OriginalString : (COLOR_K14("사쿠야"), "「う、五月蝿いわね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「시, 시끄럽네요…」")
				PRINTFORMW 「안되어요, 소야…분명하게 평상시 대로에 해 주세요」
			CASE "적극적으로 한다", "태연하게 한다"
				;フェードアウト
				CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 2))
				CLEARLINE 1
;OriginalString : (COLOR_K14("사쿠야"), "「んっ！ やだこれ、気持ち良いっ！ んんっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏！ (이)나다 이것, 기분 좋닷！ 응읏！」")
				PRINTFORMW 「좋은 느껴라, 소야. 그 상태로 계속해♪」
			CASEELSE ;それ以外
				PRINTFORMW 「그대로 계속해도 좋아요, 소야」
				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「今更やめろって言っても、止められそうに無いみたいだしし、ね")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자も자위중
	;=================================================
	IF 구상記入チェック(100) && GETBIT(TEQUIP:자위중, 1)
		;元々はSELECTCASEでPALAM:쾌Ｃ派生を見てた箇所。TARGETにも가버려が実装당했다のでモディファイ
		IF TCVAR:TARGET:가버려 && !TEQUIP:눈가리개
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 1))
			CLEARLINE 1
			PRINTFORMDW 앨리스는%CLITORIS_DESCRIPTION(TARGET)%음핵에 스스로의 애액을 발라 손가락끝으로 격렬하게 훑어 내고 있다…
			CALL QUAKE(2)
			PRINTFORMW 「아″아″！ 개, 이것！ 굉장하다！ 굉장한 것！」
			PRINTFORMW 「, 소야…나, 도, 이제！ 아개, 아, 아！」
;OriginalString : (COLOR_K14("사쿠야"), "「くっ…そんなの見せられたら、私…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「쿳…그런 것 보여지면, 나…」")
		ELSEIF PALAM:TARGET:쾌Ｃ >= 5000
			;フェードアウト
			CALL FADE(20, COLOR_K14("white"), COLOR_K14("black"), KOJO_RAND_K14(("자위"), 1))
			CLEARLINE 1
			PRINTFORMW 「아, 나다…여기 만지작거리면(자) 조크는 해버린다！ 손가락, 멈추지 않아서…응″응！」
			PRINTFORMW 「이봐요, 소 밤에도 해…어차피, 이제 인내 할 수 없게 되어 오고 있지 지만♪」
;OriginalString : (COLOR_K14("사쿠야"), "「そ、そんな事っ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 일！」")
			PRINTFORMW 「솔직하지 않아？ (이)라면, 어째서 조금 전부터 모지모지 하고 있는 거나, 응 소야？ 후후후」
		ELSE
			PRINTFORML 「이봐요, 소야도 함께 해…」
			PRINTFORMW 「혼자서 소곤소곤 하는 것보다, 반드시 즐길 수 있다고 생각해요♪」
			PRINTL
			CALL PRINTCOLORL(COLOR_K14("사쿠야"), "「なっ！？」")
			PRINTFORMW 「후후, 내가 깨닫지 않다고 생각해？」
			PRINTL
			PRINTFORMW 「뭐,1개(살) 가르쳐 두어 주는데…그렇게 소리 내고 있으면, 싫어도 들려버리는 원이야？」
			PRINTFORMW 「그렇게 말하면, 누군가의 이름을 외치고 있었어？ 어와…」
;OriginalString : (COLOR_K14("사쿠야"), "「あぁもう、わかった！ わかったわよ！ やればいいんでしょう！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아벌써, 알았다！ 알았어요！ 하면 되겠지요！」")
			PRINTFORMW 「후후, 최초부터 솔직하게 하면 좋았을텐데 , 응 소야？ 킥킥」
		ENDIF
		RETURN 1
	ENDIF
	
	;징계・모드分岐は"징계해"を선택했다場合延々と呼ばれるので確率を絞る
	;以前は上層に置いてたけど、それは誤りな感があるので下層に持ってきた
	;内容的には只の開始の分岐だし
	;=================================================
	;징계・모드
	;=================================================
	IF 구상記入チェック(45) && TFLAG:징계・플래그
		SELECTCASE RANDACT(2, 0)
			CASE 0
				PRINTFORMW 「, 자위를 해 주세요」
				PRINTFORMW 「소야가 보기 흉하게 자신을 위로하는 곳(중), 차분히 봐 주어요♪」
			CASE 1
				PRINTFORMW 「징계 귀로 해 귀부인에게는 자위를 해 받아요, 소야…」
				PRINTFORMW 「이라고 말해도…징계 귀로 말하는 것보다, 포상과 같은 생각도 드는데♪」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;정욕/울굴/몽롱
	;=================================================
	IF 구상記入チェック(100) && GROUPMATCH(GET_CONDITION(MASTER), "정욕", "울굴", "몽롱")
		PRINTFORMW 「이봐요, 사양하지 말아줘. 스스로 시라고 좋아요？」
;OriginalString : (COLOR_K14("사쿠야"), "「な、なんの話、よ…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무, 무슨 이야기,…」")
		PRINTFORM 「후후, 시치미를 떼어도 타목. 스스로 만지작거리고 싶어서 감등 없는 버릇에
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;분노
	;=================================================
	IF 구상記入チェック(100) && M_COND("분노")
		PRINTFORMW 「기분 나쁜 것 같구나？ 소야. 그러면, 여기서 자위해 보이며」
;OriginalString : (COLOR_K14("사쿠야"), "「ねぇ엘리스、一つ聞いていいかしら…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「저기엘리스, 1개 (들)물어도 좋을까…」")
		PRINTFORMW 「뭐야？」
;OriginalString : (COLOR_K14("사쿠야"), "「『機嫌が悪い』と『자위』に一体何の繋がりがあるのよ！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「『기분이 나쁘다』와 『자위』에 도대체 무슨 연결이 있는거야！」")
	
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "「別に無いけど？ ただ面白そうだと思って、ね")
		CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		
		PRINTL
		
		;今なら違う記述でいける感
		CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「何が『")
		CALL PRINTCOLOR(COLOR_K14("엘리스"), "ね")
		CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
;OriginalString : (COLOR_K14("사쿠야"), "』よ。まったく…」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "』. 완전히…」")
		
		PRINTFORMW 「화내거나 기가 막히거나 소야는 바쁘네요？」
;OriginalString : (COLOR_K14("사쿠야"), "「誰のせいよ、誰の！」")
		CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「누구의 탓이야, 누구의！」")
		PRINTFORMW 「후후…거기에 붙어 나의 잘못을 인정해도 괜찮은데. 뭐, 그것은 그것으로서…」
		PRINTFORMW 「이제 시작해 받을까, 응 소야？」
		RETURN 1
	ENDIF
	
	;=================================================
