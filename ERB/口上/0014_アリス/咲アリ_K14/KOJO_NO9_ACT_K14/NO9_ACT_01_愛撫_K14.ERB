;========================================================================
;애무系 ACT10～19 & 27
;========================================================================
;・TFLAG:ACT(10=손으로 애무/11=가슴애무/12=입으로 애무/13=항문애무/14=항문펠라/15=키스/16=성기마찰한다/17=파이즈리한다/18=풋잡하기/19=손가락삽입한다/27=밀어 넘어트린다)
;・TFLAG:ACT분류 == 1に該当
;・ACT18, 발コキは이상系から派生する場合あり
;・汎用リアクションにおいてはMENUMATCH(TFLAG:ACT, "애무")で拾う事が可能
;・죠교자が죠교대상を애무する。この구상の時点で죠교자はすでに손を出している
;・미약を使用するケースがある
;-------------------------------------------------
;・戻り値(RESULT)について
;・システム側では구상側のRESULT(終端のRETURN 1など)によって구상が表示당했다のか否か、
;・また箇所毎の特有の挙動(미약等구상をスキップなど)を制御しています
;・@구상記入チェックの場合代入処理を省略できますが、RESULT値は구상側で決めうちする필요があります
;・特別な理由がない限り、テンプレとしてはRETURN 1を記述しておきます
;-------------------------------------------------
;関連関数…
;FIRSTACT
;처음ACT判定関数
;TFLAG:ACT（TFLAG:90）を自動で参照、現在実行されているACTごとに번호が独立して保存される
;-------------------------------------------------

;────────────────────────────────────
;NO9_ACT10, 손으로 애무(ACT10_0, 애무する/ACT10_1, 수음する/ACT10_2, 페니스밴드수음)
;────────────────────────────────────
;・Ｃ中心に애무する
;・죠교대상に페니스がある場合、及び페니스밴드장착時には派生コマンドになる
;・죠교대상が자위중の場合は解除される
;・죠교자の손に汚れがある場合불쾌SOURCEが変動する
;・죠교자기교、애무、손가락기술素質に応じて발기도上昇
;-------------------------------------------------
@KOJO_NO9_MS_ACT_K14_10
;-------------------------------------------------
;・TFLAG:ACT파생(0=애무/1=수음/2=페니스밴드수음)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;NO9_ACT10_0, 애무する
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && !TFLAG:ACT파생
	[SKIPSTART]
	;=================================================
	;처음
	;=================================================
	IF 구상記入チェック(0) && FIRSTACT(0)
	ENDIF
	
	;=================================================
	;조교중初回
	;=================================================
	IF 구상記入チェック(0) && ONCEACT(0)
		SELECTCASE POLICY("금일") ;금일の죠교방침派生処理
			CASE "통상"
				PRINTFORMW
			CASE "징계・"
				PRINTFORMW
			CASE "하드"
				PRINTFORMW
			CASE "노닥노닥"
				PRINTFORMW
			CASEELSE ;その他
				PRINTFORMW
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;밀어넘어짐상태
	;=================================================
	IF 구상記入チェック(100) && TEQUIP:밀어넘어지는중
		IF TFLAG:ACT == TFLAG:전ACT ;연속실행
			PRINTFORMW 「이대로 손으로 이나 키챠오 건더기등♪」
			PRINTFORMW 「이봐요, 좋을 것입니다？ 좀 더 기분 좋아져…」
		ELSE ;통상
			PRINTFORMW 「, 나의 손가락으로 즐겨 주세요」
			PRINTFORMW 「민감한 곳도 만져 주어요… 후후…」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상が가버려
	;=================================================
	IF 구상記入チェック(100) && TCVAR:MASTER:가버려
		;前から
		IF TCVAR:MASTER:위치전후 == 1
			SELECTCASE TCVAR:MASTER:애태움도 ;애태움도派生処理
				CASE IS > 1 ;애태움中
					PRINTFORMW 「간단하게 이라고 해도들은은 곤란해요？ 사쿠야」
					PRINTFORM 「 좀 더 나를 즐기게 해 주세요
					CALL PRINT_SYMBOL_K14
				CASE 1 ;애태움終了
					PRINTFORMW 「후후, 이렇게 부풀려 버려서 이제 한계일까？」
					CALL PRINTCOLOR(COLOR_K14("엘리스"), "「そろそろとどめを刺してあげるわ…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
				CASEELSE ;애태움無し
					PRINTFORMW 「이제 때일까？」
					CALL PRINTCOLOR(COLOR_K14("엘리스"), "「じゃ、少し격렬하게あげるわ…")
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		;後ろから
		ELSE
			SELECTCASE TCVAR:MASTER:애태움도 ;애태움도派生処理
				CASE IS > 1
					PRINTFORMW 「후후, 이제 이 나무 그런？ 얼굴이 안보여도 알아요」
					PRINTFORM 「 그렇지만 간단하게 이인가 다툴 생각은이야. 좀 더 초조하게 해 준다
					CALL PRINT_SYMBOL_K14
				CASE 1
					PRINTFORMW 「좋아요, 이 오세요 사쿠야. 나의 팔 중(안)에서 이나 키라고 주기 때문에…」
					PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 사쿠야를 절정으로 막상 되었다…
				CASEELSE
					PRINTFORMW 「후후, 이것으로는 도망가지 않아요？」
					PRINTFORMW 「이대로 이나 키라고 주기 때문에, 얌전하게 하고 있어…」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	;PALAMを噛ますべきと思う
	;=================================================
	;연속실행
	;=================================================
	IF 구상記入チェック(100) && TFLAG:ACT == TFLAG:전ACT
		;-------------------------------------------------
		;하반신服装が要押し退け
		;-------------------------------------------------
		IF 구상記入チェック(100) && PANTSCHECK(1, 1)
			PRINTFORMW 「응 사쿠야…속옷 위로부터에서도 아는 위 부풀어 올라 왔어요？」
			IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0 ;안면승마中
				PRINTFORMW 「어머나, 미안해요. 이 상태는 말할 수 없네요？ 후후」
			ELSE ;それ以外
;OriginalString : (COLOR_K14("사쿠야"), "「しっ、知らないわよ！ そんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요！ 그런 것…」")
				CALL PRINTCOLOR(COLOR_K14("엘리스"), "「ふふ、誤魔化しても駄目")
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　体は正直なんだから」")
			ENDIF
			RETURN 1
		ENDIF
		
		;---------------------
		;穿いてない
		;---------------------
		PRINTFORMW 「상당히 넌더리나고 넌더리나고 해 온 원이군요. 그렇게 기분 좋은 것일까, 응 사쿠야？」
		IF TRUECHECK(TEQUIP:안면승마, TEQUIP:안면승마항문) > 0 ;안면승마中
			PRINTFORMW 「이라고 해도, 이 상태는 대답은 할 수 없을까. 후후…」
		ELSE ;それ以外
;OriginalString : (COLOR_K14("사쿠야"), "「今そんな事聞かないで欲しいわね…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「지금 그런 일 (듣)묻지 않으면 좋네요…」")
			PRINTFORMW 「후후, 그렇구나. 생각이 잘 미치지 않아서 미안해요♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;전회Ｖ中出し
	;=================================================
	;IF 구상記入チェック(0) && TFLAG:전회질싸 == 1
	;ENDIF
	
	;=================================================
	;눈가리개장착中
	;=================================================
	IF 구상記入チェック(100) && TEQUIP:눈가리개
		PRINTFORMDL 앨리스는 시야를 빼앗긴 사쿠야의 %CLITORIS_DESCRIPTION(MASTER)%음핵을 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「호흡이 거칠네요？ 사쿠야. 눈 가림되어 흥분하고 있는 거나」
		PRINTFORMW 「후후, 몇시, 어디를 손대어지는지 모른다고 것도 좋은 것」
		RETURN 1
	ENDIF
	
	;=================================================
	;클리캡장착中
	;=================================================
	IF 구상記入チェック(100) && TEQUIP:클리캡
		PRINTFORMDL 앨리스는 구리 캡 넘어로사쿠야의 %CLITORIS_DESCRIPTION(MASTER)%클리토리스를 애무하기 시작했다…
		PRINTL
		PRINTFORMW 「이대로 만지작거리면(자), 귀부인 어떻게 되어버릴까 해들？ 응 사쿠야」
		PRINTFORMW 「후후후…」
		RETURN 1
	ENDIF
	
	;eraWIZ K12 ボクっ딸구상にインスパイア당했다箇所
	;まぁRevの場合は任意にコマンド선택できる訳じゃないんでこれが成立する確率はかなり低めかなｗ
	;=================================================
	;전ACTが스펭킹
	;=================================================
	IF 구상記入チェック(100) && PREVACTNAME() == "스펭킹"
		PRINTFORMDW 앨리스는 보라색의 타척의 자국을 소프트에, 그리고 열심히 어루만지고 돌렸다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼派生
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「んんっ、あっ")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「귀부인에게 있어서는 아픔도 쾌락인 것？ 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
;OriginalString : (COLOR_K14("사쿠야"), "「あぁっ！ やっ、いやぁっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗！ 얏, 싫어엇！」")
				PRINTFORMW 「아픈 것은 지금 가운데만…곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;전ACTが채찍
	;=================================================
	IF 구상記入チェック(100) && PREVACTNAME() == "채찍"
		PRINTFORMDW 채찍에 의해 사쿠야의 신체에 새겨진 상흔을, 앨리스는 섬세한 손가락 값어치 살그머니 위를 덧쓰고 있다…
		SELECTCASE ABL:MASTER:마조끼 ;마조끼派生
			CASE IS >= 4
				CALL QUAKE(1)
				CALL PRINTCOLOR(COLOR_K14("사쿠야"), "「んんっ、あっ")
				CALL PRINT_HEARTCBW_K14(COLOR_K14("red"), , , COLOR2_K14("SAKUYA"))
				PRINTFORMW 「귀부인에게 있어서는 아픔도 쾌락인 것？ 좋아요, 좀 더 상흔을 손대어 주기 때문에」
			CASEELSE
;OriginalString : (COLOR_K14("사쿠야"), "「あぁっ！ やっ、いやぁっ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아앗！ 얏, 싫어엇！」")
				PRINTFORMW 「아픈 것은 지금 가운데만…곧바로 좋아져요, 사쿠야…」
		ENDSELECT
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;仮設
	;=================================================
	;전ACTが바늘
	;=================================================
	IF 구상記入チェック(0) && PREVACTNAME() == "바늘"
	ENDIF
	
	;仮設
	;밧줄は解除の場合もあるからなんともなところ
	;=================================================
	;전ACTが밧줄
	;=================================================
	IF 구상記入チェック(0) && PREVACTNAME() == "밧줄"
	ENDIF
	[SKIPEND]
	
	;=================================================
	;하반신服装が要押し退け
	;=================================================
	IF 구상記入チェック(100) && PANTSCHECK(1, 1)
		IF PALAM:MASTER:윤활 >= 500 ;윤활十分
			PRINTFORMW 「 이제 이렇게 속옷을 더럽혀 버려서, 기다릴 수  없었던 것일까 해들？」
;OriginalString : (COLOR_K14("사쿠야"), "「違っ！ こ、これは…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「달랏！ 이, 이것은…」")
			PRINTFORMW 「후후, 부끄러워하지 않아 좋아요. 젖고 있는 (분)편이 다양하게 적당한 것, 이군요♪」
		ELSE ;윤활不발
			PRINTFORMW 「그다지 젖지 않네요？」
			PRINTFORMW 「좋아요, 곧바로 자신으로부터 속옷을 벗고 싶어질 정도로 자주(잘) 해 주기 때문에♪」
		ENDIF
		RETURN 1
	ENDIF
	
	;위치전후 == 2になる確立、及び손間の観点からこのような有様に
	;RETURNせずに下層に流すと状況によっては違和感が出るのでここで完結시킨다
	;=================================================
	;背後から
	;=================================================
	IF 구상記入チェック(100) && TCVAR:MASTER:위치전후 == 2
		SELECTCASE TENSION(TARGET, 8) ;죠교자만족度派生処理
			CASE "高い"
				PRINTFORMW 「이봐요, 움직이지 마. 뒤로부터 귀여워해 주기 때문에」
				PRINTFORMDW 앨리스는 사쿠야의 목덜미에 키스를 하면서, 애무하기 시작했다…
;OriginalString : (COLOR_K14("사쿠야"), "「んっ！ やだ、엘리스…こ、こんなの…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「읏！ 싫다,엘리스…이, 이런 건…」")
				PRINTFORMW 「 그렇지만 싫지 않겠지요？ 후후, 나에게는 알아요」
			CASE "一般"
				PRINTFORMW 「이봐요 어때？ 뒤로부터 만지작거려지는 것은 좋을 것입니다？」
;OriginalString : (COLOR_K14("사쿠야"), "「し、知らないわよ！」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「모, 몰라요！」")
				PRINTFORMW 「그렇게는 말하는데……반응이 조금 달라. 후후」
			CASE "低い"
				PRINTFORMW 「뒤로부터 꼭 껴안을 수 있으면 얌전해져버린다 것？」
;OriginalString : (COLOR_K14("사쿠야"), "「なんの事か、私にはさっぱりね…」")
				CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「무슨 일인가, 나에게는 형편없구나…」")
				PRINTFORM 「후후, 별로 좋아요. 다만, 좋은 소리로 울어 주기만 하면, 이군요
				CALL PRINT_SYMBOL_K14
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자상태변화派生処理
	;=================================================
	IF 구상記入チェック(100)
		;-------------------------------------------------
		;정욕/울굴/몽롱
		;-------------------------------------------------
		IF 구상記入チェック(100) && GROUPMATCH(GET_CONDITION(TARGET), "정욕", "울굴", "몽롱")
			SELECTCASE TENSION(TARGET, 8) ;죠교자만족度派生処理
				CASE "高い"
					IF PALAM:MASTER:윤활 >= 500 ;濡れてる
						PRINTFORMW 「만지작거렸으면 좋았으면, 솔직하게 말하면 좋은데는 나는 생각하는데. 응 사쿠야？」
;OriginalString : (COLOR_K14("사쿠야"), "「なっ！？ 何を言ってるのか全然判らないわ…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「낫！？ 무슨 말을 하고 있는지 전혀 알지 않아요…」")
						PRINTFORM 「후후…귀부인은 알지 않아도, 여기는 갖고 싶고 이제 기다릴 수  없는 것 같지만, 이군요…
						CALL PRINT_SYMBOL_K14
					ELSE ;濡れてない
						PRINTFORM 「, 색정광의 변태 메이드에게 포상을 주어요！ 마음껏 즐겨 주세요
						CALL PRINT_SYMBOL_K14
;OriginalString : (COLOR_K14("사쿠야"), "「その発言の内容は兎も角…なにその鬱陶しいハイテンションは…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그 발언의 내용은 차치하고…뭐 그 음울한 하이 텐션은…」")
						CALL PRINTCOLOR(COLOR_K14("엘리스"), "「別にいいじゃない。私が愉しんでるのは貴女にとっても悪い事じゃないはずよ、사쿠야…")
						CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					ENDIF
				CASE "一般"
					IF PALAM:MASTER:윤활 >= 500 ;濡れてる
						PRINTFORMW 「 이제 이렇게 적셔, 사쿠야. 정말 이야라시이의 까…」
;OriginalString : (COLOR_K14("사쿠야"), "「そういう事、一々言わないで欲しいわね…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그런 일, 하나 하나 말하지 않으면 좋네요…」")
						PRINTFORM 「후후, 방법 없지요. 진짜일이니까
						CALL PRINT_SYMBOL_K14
					ELSE ;濡れてない
						PRINTFORML 「이나다 사쿠야, 아직 젖지 않아요…」
;OriginalString : (COLOR_K14("사쿠야"), "「だから、そんな事言われても困るから…」")
						CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그러니까, 그런 일 말해져도 곤란하기 때문에…」")
