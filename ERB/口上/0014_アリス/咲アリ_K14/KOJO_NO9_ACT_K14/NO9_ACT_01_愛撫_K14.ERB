;━━━━━━━━━━━━━━━━━━━━━━━━
;・가슴の揉み合いを要求する。この時点での죠교대상の反応は未定
;・乳首系の条件から漏れた場合、죠교자の욕망+Ｂ감각３以上なら、1/2で가슴の揉み合いになる
;-------------------------------------------------
IF 구상記入チェック(100) && TFLAG:ACT파생 == 2
	;=================================================
	;상반신服装が要押し退け
	;=================================================
	IF 구상記入チェック(100) && BRACHECK(1, 1)
		IF BRACHECK(1) ;상반신服装が邪魔
			PRINTFORMW 「응, 브라 위로부터 비비어지는 것은 어떤 느낌 까？」
			PRINTFORMW 「사쿠야에는 조금 어딘지 부족할지도 모르는데」
;OriginalString : (COLOR_K14("사쿠야"), "「ば、馬鹿な事言わないでよ！」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「바, 바보같은 일 말하지 말아！」")
			PRINTFORMW 「그렇게？ 후후후…」
		ELSEIF UPPERJACKET(1) == 1 ;상반신겉옷が要押し退け
			PRINTFORMW 「완전하게 벗게 한 없다고 것도 좋은 것？」
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「凄くイケない事してるみたい…")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ELSEIF UPPERUNDER(1) == 1 ;상반신속옷が要押し退け
			PRINTFORMW 「%INNER_T(MASTER)%의 안에 손을 넣고 있으면, 못된 장난하고 있는 것 같아 흥분해요…」
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「というか、実際にしてるんだけど、ね…")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
		ENDIF
		PRINTL
		PRINTFORMW 「이봐요, 분명하게 나의 것도 만지작거려 주지 않으면 타목」
		PRINTFORMW 「함께 기분 좋아져, 사쿠야…」
		RETURN 1
	ENDIF
	
	;-------------------------
	;その他
	;-------------------------
	SELECTCASE GET_CONDITION(MASTER) ;죠교대상の상태派生処理
		CASE "정욕", "울굴"
			PRINTFORMW 「상당히 흥분해 버린다 같구나？ 사쿠야」
;OriginalString : (COLOR_K14("사쿠야"), "「そ、そんな事！ ない、わよ…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「그, 그런 일！ 없는, 원이야…」")
			CALL PRINTCOLOR(COLOR_K14("엘리스"), "「嘘ついても駄目…")
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　こんなに乳首立たせてるのに、説得力がないわよ。ふふっ」")
		CASEELSE ;それ以外
			PRINTFORMW 「역시 직접 만지작거려지는 편이 좋은 것일까？」
			PRINTFORMW 「귀부인, 조금 기쁜 듯한 얼굴 하고 있는 원이야. 사쿠야♪」
;OriginalString : (COLOR_K14("사쿠야"), "「知らないわよ、そんなの…」")
			CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「몰라요, 그런 것…」")
			PRINTFORMW 「후후, 부끄러워하는 일은 없어요」
	ENDSELECT
	;一行空ける
	PRINTL
	PRINTFORMW 「이봐요, 분명하게 나의 것도 만지작거려 주지 않으면 타목」
	PRINTFORMW 「함께 기분 좋아져, 사쿠야…」
	RETURN 1
ENDIF

