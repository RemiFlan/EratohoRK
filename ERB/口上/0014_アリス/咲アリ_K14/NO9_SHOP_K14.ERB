;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상が恥ずかしがって着替えられない FIRSTEVENT(50)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG >= 1000 && (ARG & 10) == 2
	SELECTCASE ARG
		;반라だって恥ずかしい
		CASE 1012
			IF CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도
				PRINTFORMW 「그렇게, 어쩔 수 없네요…」
			ELSE
				PRINTFORMW 「부끄러워했더니, 이제 와서 어쩔 도리가 없는데？」
				PRINTFORMW 「뭐, 이번은 놓쳐 주어요」
			ENDIF
			RETURN 1
		;普段から전라なんて恥ずかしい
		CASE 1022
			IF CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도
				PRINTFORMW 「그렇게, 유감이구나…」
			ELSE
				PRINTFORMW 「%CNAME(MASTER, TARGET)%에 아직 수치심이 남고 있다니…」
;OriginalString : 인8, "「ちょっと驚きね？」")
				CALL FADEIN(8, "「조금 놀라？」")
				WAIT
			ENDIF
			RETURN 1
		;パンツがないから恥ずかしいもん！
		;CASE 1032
		;	PRINTFORMW 「いいじゃない、どうせ減るものじゃあるまいし…」
		;	RETURN 1
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상が嫌がって着替えられない FIRSTEVENT(60)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG >= 1000 && (ARG & 10) == 3
	SELECTCASE ARG
		;반라は嫌
		CASE ARG == 1013
			PRINTFORMW 「그렇게 싫어…？」
			RETURN 1
		;普段から전라なんて嫌
		CASE 1023
			PRINTFORMW 「아무래도 안돼…？」
			RETURN 1
		;すーすーするのは嫌
		;CASE 1033
		;	PRINTFORMW 「%CNAME(MASTER, TARGET)%も頑固ね…」
		;	RETURN 1
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상を着替え시킨다のをやめた FIRSTEVENT(70)
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:MASTER%의 의상はすでに[　　]に設定されています
;------------------------------------------------
IF 구상記入チェック(100) && ARG >= 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		;통상のまま
		CASE 1000
			PRINTFORMW 「현재, 갈아입어 받을 필요는 없을까」
			RETURN 1
		;반라のまま
		CASE 1010
			PRINTFORMW 「그렇구나. 우선, 그대로의 모습으로 있어 받을까」
			RETURN 1
		;전라のまま
		CASE 1020
			PRINTFORMW 「벌거벗은 채로에서도 태연같고,%CNAME(MASTER, TARGET)%(은)는 그대로 좋아요」
			SELECTCASE CFLAG:MASTER:의존도
				CASE IS > 5000
					PRINTFORM 「오히려 알몸이 좋겠지요？ 이 변태…
					CALL PRINT_SYMBOL_K14
				CASE IS > 3000
					PRINTFORM 「잘 어울리고 있다고 생각하고. 그런데
					CALL PRINT_SYMBOL_K14
				CASEELSE
					PRINTFORMW 「하인으로서는, 그것이 적당한 모습이라고 생각하고. 응？」
			ENDSELECT
			RETURN 1
		;CASE 1030
		;	RETURN 0
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;────────────────────────────────────
;EV102, 죠교자変更
;────────────────────────────────────
;・죠교자を交代시켰다ときに表示
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_102(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=(0, 구상主が他のキャラに任せた、1, 구상主が죠교자되었다)
;交代する相손の名前は「A」で参照できる
;구상主はTARGETで参照
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;他の죠교자に交代
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 0
	;엘리스が주인님
	IF TALENT:TARGET:주인님
		;상사상애
		IF AFFECTION("상사상애")
			PRINTFORMW 「기분 전환은 (뜻)이유？」
			PRINTFORMW 「아무튼 좋아요. %RAND_SPLIT(@"정령들%CNAME(A, TARGET)%에 귀여워해 받는 것！ /조금 정도의 이기적임은 허락해 준다.")%그렇지만…」
			PRINTFORMW 「 나를 언제까지나 기다리게 하면 어떻게 되는지, 자주(잘) 생각해 두는 일이군요！！」
			PRINTFORMD 앨리스는 그렇게 말해 발하면
				SELECTCASE RANDEVENT(3, 0)
					CASE 0
						PRINTFORMDW %CALLNAME:MASTER%에 짚으로 만든 인형을 과시했다……
					CASE 1
						PRINTFORMDW %CALLNAME:MASTER%에 극태어널 바이브래이터를 과시했다……
					CASE 2
						PRINTFORMDW 그리모워르를 꺼내, 주저의 주문을 주창하고 있다……
				ENDSELECT
		;その他
		ELSE
			PRINTFORMW 「뭐야, 나는 불만이라고 말해？」
			PRINTFORMW 「아무튼 좋은데. 나도 조금 생각하지 않으면 안 되게！」
		ENDIF
	;普通の조수
	ELSE
		PRINTFORMW 「%RAND_SPLIT("흐음/아라, 그렇게")%. 별로 좋아요？」
		PRINTFORMW 「그러면, 후는%CNAME(A, TARGET)%에 맡겨…나는 한가로이 시켜 줄까」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さまが죠교자되었다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 1
	;엘리스が주인님
	IF TALENT:TARGET:주인님
		;상사상애
		IF AFFECTION("상사상애")
			SELECTCASE RANDEVENT(2, 1)
				CASE 0
					PRINTL
					PRINTFORMW 「뭐, 결국 이러한 것은 알았지만！」
					PRINTFORMDW 앨리스는 조금 등져 보인 뒤, %CALLNAME:MASTER%에 몸을 접대어 왔다… 
					PRINTFORMW 「 나에게 외로운 생각을 시킨 보상, 빈틈없이 받으니까요？ 우후후…」
				CASE 1
					PRINTL
					PRINTFORMW 「어머나, 내가 그리워졌는지 해들？」
					PRINTFORMW 「이봐요, 왜？ 이제 , 솔직하게 말하세요！」
					PRINTFORMW 「제대로 말할 수 있었다면, 이번 일은 없었던 것으로 해 주어도 괜찮아요？」
			ENDSELECT
		;その他
		ELSE
			PRINTL
			PRINTFORMW 「후후… 떨어져 보고 아는 「앨리스님」의 좋은 점이라는 곳일까？」
			PRINTFORMW 「계(오)세요. 오늘은 충분히 귀여워해 준다」
			PRINTFORMW 「다양한 의미로, 군요♪」
		ENDIF
	;普通の조수
	ELSE
		PRINTL
		PRINTFORMW 「조금%CNAME(A, TARGET)%. 나에게 귀찮음마다를 꽉 누를 생각？」
		PRINTFORMW 「아무튼, 좋아요. 어차피라면, 정령들 즐기게 해 주기 때문에」
		PRINTFORMW 「부수어 버려서도 모르지만, 이군요♪」
	ENDIF
	RETURN 1
ENDIF

;────────────────────────────────────
;EV110, 마력使用
;────────────────────────────────────
;・마력を消費して何らかの特典を購入했다ときに表示
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_110(ARG)
;-------------------------------------------------
;引数内訳…
