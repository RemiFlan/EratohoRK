;========================================================================
;ショップ이벤트구상
;========================================================================

;────────────────────────────────────
;EV101, 의상変更
;────────────────────────────────────
;・ショップから의상変更を行った場合に表示
;-------------------------------------------------
@KOJO_NO9_MS_EVENT_K14_101(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=
;　一の位…（0, もう着てる、1, 着替えた、2, 恥ずかしくて着られない、3, 嫌がって着ない、9, 持ってない）
;　十の位…（0, 통상、10, 반라、20, 전라、30, 속옷無し）
;　百の位…코스프레のために保留
;　千の位…（0, 죠교자, 1000, 죠교대상）
;　例（죠교대상が恥ずかしがって전라にできなかった＝1022）
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が着替えた FIRSTEVENT(0)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상되었다
		CASE 1
			PRINTFORMW 「이것이１번사인것 같을까､그런데…？」
			RETURN 1
		;반라되었다
		CASE 11
			PRINTFORMW 「%CNAME(MASTER, TARGET)%, 이런 모습이 취미인가 해들…？」
			RETURN 1
		;전라되었다
		CASE 21
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORML 「집안이라고는 해도……」
				PRINTFORMW 「임산부가 전라로 우왕좌왕하는 것은, 조금 제발은 생각도 들어요」
			ELSE
				PRINTFORMW 「…변태」
			ENDIF
			RETURN 1
		;……すーすーする						現在のR#ぽでは実装されていない。将来的には復活する可能性は割りとある
		;CASE 31
		;	PRINTFORMW 「私に스커트のたくし上げをして欲しいのかしら…？」
		;	RETURN 1
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が恥ずかしがって着替えられない FIRSTEVENT(10)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 3
	SELECTCASE ARG
		;반라だって恥ずかしい
		CASE 12
			PRINTFORMW 「%RAND_SPLIT(@"%CNAME(MASTER, TARGET)%…, 바보같아？/싫어요")%」
			RETURN 1
		;普段から전라はちょっと……
		CASE 22
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORMW 「임신중의 나에게, 평상시부터알몸으로 있어 줘는. 그런…」
				PRINTFORMW 「응%CNAME(MASTER, TARGET)%…, 한번 죽어 본다？」
			;この条件を成立시킨다のは難しい
			ELSEIF CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도
				PRINTFORMW 「 나 그런 변태는, 없다…」
			ELSE
				PRINTFORMW 「굉장히 바보 같아요. 거절이야」
			ENDIF
			RETURN 1
		;パンツが無いから恥ずかしいもん！		現在のR#ぽでは実装されていない
		;CASE 32
		;	PRINTFORMW 「却下よ」
		;	RETURN 1
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が嫌がって着替えられない FIRSTEVENT(20)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 2
	SELECTCASE ARG
		;반라は嫌
		CASE 13
			PRINTFORMW 「%RAND_SPLIT("시시하네요. 거절이야/싫엇！")%」
			RETURN 1
		;普段から전라なんて嫌
		CASE 23
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORML 「은？ %CNAME(MASTER, TARGET)%…, 장난쳐？」
				PRINTFORMW 「 나, 임신해！？ 배안에 있는 것은%CNAME(MASTER, TARGET)%의 아이야！？」
				PRINTFORMW 「너무 바보같은 일 말하고 있으면, 때려 누여 주기 때문에…」
			ELSEIF CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도
				PRINTFORMW 「집에 누군가 왔을 때 어떻게 해…」
				PRINTFORMW 「절대로 혐！」
			ELSE
				PRINTFORMW 「%RAND_SPLIT("바보같은 일 말하지 않고, 빨리 죽어 준다？/싫어요. 있을 수 없어요")%」
			ENDIF
			RETURN 1
		;すーすーするのは嫌						現在のR#ぽでは実装されていない
		;CASE 33
		;	PRINTFORMW 「嫌よ、そんな馬鹿っぽい格好…」
		;	RETURN 1
		;その他
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교자が着替えるのをやめた	FIRSTEVENT(30)
;━━━━━━━━━━━━━━━━━━━━━━━━
;%CALLNAME:TARGET%의 의상はすでに[　　]に設定されています
;------------------------------------------------
IF 구상記入チェック(100) && ARG < 1000 && (ARG % 10) == 0
	SELECTCASE ARG
		;통상のまま
		CASE 0
			PRINTFORMW 「별로%RAND_SPLIT("갈아입는다필요없네요/이대로 좋네요")%」
			RETURN 1
		;반라のまま
		CASE 10
			PRINTFORMW 「뭐, 이대로 좋을까…」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%도 그 쪽이 기쁘겠지요？ 이 변태…」
			RETURN 1
		;전라のまま
		CASE 20
			IF TALENT:TARGET:임신 && TALENT:TARGET:주인님
				PRINTFORMW 「정직, 배의 아이에게 좋지 않는 생각이 들어요. 이것…」
			ELSE
				PRINTFORMW 「이제 와서%CNAME(MASTER, TARGET)%에 대해서 숨기는 것은 없고」
				PRINTFORMW 「별로 벌거벗은 채로라도 상관없어요. 다만…」
				PRINTFORM 「냉정하게 생각하면, 굉장히 바보 같은데. 그런데
				CALL PRINT_SYMBOL_K14
			ENDIF
			RETURN 1
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상を着替え시켰다 FIRSTEVENT(40)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG >= 1000 && (ARG % 10) == 1
	SELECTCASE ARG
		;통상되었다
		CASE 1001
			PRINTFORMW 「그것이１번%CNAME(MASTER, TARGET)%답네요」
			RETURN 1
		;반라되었다
		CASE 1011
			PRINTFORMW 「이런 것도 나쁘지 않은, 일지도…」
			RETURN 1
		;전라되었다
		CASE 1021
			PRINTFORM 「후후…, 이 변태
			CALL PRINT_SYMBOL_K14
			RETURN 1
		;……すーすーする
		;CASE 1031
		;	PRINTFORMW 「これはコレで有り、かも…」
		;	RETURN 1
		;その他
