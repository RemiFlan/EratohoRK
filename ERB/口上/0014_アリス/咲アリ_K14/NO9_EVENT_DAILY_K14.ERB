;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_0 股間まさぐり
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상記入チェック(0) && ARG == 0
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_1 おっぱいまさぐり
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 1
	PRINTFORMW 「응 소야…」
;OriginalString : (COLOR_K14("사쿠야"), "「はいはい、忙しいから後にして」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「네네, 바쁘기 때문에 뒤로 해」")
	PRINTFORMW 「상관해 주지 않으면, 못된 장난해요？」
;OriginalString : (COLOR_K14("사쿠야"), "「はいはい、どうぞご자유に」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「네네, 아무쪼록자유에」")
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_2 無視했다ら諦めて出て行った
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 2
	IF AFFECTION("상사상애")
		PRINTFORMW 「응, 안돼？ 지금은 시 싶지 않은거야…？」
		PRINTFORMW 「……이제 , 어쩔 수 없네요. 방해 해 나빴어요, 그대로 계속해 주세요」
	ELSE
		PRINTFORMW 「어머나, 무정하네요…」
		PRINTFORMW 「방해였던 것 같구나. 그러면, 그대로 계속해 주세요」
	ENDIF
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_3 身をゆだねて股間でイかされ
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && ARG == 3
	PRINTW
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_4 身をゆだねておっぱいで
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 4
;OriginalString : (COLOR_K14("사쿠야"), "「ちょ、ちょっと엘리스！ どこ触ってるのよ！」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「조, 조금엘리스！ 어디 손대고 있는거야！」")
	PRINTFORMW 「소야가 좋다고 말했어요？」
	PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「いや、確かに、言いま했다けど…」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아니, 확실히, 말지금했다하지만…」")
	PRINTFORMW 「이지요？ 이봐요, 얌전하게 해 주세요」
	PRINTL
;OriginalString : (COLOR_K14("사쿠야"), "「ま、まだ、するの…？」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아, 아직, 해…？」")
	PRINTFORM 「물론
	CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
	PRINTFORMW  소야를 이인가 다툴 때까지, 그만두지 않아요？ 후후」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;スキンシップ0_5 ぱんつお着替えさせてくれま했다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && ARG == 5
	PRINTFORMW 「대강, 이렇게 속옷을 더럽혀 버려서…」
;OriginalString : (COLOR_K14("사쿠야"), "「う…そ、それは…」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우…그, 그것은…」")
	PRINTL
	PRINTFORMW 「이야이야 말해도, 결국 좋아져 버렸어？ 후후」
;OriginalString : (COLOR_K14("사쿠야"), "「う、ウルサイわねっ！ 仕方無いでしょう！」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「우, 시끄럽네요！ 방법 없을 것입니다！」")
	PRINTL
	PRINTFORMW 「그래요？ 방법 없어요？ 후후후」
;OriginalString : (COLOR_K14("사쿠야"), "「あぁ…なんか凄く悔しいわ…」")
	CALL PRINTCOLORW(COLOR_K14("사쿠야"), "「아아…뭔가 굉장히 분해요…」")
	PRINTFORM 「아무튼 아무튼
	CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
	PRINTFORMW  갈아입음을 가져와 주기 때문에, 기분이 나쁜 것을 바꾸는 것이라고♪」
	RETURN 1
ENDIF

