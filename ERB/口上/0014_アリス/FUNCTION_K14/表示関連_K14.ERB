;AUTOHEARTCB2	文中での使用を想定。ハート、若しくは八分音符を表示する
;-------------------------------------------------
@PRINT_SYMBOL_K14
#LOCALSIZE 1
IF AFFECTION("상사상애")
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			IF RAND:3                   ;1/3でハートx2
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ELSE
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), 2, , COLOR2_K14("ALICE"))
			ENDIF
		CASE "一般"
			SELECTCASE RAND:3
				CASE 0
					CALL PRINT_8THNOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
				CASE 1
					CALL PRINT_W8NOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
				CASEELSE
					CALL PRINT_W16NOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
			ENDSELECT
		CASE "低い"
			PRINTW 」
	ENDSELECT
ELSEIF AFFECTION("익애")
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			IF RAND:3
				SELECTCASE RAND:3
					CASE 0
						CALL PRINT_8THNOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
					CASE 1
						CALL PRINT_W8NOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
					CASEELSE
						CALL PRINT_W16NOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
				ENDSELECT
			ELSE
				CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			ENDIF
		CASE "一般"
			CALL PRINT_8THNOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
		CASE "低い"
			PRINTW 」
	ENDSELECT
ELSE
	SELECTCASE TENSION(TARGET, 8)
		CASE "高い"
			CALL PRINT_8THNOTECW_K14(COLOR2_K14("white"), , , COLOR2_K14("ALICE"))
		CASEELSE
			PRINTW 」
	ENDSELECT
ENDIF

