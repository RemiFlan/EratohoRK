@KOJO_POSE_K14(ARG, ARG:1, ARG:2)
;ARG = 자세
;ARG:1 = 위치전후
;ARG:2 = 위치상하
;-------------------------------------------------
[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(자유)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && ARG:2 == 0
	;=================================================
	;자세변화あり
	;=================================================
	IF 구상記入チェック(0) && ARG >= 0
		;=================================================
		;자유
		;=================================================
		IF 구상記入チェック(0) && ARG == 0
		ENDIF
		
		;=================================================
		;누움
		;=================================================
		IF 구상記入チェック(0) && ARG == 1
		ENDIF
		
		;=================================================
		;座り
		;=================================================
		IF 구상記入チェック(0) && ARG == 2
		ENDIF
	
		;=================================================
		;네발로 엎드림
		;=================================================
		IF 구상記入チェック(0) && ARG == 3
		ENDIF
		
		;=================================================
		;기립
		;=================================================
		IF 구상記入チェック(0) && ARG == 4
		ENDIF
		
		;=================================================
		;무릎으로 서있다
		;=================================================
		IF 구상記入チェック(0) && ARG == 10
		ENDIF
	ENDIF
	
	;=================================================
	;자세변화なし
	;=================================================
	IF 구상記入チェック(0) && !ARG
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(上に乗った)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && ARG:2 == 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(上に乗せた)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && ARG:2 == 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;위치상하(上下변화なし)
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && !ARG:2
	;=================================================
	;누움
	;=================================================
	IF 구상記入チェック(0) && ARG == 1
;OriginalString : 
		PRINTFORMW
		IF ARG:1 >= 0
			SELECTCASE ARG:1
				CASE 0
				CASE 1
				CASE 2
			ENDSELECT
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	
	;=================================================
	;座り
	;=================================================
	IF 구상記入チェック(0) && ARG == 2
	ENDIF
	
	;=================================================
	;네발로 엎드림
	;=================================================
	IF 구상記入チェック(0) && ARG == 3
	ENDIF
	
	;=================================================
	;기립
	;=================================================
	IF 구상記入チェック(0) && ARG == 4
	ENDIF
	
	;=================================================
	;무릎으로 서있다
	;=================================================
	IF 구상記入チェック(0) && ARG == 10
	ENDIF
	
	;=================================================
	;자유
	;=================================================
	IF 구상記入チェック(0) && ARG == 0
	ENDIF
ENDIF
[SKIPEND]
