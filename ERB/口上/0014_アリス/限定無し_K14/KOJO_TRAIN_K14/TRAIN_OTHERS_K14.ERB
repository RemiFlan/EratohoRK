;────────────────────────────────────
;EV21, 大만족구상
;────────────────────────────────────
;・죠교자の대만족보너스を취득했다場合に表示
;-------------------------------------------------
@KOJO_EVENT_K14_21(ARG)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま大만족
;━━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100)
	;=================================================
	;상냥하게 응답한다
	;=================================================
	IF 구상記入チェック(100) && IS_NOWCOMNAME("상냥하게 응답한다")
		PRINTFORM 「후후, 애교가 있는 아이는 정말 좋아해요
		CALL PRINT_SYMBOL_K14
		SIF GET_COM_RANK() == "無理を했다행동"
			PRINTFORMW 「조금 비결과인것 같은 생각도 들지만, 이군요」
		PRINTFORMDW 앨리스는\@ TFLAG:REACT부하 > 2 ? 만면의 # \@웃음을 띠어%CALLNAME:MASTER%(을)를 응시하고 있다
		RETURN 1
	ENDIF
	
	;=================================================
	;쾌감을 받아들인다/기분 좋다!
	;=================================================
	IF 구상記入チェック(100) && IS_NOWCOMNAME("쾌감을 받아들인다/기분 좋다!")
		PRINTFORM 「솔직하고 사랑스럽네요
		CALL PRINT_SYMBOL_K14
		PRINTFORMDW 앨리스는 만면이 웃음을 띠어%CALLNAME:MASTER%(을)를 응시하고 있다
		RETURN 1
	ENDIF
	
	;=================================================
	;안에 내!
	;=================================================
	IF 구상記入チェック(100) && IS_NOWCOMNAME("안에 내!")
		PRINTFORMW 「후후, 질내사정이라니……」
		PRINTFORM 「은 사랑스러운 것일까…
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF
	
	[SKIPSTART]
	;=================================================
	;비명지른다
	;=================================================
	IF 구상記入チェック(0) && IS_NOWCOMNAME("비명지른다")
		IF AFFECTION("상사상애")
			PRINTFORMW 「은 단 소리로, 울까……」
			PRINTFORMW 「응, 좀 더……, 좀 더 들려주며」
		ELSE
			PRINTFORMW 「좋은 소리로 울어요……」
			PRINTFORMW 「…견딜 수 없어요. 그 한심한 얼굴도 멋져요？」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「フフ…」")
			CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「후후…」")
			CLEARLINE 1
		ENDIF
		RETURN 1
	ENDIF
	[SKIPEND]
	
	;=================================================
	;애정을 담아 한다
	;=================================================
	IF 구상記入チェック(100) && IS_NOWCOMNAME("애정을 담아 한다")
		IF AFFECTION("상사상애")
			PRINTFORMW 「%CNAME(MASTER, TARGET)%의 마음이 전해져 유곽…」
			PRINTFORMDL 앨리스는 녹고 자른 표정을 띄워 몸을 접대어 왔다…
			PRINTL
			PRINTFORM 「좋아해요…, 너무 좋아…\@ CFLAG:TARGET:의존도 > CFLAG:MASTER:의존도 ? 읏！ # \@
			CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
			PRINTFORMDW 앨리스는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하고 있다…
		ELSE
			PRINTFORMW 「이렇게 되어…, 나로, 나같은 것에(이)…」
			PRINTFORMDW 앨리스는 녹고 자른 표정을 띄워 몸을 접대어 왔다…
			PRINTFORM 「후후, 나쁘지 않아요. 이런 것…
			CALL PRINT_SYMBOL_K14
		ENDIF
		RETURN 1
	ENDIF
	
	;SELECTCOM == 15は上層で処理
	;=================================================
	;REACT분류が적극적으로 한다(적극적으로 한다、이루는대로等)
	;=================================================
	IF 구상記入チェック(100) && IS_COMGRONAME("적극적으로 한다/受け入れる")
		PRINTFORMW 「좋아요……, 매우 좋은 느낌…」
		PRINTFORMDW 앨리스는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하고 있다…
		RETURN 1
	ENDIF
	
	;---------------------------------------------
	;その他を処理
	;---------------------------------------------
	PRINTFORMW 「후후, 좋은 느낌이군요」
	PRINTFORM 「%CNAME(MASTER, TARGET)%, 나쁘지 않아요
	CALL PRINT_SYMBOL_K14
	RETURN 1
ENDIF

;────────────────────────────────────
;EV22, 상태변화구상
;────────────────────────────────────
;・상태이상が付いたときに表示
;・TCVAR:상태변화が죠교자、TCVAR:MASTER:상태변화が죠교대상
;・상태변화が起こると3ターン지속する
;-------------------------------------------------
;・TCVAR:상태변화지속を一律3ターンとか()
;-------------------------------------------------
@KOJO_EVENT_K14_22(ARG)
;-------------------------------------------------

;*************************************************
;EV22_0, 죠교자상태변화
;*************************************************
;T_COND TARGETが指定했다상태なら真을반환
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[피폐]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("피폐") && !TFLAG:일회휴게
	;상태변화지속ターンを2に
	TCVAR:상태변화지속 = 2
	;2/3의확률로표시無し RESULTに0을반환
	IF RAND:3
		RETURN 0
	;1/3의확률로표시
	ELSE
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「지쳤지만…기분 좋은 피로라는 느낌…♪」
			CASE "一般"
				PRINTFORMW 「조금 지친 원이군요…」
			CASE "低い"
				PRINTFORMW 「……지친 원이군요……」
		ENDSELECT
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[쇠약]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("쇠약") && !TFLAG:일회휴게
	TCVAR:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「후~……후~……」
		PRINTFORMDW 앨리스는 어깨로 숨을 쉬면서, 지치고 자른 표정을 보였다
		PRINTFORMW 「조금……야지나 버렸다 원이군요…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[무기력]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("무기력")
	TCVAR:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORMW 「자주(잘) 알지 않는데…갑자기 모두 싫어졌어요…」
			CASE "一般"
				PRINTFORMW 「무슨…아무것도 할 마음이 생기지 않네요…」
			CASE "低い"
				PRINTFORMW 「무슨 흥이 깨지구나……」
		ENDSELECT
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[몽롱]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("몽롱")
	TCVAR:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「응…！ 머리가 어질 어질해요…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[정욕]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("정욕")
	;정욕は3ターンのままで
	;TCVAR:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORML 「구……！ 이제 타목…, 참을 수 없다……！」
				PRINTFORML 「철저하게 귀여워해 주기 때문에 각오 해…」
			CASE "一般"
				PRINTFORML 「안돼, 이런 건…억제하지 않으면…」
				PRINTFORML 「 그렇지만,%CNAME(MASTER, TARGET)%(을)를 엄청으로 하고 싶어서, 나…」
			CASE "低い"
				PRINTFORML 「 나…스스로도 무서울 정도  흥분하고 있다…」
		ENDSELECT
		PRINTFORMDW %CALLNAME:MASTER%(을)를 응시하는 앨리스의 눈동자는, 요염하게 물기를 띠고 있다…
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[분노]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("분노")
	IF AFFECTION("상사상애")
		TCVAR:상태변화지속 = 1
	ELSEIF AFFECTION("익애")
		IF RAND:3
			TCVAR:상태변화지속 = 2
		ELSE
			TCVAR:상태변화지속 = 1
		ENDIF
	ELSEIF AFFECTION("권애")
		IF RAND:3
			TCVAR:상태변화지속 = 3
		ELSE
			TCVAR:상태변화지속 = 2
		ENDIF
	ELSE
		TCVAR:상태변화지속 = 3
	ENDIF
	IF RAND:3
		RETURN 0
	ELSE
		;엘리스さま만족度派生処理
		SELECTCASE TENSION(TARGET, 8)
			CASE "高い"
				PRINTFORML 「나쁘지 않은 기분이었지만…완전히 엉망으로 해 준 원이군요？」
				PRINTFORMW 「그 대상은 비싸게 들기 때문에, 각오 해…」
			CASE "一般"
				PRINTFORML 「부탁이니까, 그다지 화나게 하지 말고 주세요」
				PRINTFORMW 「어디까지 자신을 누를 수 있는지, 그다지 자신이 없어요…」
			CASE "低い"
				PRINTFORMW 「 나를 화나게 해도 좋은 일은 없어요…」
		ENDSELECT
		RETURN 2
	ENDIF
ENDIF

;100%
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[싫증]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && T_COND("싫증")
	;죠교の경과시간
	SELECTCASE TFLAG:경과시간
		;長い
		CASE IS >= 60
			PRINTFORMW 「유석에, 흥이 깨어 온 원이군요……」
			TCVAR:상태변화지속 = 3
		;普通
		CASE IS >= 30
			PRINTFORMW 「하나 더, 흥이 타지 않아요……」
			TCVAR:상태변화지속 = 2
		;短い
		CASEELSE
			PRINTFORMW 「무슨, 이마이치, 군요……」
			TCVAR:상태변화지속 = 1
	ENDSELECT
	RETURN 2
ENDIF

[SKIPSTART]
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さま[울굴]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(0) && T_COND("울굴")
	TCVAR:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		RETURN 2
	ENDIF
ENDIF
;TALENT:광기がある場合にのみ1/5の確率で울굴化
[SKIPEND]

;*************************************************
;EV22_1, 죠교대상상태변화
;*************************************************
;M_COND MASTERが指定했다상태なら真을반환
;------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[피폐]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && M_COND("피폐") && !TFLAG:일회휴게
	TCVAR:MASTER:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「조금 지치고 같구나？」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[쇠약]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && M_COND("쇠약") && !TFLAG:일회휴게
	TCVAR:MASTER:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「…심한 모양이군요. 너무 조금 했는지 해들…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[무기력]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && M_COND("무기력")
	TCVAR:MASTER:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「…마음에 들지 않네요. 그 기분이 빠진 태도…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[몽롱]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && M_COND("몽롱")
	TCVAR:MASTER:상태변화지속 = 2
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORML 「발 밑이 휘청거리고 있는 원. 도대체 어떻게 해 버렸어 까, 응？」
		PRINTFORMW 「후후후…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[정욕]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(33) && M_COND("정욕")
	;눈가리개장착中
	IF TEQUIP:눈가리개
		PRINTFORMW 「은？ 뭐야. 눈 가림되어 콧김 난폭하게 해 버려서」
		PRINTFORMW 「%CNAME(MASTER, TARGET)%, 흥분해 버린다 (뜻)이유？」
	;その他
	ELSE
		PRINTFORM 「조금 건방지구나？ 이
		CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "私", 1)
		PRINTFORMW (을)를 그런 눈으로 응시하다니」
	ENDIF
	SIF AFFECTION("상사상애")
		PRINTFORMW 「 그렇지만 그렇구나…, 기분은 나쁘지 않아요. 후후」
	RETURN 2
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;죠교대상[분노]化
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && M_COND("분노")
	IF AFFECTION("상사상애")
		TCVAR:MASTER:상태변화지속 = 1
	ELSEIF AFFECTION("죠교完了")
		IF RAND:3
			TCVAR:MASTER:상태변화지속 = 2
		ELSE
			TCVAR:MASTER:상태변화지속 = 1
		ENDIF
	ELSEIF AFFECTION("죠교済み")
		IF RAND:3
			TCVAR:MASTER:상태변화지속 = 3
		ELSE
			TCVAR:MASTER:상태변화지속 = 2
		ENDIF
	ELSE
		TCVAR:MASTER:상태변화지속 = 3
	ENDIF
	IF RAND:3
		RETURN 0
	ELSE
		PRINTFORMW 「어머나, 기분 나쁜 것 같구나？」
		PRINTFORMW 「다만, 너무 현명한 태도라고는 할 수 없어요. 그러한 것…」
		RETURN 2
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
