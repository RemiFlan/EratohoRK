;-------------------------------------------------
@KOJO_ACT_K14_1
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT1, 初回
;━━━━━━━━━━━━━━━━━━━━━━━━
;IF 구상記入チェック(0) && FIRSTACT(0)
;ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;その죠교の最初のアクション
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:경과시간 == 1 && !GET_KFLAG_K14("상사상애")
	PRINTFORMW 「응…, 조금 들려주었으면 좋지만」
	PRINTFORMW 「%CNAME(MASTER, TARGET)%, 나의 일 어떻게 생각하고 있는 거나？」
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;상사상애かつ이벤트で플래그が立っている
;━━━━━━━━━━━━━━━━━━━━━━━━
;엘리스さんはこういうのが似合いすぎて困る
;ここは状況描写を捏造してもいいかも試練
;-------------------------------------------------
IF 구상記入チェック(100) && GET_KFLAG_K14("상사상애")
	LOCALS = \@ TENSION(TARGET, 8) == "高い" ? っ！ # \@
	;kawaii
	SELECTCASE RANDACT(4, 0)
		CASE 0
			PRINTFORMW 「이제 와서 대답은 (들)물을 것도 없지만, 일단 (들)물어 두어요」
			PRINTFORMW 「%CNAME(MASTER, TARGET)%…, 나의 일 어떻게 생각하고 있다？」
		CASE 1
			PRINTFORMW 「다 안 일에서도, 가끔 말로 하는 것은 중요해」
			PRINTFORMW 「이봐요, (들)물어 주기 때문에 빨리 좋다 차이야%LOCALS%」
		CASE 2
			PRINTFORMW 「무엇으로 지금 쯤 그런 일이라든지, 시시한 것을 말%RAND_SPLIT("우노는 그만두군요/도 참 때려요")%…」
			PRINTFORMW 「이런 것은, 한 번 말하면 끝난다 라는 이야기는 아닌거예요. 안다？」
		CASE 3
			PRINTFORMW 「이봐요！ 멍청히 하지 않고, 삿사라고 대답하세요」
			PRINTFORMW 「어차피…, 대답은 다 알고 있지만%LOCALS%」
	ENDSELECT
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;기타・조건없음
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100)
	PRINTFORMW 「%CNAME(MASTER, TARGET)%(은)는 나의 일 어떻게 생각하고 있는 거나？」
	PRINTFORMW 「응, 들려주어 주세요…」
