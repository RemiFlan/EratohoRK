;========================================================================
;회화系 REACT0～9
;========================================================================
;・1231版よりTFLAG:REACT부하が導入され、선택했다COMが自然な행동かどうかが参照できるように
;・0127版以降のリアクション後の地の文は、基本的にREACT부하で分岐するようになっています
;・TFLAG:REACT부하(0=自然な행동/1=無理のない행동/2=無理を했다행동)
;-------------------------------------------------
;　회화系はあまり汎用処理には向かないので、汎用リアクション(COM)に流さず個別リアクション(REACT)
;　で完結させてしまうのが望ましいと考えます
;-------------------------------------------------

;=================================================
;関連変数/関数等…
;TFLAG:ACT파생 当該ACTでのパターン변화
;
;TFLAG:REACT분류 (0=なし/1=소극적으로 한다/2=적극적으로 한다/3=용서를 빌다/4=날뛴다/5=거부/6=逃げる)
;IS_COMGRONAME（"なし/소극적으로 한다/적극적으로 한다/용서를 빌다/날뛴다/거부/逃げる"）
;
;TFLAG:REACT부하（0=自然な행동/1=無理のない행동/無理を했다행동）
;COMCOR_POSI（消極的反応の内容が、実は적극적으로했다いのか、そうでないのかを判別する）
;
;FIRSTREACT
;처음ACT判定関数
;TFLAG:ACT（TFLAG:90）を自動で参照、現在実行されているREACTごとに번호が独立して保存される
;
;ONCEREACT
;조교중未読判定関数　죠교前に初期化される
;その他の仕様はほぼFIRSTREACTと同様
;
;RANDREACT
;全ての번호を見るまで同じものは返さない。全部見たらリセット
;이벤트번호はREACT毎に관리される（FIRST系、ONCE系と同様の仕様）
;=================================================

;────────────────────────────────────
;REACT0, 지금 기분을 묻다・個別リアクション
;────────────────────────────────────
;・ACT파생への代入は죠교대상のリアクションに応じて@ACTION_APPLY2_0にて行われる
;-------------------------------------------------
@KOJO_REACT_K14_0
;-------------------------------------------------
;・TFLAG:ACT파생(0=歯切れ悪く答えた/1=勇気を出して強く当たった/2=皮肉/3=はっきりと今の気持ちを告げた
;・4=まあまあ悪くはない/5=気持ち良いこと했다い)
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,0 歯切れ悪く答えた
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류が소극적으로 한다で、リアクションが빈정거린다以外の場合
IF 구상記入チェック(100) && TFLAG:ACT파생 == 0
	SELECTCASE GET_COM_RANK()
		CASE "自然な행동"
			PRINTFORMW 「어차피 대답한다면, 좀 더 분명하게로서 갖고 싶네요」
			PRINTFORMW 「무슨 말을 하고 있는지, 자주(잘) 들리지 않아요」
		CASE "無理のない행동"
			PRINTFORMW 「무엇인가？ 자주(잘) 들리지 않아요」
		CASE "無理を했다행동"
			PRINTFORMW 「뭐야 그것. 말하고 싶은 일이 있다면 분명하게 말하면 어떻게야？」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,1 勇気を出して強く当たった
;━━━━━━━━━━━━━━━━━━━━━━━━
;リアクションが강하게 응답한다の場合
IF 구상記入チェック(100) && TFLAG:ACT파생 == 1
	SELECTCASE GET_COM_RANK()
		CASE "自然な행동"
			PRINTFORMW 「에 , 말하잖아…」
			PRINTFORMW 「다음에 후회하는 일이 되지 않으면 좋네요」
		CASE "無理のない행동"
			PRINTFORMW 「인형의 버릇에 건방지구나…」
			PRINTFORMW 「아무튼 좋은데. 그렇게 말할 수 있는 것도 지금 가운데야」
		CASE "無理を했다행동"
			PRINTFORMW 「건방진 일 하는 버릇에, 나와 눈을 맞출 수 없다든가」
			PRINTFORMW 「굉장한 일 없네요,%CNAME(MASTER, TARGET)%…」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,2 皮肉「今の気分は、と聞かれても」
;━━━━━━━━━━━━━━━━━━━━━━━━
;リアクションが빈정거린다の場合
IF 구상記入チェック(100) && TFLAG:ACT파생 == 2
	SELECTCASE GET_COM_RANK()
		CASE "自然な행동"
			PRINTFORMW 「재미있지 않다……」
			PRINTFORMW 「이것은 다시 조금 예의범절을 가르칠 필요가 있을 듯 하구나…」
		CASE "無理のない행동"
			PRINTFORMW 「%CNAME(MASTER, TARGET)%, 꽤 재미있는 일을 말해요？」
			PRINTFORMW 「이것은, 학째가 겉껍데기 있을 것이구나……」
		CASE "無理を했다행동"
			PRINTFORMW 「그런 허리가 파한 태도로 빈정거려져도, 응？」
			PRINTFORMW 「이라고 하는지, 아무튼……보기 흉하구나」
	ENDSELECT
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,3 はっきり今の気持ちを告げた
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류が적극적으로 한다の場合
IF 구상記入チェック(100) && TFLAG:ACT파생 == 3
	;=================================================
	;주인님と居られることが嬉しい「初回」
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "自然な행동" && FIRSTREACT(30)
		PRINTFORMW 「어머나, 그래？ 뜻밖의 반응이군요, 그렇지만…」
		PRINTFORMW 「그 말에 거짓말은 없는 것 같고, 나도 솔직하게 기쁘다고 생각해요. 고마워요」
		PRINTFORM 「이런 것, 그다지 (무늬)격이 아니지만, 이군요
		CALL PRINT_SYMBOL_K14
		RETURN 1
	ENDIF ;			ここでKFLAG代入してもいいかも知れない
	
	;負荷1に限定する
	;=================================================
	;전턴で中出し（성교/성교봉사）
	;=================================================
	IF 구상記入チェック(100) && GROUPMATCH(TFLAG:전회질싸, 1, 2, 5, 6) > 0 && GET_COM_RANK() == "無理のない행동"
		;죠교대상からの호의2000以上		これは호감도の下限を見ているので호의3000以上等でもヒットする
		IF AFFECTION("익애")
			SELECTCASE TFLAG:전회질싸
				;전턴성교봉사で中出し
				CASE 2, 6
					PRINTFORMW 「 나\@ TFLAG:전회질싸 == 6 ? 의 엉덩이에 # 에 \@질내사정해, 완전히 기분이군요？」
					PRINTFORM 「그것은 최상이지만…, 이것으로 마지막은 (뜻)이유가 아니니까…
				;전턴성교で中出し
				CASE 1, 5
					PRINTFORMW 「후후,\@ TFLAG:전회질싸 == 5 ? 엉덩이의 # \@안에 나와 기쁜 듯하구나」
					PRINTFORM 「%CNAME(MASTER, TARGET)%가 바란다면, 얼마든지 시라고 주어요
			ENDSELECT
			CALL PRINT_SYMBOL_K14
		;その他
		ELSE
			SELECTCASE TFLAG:전회질싸
				;전턴성교봉사で中出し
				CASE 2, 6
					PRINTFORMW 「어머나, 기분이군요？ 나중에서 이의 것이 그렇게 좋았던 것일까 해들？」
					PRINTFORMW 「후후, 안되어요.%RAND_SPLIT("기쁜 듯한/에 질투났다/그런")%얼굴로 부정해도, 설득력이 부족해요」
				;전턴성교で中出し
				CASE 1, 5
					PRINTFORMW 「후후…,\@ TFLAG:전회질싸 == 5 ? 엉덩이의 # \@안에 나와 만족하구나」
					PRINTFORMW 「어머나, 속여도 안되어요. 그 기쁜 듯한 얼굴은, 나의 기분탓은 (뜻)이유가 아닐 것입니다？」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	
	LOCALS = \@ AFFECTION("상사상애") ? ふふふ # フフフ \@
	;-----------------------------------
	;条件から漏れたらここで処理
	;-----------------------------------
	SELECTCASE GET_COM_RANK()
		CASE "自然な행동"
			PRINTFORMW 「%RAND_SPLIT("뭐 당연해요/에에, 알고 있었어요/이제 와서 지나요")%？ 우도, 다른 대답이라면%RAND_SPLIT("殴っ/張り倒し")%비칠지도 모르는데」
			;1/2でPRINT
			IF RAND:2
				PRINTFORMW 「%LOCALS%…」
			;1/2でフェードアウト
			ELSE
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), @"「%LOCALS%…」")
				SIF ANIME_CONFIG("FADE")
					CLEARLINE 1
			ENDIF
			PRINTFORMDW %CALLNAME%(은)는 농담인 듯한 모습으로 대답했지만, 그 표정은 웃는 얼굴로 가득 차고 흘러넘치고 있었다…
		CASE "無理のない행동"
			;죠교자BASE:만족派生
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					PRINTFORM 「솔직한
					IF TALENT:MASTER:작은체형
						PRINTFORM \@ TALENT:MASTER:남자 ? 남자 # 여자 \@의 아이는 좋아해요
					ELSE
						PRINTFORM 의는 좋은 일이군요
					ENDIF
					CALL PRINT_SYMBOL_K14
				CASE "一般"
					PRINTFORMW 「\@ AFFECTION("익애") ? 후후 # 후후 \@…, 좋은 경향이군요」
				CASE "低い"
					PRINTFORMW 「그래요…, 그것으로 좋아요……」
			ENDSELECT
		CASE "無理を했다행동"
			;죠교자BASE:만족派生
			SELECTCASE TENSION(TARGET, 8)
				CASE "高い"
					;こういうの大好きです
					PRINTFORMW 「%RAND_SPLIT("아라/헤에")%, 그렇게. 그렇지만 그 거, 입으로부터 출 맡겨라？」
					PRINTFORM 「마음이 가득차지 않은 말,
					CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "ありがとう", 1)
					PRINTFORMW 」
					;죠교자の상태が분노
					SIF IS_CONDITION(TARGET, "분노")
						PRINTFORMW 「너무 감격해 , 눈물이 나와요！」
				CASE "一般"
					PRINTFORM 「그 정도로 이
					CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "私", 1)
					PRINTFORMW %RAND_SPLIT("의 비위를 맞추자/에 아양을 팔자")%이라니. 싸게 볼 수 있던 것…」
					PRINTFORMW 「보이고 틈이 난 연기는 그만두어%RAND_SPLIT("받을 수 있을까？/줘.")% 불%RAND_SPLIT("愉/")%기분 좋은 것, 분명하게 말하며」
				CASE "低い"
					PRINTFORMW 「%RAND_SPLIT("노골적인/보이고 틈이 났다")%기분 잡기는, 실로%RAND_SPLIT("불쾌なもの/不愉快")%그런데」
					PRINTFORMW 「어차피라면, 좀 더 능숙하게 하면 된다고 생각하는데.%CNAME(MASTER, TARGET)%에는%RAND_SPLIT("무리/어렵다")%까. 응？」
			ENDSELECT
	ENDSELECT
	RETURN 1
ENDIF

;なんだこの微妙なACT파생はｗ
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,4 まあまあ悪くはない
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:ACT파생 == 4
	SELECTCASE GET_COM_RANK()
		;自然な행동/無理のない행동		面倒なんでまとめてしまう
		CASE "自然な행동", "無理のない행동"
			IF AFFECTION("상사상애")
				PRINTFORMW 「은？ \@ GET_COM_RANK() == "자연스러운행동" ? 나쁘다고 생각하지 않았다 # 『그저 나쁘지는 않다』\@(이)라든지, 이제 그러한 단계가 아니라고 생각하는데？」
				PRINTFORM 「%CNAME(MASTER, TARGET)%, 혹시…, 이
				CALL PRINTFONTSTYLE(COLOR2_K14("ALICE"), "私", 1)
				PRINTFORMW (을)를 화나게 하고 싶은 것일까. 응？」
				PRINTFORMDW %CALLNAME%(은)는 평정을 가장하고 있지만, 그 눈은 얼음과 같이 차가왔다…
			ELSEIF AFFECTION("익애")
				PRINTFORMW 「그렇게, 그것은 최상이구나？ 그렇지 않으면 괴로운 것뿐이지요 해」
				CALL FADEIN(13, "「서로 취해…」")
			ELSEIF AFFECTION("권애")
				PRINTFORMW 「기우군요？ 나도%CNAME(MASTER, TARGET)%의 일, 그렇게 나쁘지 않다고 생각하고 있었어요」
				PRINTFORM 「그렇다고 해서, 그만큼 좋다고 생각되지 않지만…그런데
				CALL PRINT_SYMBOL_K14
			ELSE
				PRINTFORMW 「어머나, 그래？ 다양하게 심한 일 되어 왔는데？」
				PRINTFORMW 「아과연….\@ ABL:MASTER:마조끼 > 4 ? 마조돼지 # %CNAME(MASTER, TARGET)% \@에 있어 나쁜 상황도 아니다는 것인 거네」
				PRINTFORMW 「뭐, 그러한 일이라면…, 지금부터 좀 더 즐겨 받을 수 있다고 생각해요？」
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「ウフフ…」")
				CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
				CLEARLINE 1
			ENDIF
		CASE "無理を했다행동"
			;実は積極
			IF COMCOR_POSI()
				PRINTFORMW 「뭐야 그것.\@ !TEQUIP:눈가리개 ? %CNAME(MASTER, TARGET)%, 어디 봐 말해 # 작은 소리로 중얼거려도 들리지 않아 \@」
				PRINTFORMW 「이봐요, 여기를 향해. 사\@ !TEQUIP:눈가리개 ? 의 눈을 보면서 # 의 (분)편을 향해 \@\@ AFFECTION("익애") ? 말하는거야 # 말하세요 \@」
				PRINTFORMW 「그러면, 최초부터 다시 해. 분명하게 (들)물어 있어 주기 때문에.\@ AFFECTION("익애") ? 후후 # 후후 \@…」
			;実は嫌
			ELSE
				PRINTFORMW 「…필사적으로 강한척 해 버려서.%RAND_SPLIT("사랑스러운 거네/철없네요")%」
				PRINTFORMW 「 그렇지만 그렇구나….%CNAME(MASTER, TARGET)%가 나쁘지 않다고 말한다면…, 사양은 필요하지 않은 것일까. 응？」
				IF ANIME_CONFIG("FADE") && !RAND:2
;OriginalString : 20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「ウフフ…」")
					CALL FADE(20, COLOR2_K14("ALICE"), COLOR_K14("black"), "「우후후…」")
					CLEARLINE 1
				ELSE
					CALL FADEIN(13, "「우후후…」")
					WAIT
				ENDIF
			ENDIF
	ENDSELECT
	RETURN 1
ENDIF

;REACT분류"受け入れる"(COM6, 더 하드하게 해/COM8, 기분좋게 해)等の場合にACT파생5になる
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT파생,5 気持ち良いこと했다い
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TFLAG:ACT파생 == 5
	;=================================================
	;自然な행동
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "自然な행동"
		;-------------------------------------------------
		;자위중/자위를과시한다中
		;-------------------------------------------------
		IF 구상記入チェック(100) && TRUECHECK(GETBIT(TEQUIP:자위중, 0), GETBIT(TEQUIP:자위중, 1)) > 0 && !TEQUIP:눈가리개
			;죠교대상が자위중
			IF GETBIT(TEQUIP:자위중, 0)
				SELECTCASE RANDREACT(2, 50)
