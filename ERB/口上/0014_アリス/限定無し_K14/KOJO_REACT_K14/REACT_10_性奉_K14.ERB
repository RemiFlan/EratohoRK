;========================================================================
;성교봉사系
;========================================================================
;・COM20, 허리를 흔든다はACTによってREACT분류が변화する
;・역강간の場合はREACT분류("受け入れる")
;・その他성교봉사ACTではREACT분류("적극적으로 한다")扱いとなる
;・この변화は関数@SEX_INITIATIVEにて行われる
;-------------------------------------------------

;────────────────────────────────────
;REACT95, 정상위시킨다・個別リアクション
;────────────────────────────────────
;・TFLAG:ACT파생(0=통상/1=연속/2=Ｖ성교봉사系から/3=Ａ성교系から/4=Ｖ성교系から/5=Ａ성교봉사系から)
;-------------------------------------------------
;想定されるCOM…
;애무系反応
;	COM:부끄러워한다/싫어한다/쾌감을 참는다/쾌감을 받아들인다/기분 좋다!
;성교系反応
;	COM:허리를 흔든다
;고통系反応
;	COM:날뛴다
;봉사系反応
;	COM:거부한다/소극적으로 한다/적극적으로 한다/애정을 담아 한다/태연하게 한다
;その他反応
;	COM:가게해줘！
;-------------------------------------------------
@KOJO_REACT_K14_95
;-------------------------------------------------
;・地の文はREAC分類で分岐
;・REACT분류が날뛴다/거부の場合は취소扱い
;・ACTが취소となった場合はTEQUIP:성교봉사중もリセット
;・その他のREACT분류(受け入れる/적극적으로 한다/태연하게 한다/용서를 빌다/소극적으로 한다)の場合は受容扱いとなる
;・죠교대상が동정なら동정상실、죠교자が처녀なら처녀상실が起こる
;・TEQUIP:성교봉사중が立っている際にREACT분류が날뛴다だと拒絶扱いになる(即ち、성교봉사系ACT以外でも拒絶処理となる場合がある)
;-------------------------------------------------
;오토모드用処理
[IF_DEBUG]
	IF TCVAR:처녀상실 && FLAG:오토모드 && FLAG:오토모드표시설정 == 0 && FIRSTREACT(100)
		CALL PRINT_RAINBOW2W("처녀상실", 20)
		CALL GAME_OVER_K14
		FORCEWAIT
		RETURN 1
	ENDIF
[ENDIF]

;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류が날뛴다/逃げる/거부
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && IS_COMGRONAME("날뛴다/逃げる/거부") && FLAG:지문제어 == 2
	CALL PRINT_ACTGR_10_REJECT_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;애태움開始
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && TCVAR:MASTER:가버려 && TCVAR:MASTER:애태움도 > 1 && IS_COMGRONAME("적극적으로 한다/태연하게 한다/受け入れる")
	CALL TEXTDISPLAY_CATEGORY_10_TEASE_K14
	RETURN 1
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;COM, 쾌감을 참는다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(55) && IS_NOWCOMNAME("쾌감을 참는다") && TCVAR:MASTER:참다
	CALL TEXTDISPLAY_CATEGORY_10_COM14_K14
	RETURN 1
ENDIF

;COM15,쾌감을 받아들인다/COM16,기분 좋다!等を想定。ACT103,역강간の場合、COM20,허리를 흔든다もREACT분류受け入れるになる
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류, 受け入れる
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && IS_COMGRONAME("受け入れる")
	;=================================================
	;죠교대상が가버려		子宮입に届くほど深く云々…
	;=================================================
	IF 구상記入チェック(100) && TCVAR:MASTER:가버려
		CALL PRINT_ACTGR_10_ACCEPT_0_K14
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자が가버려
	;=================================================
	IF 구상記入チェック(100) && PALAM:쾌Ｖ > 8500
		CALL PRINT_ACTGR_10_ACCEPT_1_K14
		RETURN 1
	ENDIF
	
	;죠교대상/죠교자が가버려な場合以外を処理
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック(70)
		CALL PRINT_ACTGR_10_ACCEPT_2_K14
		RETURN 1
	ENDIF
;条件から漏れたら汎用リアクション（COM）へ
RETURN 0
ENDIF

;消極的反応をまとめる
;━━━━━━━━━━━━━━━━━━━━━━━━
;REACT분류, 消極的に従った
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && IS_COMGRONAME("소극적으로 한다/용서를 빌다")
	;=================================================
	;REACT부하2(無理を했다행동)
	;=================================================
	IF 구상記入チェック(100) && GET_COM_RANK() == "無理を했다행동"
		;実は積極
		IF COMCOR_POSI()
			PRINTFORMW 「…응. 부끄러워해 보였더니, 허리는 멈추지 않구나…」
			PRINTFORMW 「\@ AFFECTION("상사상애") ? 저기, # \@조금은 솔직해지면 어때\@ TENSION(TARGET, 7) == "高い" ? (이)야！ # ？ \@」
			SIF IS_COMGRONAME("소극적으로 한다")
				PRINTFORMW 「나머지%CNAME(MASTER, TARGET)%…, 나부터 시선을 일의,\@ AFFECTION("상사상애") ? 그만두세요！ # 그만두어 받을 수 있다？ \@」
		;実は嫌
		ELSE
			PRINTFORMW 「…\@ IS_COMGRONAME("소극적으로 한다") ? 단조롭고 시시해요 # 나의 움직임에 맞추는 일도 할 수 없다든가\@. 싫증%RAND_SPLIT("(이)야/저기")%,%CNAME(MASTER, TARGET)%…」
			PRINTFORMW 「그러니까, 그렇지 않아서！ 아 이제…, 정말로%RAND_SPLIT("속상하다/안타깝다/안절부절 한다")%\@ TENSION(TARGET, 7) == "高い" ? のよっ！ # わね… \@」
		ENDIF
		RETURN 1
	ENDIF
	
	;REACT파생1, 消極的なのが気に入らないの場合はCOMで処理
	;=================================================
	;COM41,소극적으로 한다「消極的だけどいいか」
	;=================================================
	IF 구상記入チェック(55) && IS_NOWCOMNAME("소극적으로 한다") && TFLAG:REACT파생 == 0
		IF AFFECTION("상사상애")
			PRINTFORMW 「아…팥고물……아……」
			PRINTFORMW 「응…초조하게 하지 마……좀 더 격렬하게 해……！」
		ELSEIF AFFECTION("익애")
			PRINTFORMW 「안타까워서……이상해질 것 같다…」
			PRINTFORMW 「응……좀 더, 좀 더……！」
		ELSEIF AFFECTION("권애")
			PRINTFORMW 「무슨 소극적이구나……안타까워요…」
		ELSE
			PRINTFORMW 「 좀 더 허리 거절하세요…」
			PRINTFORMW 「이봐요, 좀 더 마음껏…」
		ENDIF
		RETURN 1
	ENDIF
ENDIF

;━━━━━━━━━━━━━━━━━━━━━━━━
;허리를 흔든다
;━━━━━━━━━━━━━━━━━━━━━━━━
IF 구상記入チェック(100) && IS_NOWCOMNAME("허리를 흔든다")
	;=================================================
	;가버려상태
	;=================================================
	IF 구상記入チェック(100) && TCVAR:MASTER:가버려
		SELECTCASE TCVAR:MASTER:애태움도
			CASE 1 ;애태움終了
				;@AUOTHEART系を使い、地の文捏造箇所を分岐させてやれば一つに纏められそうだけど後回し
				IF AFFECTION("상사상애")
					;RR 2
					SELECTCASE RANDREACT(2, 2)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응！ , 바보！ 그렇게 격렬하게 찔리면(자)！」
							CALL QUAKE(4)
							PRINTFORM 「이나 , 격렬해져！ 팥고물
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　あっ、あぁっ！！」")
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「은！ 아
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　あぁっ！ こ、こんなの…ってッ！」")
							CALL QUAKE(4)
							PRINTFORMW 「이나, 망가진다！ 부서져버리는！」
					ENDSELECT
					PRINTFORMDW 자궁구까지 도려내지는 감각에, 앨리스는 머리카락을 흩뜨려 허덕여 번민이라고 있다…
				ELSE
					;RR 3
					SELECTCASE RANDREACT(2, 3)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「응！ , 바보！ 그렇게 격렬하게 찔리면(자)！」
							CALL QUAKE(4)
							PRINTFORMW 「이나 , 격렬해져！ 아, 아！！」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「은！ 아, 아！ 개, 이런 건…라는！」
							CALL QUAKE(4)
							PRINTFORMW 「이나, 망가진다！ 부서져버리는！」
					ENDSELECT
					PRINTFORMDW 질내의 최안쪽까지 켜 거론되는 감각에, 앨리스는 미친 것처럼 허덕여 번민이라고 있다…
				ENDIF
			CASEELSE ;애태움無し
				IF AFFECTION("상사상애")
					;RR 4
					SELECTCASE RANDREACT(2, 4)
						CASE 0
							CALL QUAKE(4)
							PRINTFORM 「아！ , 안쪽에 해당되어！ 아, 아
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORMW 「이나, 거기！ 때굴때굴 되면(자)！」
						CASE 1
							CALL QUAKE(4)
							PRINTFORM 「아, 아
							CALL HEARTCB(COLOR2_K14("pink"), 1, "ＭＳ Ｐゴシック")
							PRINTFORM  팥고물, 후~！ 응응
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
							CALL QUAKE(4)
							PRINTFORM 「개, 이런 건…！ , 견딜 수 있는 것……
							CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"))
					ENDSELECT
				ELSE
					;RR 5
					SELECTCASE RANDREACT(2, 5)
						CASE 0
							CALL QUAKE(4)
							PRINTFORMW 「아！ , 안쪽에 해당되어！ 아, 아！」
							CALL QUAKE(4)
							PRINTFORMW 「이나, 거기！ 때굴때굴 되면(자)！」
						CASE 1
							CALL QUAKE(4)
							PRINTFORMW 「아, 아！ 팥고물, 후~！ 응응！！」
							CALL QUAKE(4)
							PRINTFORMW 「개, 이런 건…！ , 견딜 수 있는 것……！」
					ENDSELECT
				ENDIF
				PRINTFORMDW 자궁구에 닿을 정도의 깊고 격렬한 찌르기 포함에, 앨리스는 전신을 진동시키면서 허덕여 번민이라고 있다…
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;互いに정욕
	;=================================================
	IF 구상記入チェック(100) && (M_COND("정욕") && T_COND("정욕"))
		CALL QUAKE(4)
		PRINTFORMW 「은 , 팥고물！ 아……아…아, 아！！」
		CALL QUAKE(4)
		PRINTFORMW 「응！ 도, 이제……정말로！ , 이상해져버리는！」
		PRINTFORMDW 정욕에 고두 명은 서로 서로 미칠 듯하게 요구해 , 짐승과 같이 격렬하게 사귀고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교대상が정욕
	;=================================================
	IF 구상記入チェック(100) && M_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「아,！ 개, 이런 건…이제, 견딜 수 있고！ 아…아″…」
		CALL QUAKE(4)
		PRINTFORMW 「아, 후~！ (이)나…도 , 다멕！ 아, 아！」
		PRINTFORMDW 정욕에 고%CALLNAME:MASTER%의 격렬한 찌르기 포함에, 앨리스는 머리카락을 흩뜨리면서 격렬하게 허덕이고 있다…
		RETURN 1
	ENDIF
	
	;=================================================
	;죠교자が정욕
	;=================================================
	IF 구상記入チェック(100) && T_COND("정욕")
		CALL QUAKE(4)
		PRINTFORMW 「！ (이)나, 나다……요…멈추지 않게 되어버리는！」
		CALL QUAKE(4)
		PRINTFORMW 「응,…하, 팥고물！ , 거기！ 아, 아…」
		PRINTFORMDW 정욕에 고앨리스는 허리를 격렬하게 쳐 붙여 쾌락을 탐내고 있다…
		RETURN 1
	ENDIF
	
	;確率で汎用リアクションに
	;=================================================
	;その他
	;=================================================
	IF 구상記入チェック(60)
		IF AFFECTION("상사상애")
			;RR 6
			SELECTCASE RANDREACT(2, 6)
				CASE 0
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)% 응 좀 더, 좀 더…！」
					CALL QUAKE(4)
					PRINTFORM 「, 아
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　そんな、激しくッ！ あんっ、あぁっ！」")
				CASE 1
					CALL QUAKE(4)
					PRINTFORMW 「하, 하……응…응！ 아……아！」
					CALL QUAKE(4)
					PRINTFORM 「아……아, 아
					CALL PRINT_HEARTCBW_K14(COLOR2_K14("pink"), , , COLOR2_K14("ALICE"), "　んッ、んぁぁっ！」")
					PRINTFORMDW 앨리스는 물기를 띤 눈동자로%CALLNAME:MASTER%(을)를 응시하면서, 격렬한 찌르기 포함을 받아 들이고 있다…
			ENDSELECT
		ELSE
			;RR 7
			SELECTCASE RANDREACT(2, 7)
				CASE 0
					PRINTFORMW 「%KOJO_RAND_K14(("키스"), 1)% 하면서의 키스도 나쁘지 않은, 원이군요…」
					CALL QUAKE(4)
					PRINTFORMW 「, 아！ 그런, 격렬하고！ 팥고물, 아！」
				CASE 1
					CALL QUAKE(4)
					PRINTFORMW 「하, 하……응…응！ 아……아！」
					CALL QUAKE(4)
					PRINTFORMW 「아……아, 아！ 응, 응！」
					PRINTFORMDW 앨리스는 %CALLNAME:MASTER%(을)를 가만히 응시하면서, 격렬한 찌르기 포함을 받아 들이고 있다…
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF

