	;=================================================
	;죠교자が절정
	;=================================================
	IF 구상記入チェック(100) && TRUECHECK(NOWEX:Ｖ절정, NOWEX:Ａ절정) > 0
		;아라이멘도正
		IF ALI(0)
			SELECTCASE RANDEVENT(3, 0)
				CASE 0
;OriginalString : 「손、握って……？」
