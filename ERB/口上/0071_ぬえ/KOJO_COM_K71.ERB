;-------------------------------------------------
;COM0, 침묵한다
;-------------------------------------------------
;・従ったことにはならない
;・質問に対して黙秘했다場合は軽く반항했다扱いになる
;-------------------------------------------------
@KOJO_COM_K71_0
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;상사상애
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(3000, 3000)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;익애
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 2000)
;OriginalString : 「ちょっと…何黙ってんのよ…」
		PRINTFORMW 「조금…무엇 입다물고 있어…」
		RETURN LOCAL
	ENDIF 
	
	;-------------------------------------------------
	;연모
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FAVOR(0, 1000)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;-------------------------------------------------
	;不倶戴天
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && !FAVOR(-300)
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL
	ENDIF
	
	;一般
;OriginalString : 「何、だんまり？」
	PRINTFORMW 「무엇, 침묵？」
	RETURN LOCAL
ENDIF

