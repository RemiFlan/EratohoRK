;=================================================
;EV101, 의상変更
;=================================================
;・ショップから의상変更を行った場合に表示
;-------------------------------------------------
@KOJO_EVENT_K71_101(ARG)
;-------------------------------------------------
;引数内訳…
;ARG=
;　一の位…（0, もう着てる、1, 着替えた、2, 恥ずかしくて着られない、3, 嫌がって着ない、9, 持ってない）
;　十の位…（0, 통상、10, 반라、20, 전라、30, 속옷無し）
;　百の位…코스프레のために保留
;　千の位…（0, 죠교자, 1000, 죠교대상）
;　例（죠교대상が恥ずかしがって전라にできなかった＝1022）
;-------------------------------------------------

;-------------------------------------------------
;죠교자が着替えた
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;통상되었다
	IF ARG == 1
;OriginalString : 「ま、普通が一番」
		PRINTFORMW 「뭐, 보통이 제일」
		RETURN 2
	ENDIF
	
	;반라되었다
	IF ARG == 11
;OriginalString : 「にひひっ♪무라무라する？」
		PRINTFORMW 「에 히히♪불끈불끈 한다？」
		RETURN 2
	ENDIF
	
	;전라되었다
	IF ARG == 21
;OriginalString : 「ま、손っ取り早いよね」
		PRINTFORMW 「뭐, 민첩하지요」
		RETURN 2
	ENDIF
	
	;……すーすーする
	IF ARG == 31
;OriginalString : 「チラリズム～♪ ほらほらどうよぉ」
		PRINTFORMW 「치라리즘～♪ 이봐요 이봐요 어떻게」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;죠교자が恥ずかしがって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;반라だって恥ずかしい
	IF ARG == 12
;OriginalString : 「変態思考め…」
		PRINTFORMW 「변태 사고째…」
		RETURN 2
	ENDIF
	;普段から전라はちょっと……
	IF ARG == 22
;OriginalString : 「これはー…ちょっと…」
		PRINTFORMW 「이것은―…조금…」
		RETURN 2
	ENDIF
	;パンツが無いから恥ずかしいもん！
	IF ARG == 32
;OriginalString : 「…전라より恥ずかしいんだけど」
		PRINTFORMW 「…전라보다 부끄럽지만」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;죠교자が嫌がって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG < 1000
;小分岐
	;반라は嫌
	IF ARG == 13
;OriginalString : 「何で私が？」
		PRINTFORMW 「무엇으로 내가？」
		RETURN 2
	ENDIF
	;普段から전라なんて嫌
	IF ARG == 23
;OriginalString : 「お前、立場分かってないんじゃないの」
		PRINTFORMW 「너, 입장 알지 않은거 아니야」
		RETURN 2
	ENDIF
	;すーすーするのは嫌
	IF ARG == 33
;OriginalString : 「馬鹿にしてる？」
		PRINTFORMW 「바보취급 하고 있다？」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;죠교대상を着替え시켰다
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;小分岐
	;통상되었다
	IF ARG == 1001
;OriginalString : 「ま、良いけど。」
		PRINTFORMW 「뭐, 좋은데.」
		RETURN 2
	ENDIF
	;반라되었다
	IF ARG == 1011
;OriginalString : 「期待しちゃってる見たいね～♪」
		PRINTFORMW 「기대해 버린다 보고 싶다～♪」
		RETURN 2
	ENDIF
	;전라되었다
	IF ARG == 1021
;OriginalString : 「あははっ、恥ずかしく無いわけ？」
		PRINTFORMW 「아하하 , 부끄럽고 없는 것？」
;OriginalString : 「노예の自覚が出てきたの？良い子良い子♪」
		PRINTFORMW 「노예의 자각이 나왔어？좋은 아이 좋은 아이♪」
		RETURN 2
	ENDIF
	;……すーすーする
	IF ARG == 1031
;OriginalString : 「へ～…ほら、스커트めくってみなよ」
		PRINTFORMW 「에～…이봐요, 스커트 넘겨 봐」
		RETURN 2
	ENDIF
ENDIF

;-------------------------------------------------
;죠교대상が恥ずかしがって着替えられない
;-------------------------------------------------
;記入チェック
LOCAL = 1
;-------------------------------------------------
IF LOCAL && ARG >= 1000
;小分岐
	;반라だって恥ずかしい
