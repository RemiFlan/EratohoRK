	;COM13, 겁없이 웃는다(REACT분류"태연하게 한다")
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("겁없이 웃는다")
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM14, 쾌감을 참는다
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("쾌감을 참는다")
		;派生:애원					REACT분류"용서를 빌다"
		IF TFLAG:REACT파생 == 1
;OriginalString : 
			PRINTFORMW 
		;派生:耐久					REACT분류"소극적으로 한다"
		ELSE
;OriginalString : 
			PRINTFORMW
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM15, 쾌감을 받아들인다
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("쾌감을 받아들인다")
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;COM16, 기분 좋다!
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && IS_NOWCOMNAME("기분 좋다!")
;OriginalString : 
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
;条件にヒットしなければ汎用リアクションに流す
RETURN 0
ENDIF

