		;CASE 9
		;	PRINTFORMW 
		;고통각인Lv10
		CASE 10
			PRINTFORMW 
		;表示しない
		CASEELSE
			CLEARBIT LOCAL:1, 0
	ENDSELECT
	SIF GETBIT(LOCAL:1, 0) && LOCAL == 1
