;ABL生成

;순종
@CM_SET_ABL_0
;基本判定値10
LOCAL = 10
;素質判定値を利用
LOCAL += CM_TALENT_MAKE("プライド逆", "被虐", "솔직さ", "솔직さ", "モラル", "수치逆", "攻撃逆", "知性逆", "神経質逆")
;솔직だと判定値+50
SIF TALENT:MASTER:솔직
	LOCAL += 50
;반항적だと判定値-50
SIF TALENT:MASTER:반항적
	LOCAL -= 50
;겁쟁이だと判定値+30
SIF TALENT:MASTER:겁쟁이
	LOCAL += 30
;굳셈だと判定値-30
SIF TALENT:MASTER:굳셈
	LOCAL -= 30
;작은체형だと判定値+30
SIF TALENT:MASTER:작은체형
	LOCAL += 30
;얌전한と判定値+20
SIF TALENT:MASTER:얌전한
	LOCAL += 20
;건방짐だと判定値-40
SIF TALENT:MASTER:건방짐
	LOCAL -= 40
;프라이드높음と判定値-50
SIF TALENT:MASTER:프라이드높음
	LOCAL -= 50
;프라이드낮음だと判定値+25
SIF TALENT:MASTER:프라이드낮음
	LOCAL += 25
;유치だと判定値+25
SIF TALENT:MASTER:유치
	LOCAL += 25
;３回回す
FOR LOCAL:1, 0, 3
	;加算失敗했다ら終了
	SIF !PERCENT(LOCAL)
		BREAK
	ABL:MASTER:순종++
	LOCAL /= 2
NEXT

