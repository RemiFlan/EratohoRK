;────────────────────────────────────
;ボーナス취득のメニュー。ボーナスの種類と필요ポイントは仮の設定です
;────────────────────────────────────
@BONUS_GAIN
#DIM DAYONCE = -1

#DIM LINE_WIDTH
#DIM ITEM_LIST, 4, 10
#DIM ITEM_SIZE, 4
#DIM TALENT_LIST, 8, 15
#DIM TALENT_SIZE, 8
#DIM CONST TALENT_TBL = 52, 55, 57, 58, 59, 64, 66, 83, 91, 108, 109, 121, 130, 135, 140

VARSET LOCAL
VARSET ITEM_LIST
VARSET ITEM_SIZE
VARSET TALENT_LIST
VARSET TALENT_SIZE

DRAWLINE
PRINTFORML %CALLNAME:MASTER%(을)를 %CALLNAME:TARGET%이(가) 조교중
SIF ASSI:1 >= 0
	PRINTFORML 　(조수１：%CALLNAME:(ASSI:1)%)　\@ ASSI:2 >= 0 ? (조수２：%CALLNAME:(ASSI:2)%) # \@
PRINTFORML [LV {CFLAG:TARGET:0}　EXP {CFLAG:TARGET:1}/{CFLAG:TARGET:4}　NEXT {CFLAG:TARGET:4-CFLAG:TARGET:1}]
PRINTFORML (나머지 마력 {CFLAG:마력})

;────────────────────────────────────
;これまで취득했다ボーナスの表示
;処理がほぼSQRのままで、表示情報が不발していたので処理変更。微調整@revkoishi(14/05/26)
LINE_WIDTH = WIDTH()

;追加죠교자
PRINTFORM \@ FLAG:추가조교자수 ? 追加당했다죠교자：{FLAG:11,2}인\n # \@

;도구。基本的な処理はステータスの素質分類表示と同じです
;似通った処理なのでもう少し頭使えば関数化できそうな気もするけど、今ならもっと賢いやり方があるかも
FOR LOCAL:0, 0, 50	;分類ループ
	SIF ITEM:(LOCAL:0)
		ITEM_LIST:ITEM_TYPE(LOCAL:0):(ITEM_SIZE:ITEM_TYPE(LOCAL:0)++) = LOCAL:0
NEXT
IF VARSIZE("ITEM_SIZE") - MATCH(ITEM_SIZE, 0)
	PRINTFORML -- □ 취득한 도구 □ \@ ITEM:커스텀도구 ? (性具カスタマイズ済) %"-" * (LINE_WIDTH - 43)% # %"-" * (LINE_WIDTH - 22)% \@
	FOR LOCAL:0, 1, 4
		IF ITEM_SIZE:(LOCAL:0)	;その分類の도구が無ければ飛ばす
			;도구分類名を表示
			PRINTFORM %ITEM_TYPENAME(LOCAL:0), 8, RIGHT%：
			LOCAL:2 = 10	;桁位置(↑表示後の位置)
			FOR LOCAL:1, 0, ITEM_SIZE:(LOCAL:0)	;その分類の도구所持数分ループ
				;도구名を취득([도구名] )
				LOCALS:0 = [%GET_ITEMNAME(ITEM_LIST:(LOCAL:0):(LOCAL:1))%]%" "%
				LOCAL:3 = STRLENS(LOCALS:0)	;↑の文字数(도구名+[]+スペース)
				LOCAL:2 += LOCAL:3			;桁位置加算
				IF LOCAL:2 > LINE_WIDTH		;このまま表示すると途中改行されそうな場合は改行処理を行う
					;改行処理(折り返し、行頭インデント、桁位置再計算)
					PRINTFORM \n          
					LOCAL:2 = 10 + LOCAL:3
				ENDIF
				;도구名の表示
				IF GROUPMATCH(ITEM_LIST:(LOCAL:0):(LOCAL:1), 1, 11, 12, 13, 14) && ITEM:커스텀도구
					CALL PRINT_RAINBOW(LOCALS:0, , , RAND:12)
				ELSE
					PRINTS LOCALS:0
				ENDIF
			NEXT
			PRINTL 
		ENDIF
	NEXT
ENDIF

;素質の表示。上と大体同じ…だったのだけれどちょっと魔が差했다
FOR LOCAL:0, 0, VARSIZE("TALENT_TBL")
	SIF (TALENT:TARGET:(TALENT_TBL:(LOCAL:0)) ^^ CSVTALENT(NO:TARGET, TALENT_TBL:(LOCAL:0))) && TALENT_TYPE(TALENT_TBL:(LOCAL:0))
		TALENT_LIST:TALENT_TYPE(TALENT_TBL:(LOCAL:0)):(TALENT_SIZE:TALENT_TYPE(TALENT_TBL:(LOCAL:0))++) = TALENT_TBL:(LOCAL:0)
NEXT
IF VARSIZE("TALENT_SIZE") - MATCH(TALENT_SIZE, 0)
	LOCAL:4 = GETCOLOR()	;現在の表示色を保持
{
;OriginalString : ("-- □ 変動했다素質 □ (凡例：" + HTMLFONT("[<I> 취득素質 </I>] ", , 0x60FFFF) + 
	CALL HTMLPRINTL("-- □ 변동했다소질 □ (범례：" + HTMLFONT("[<I> 취득소질 </I>] ", , 0x60FFFF) + 
                    HTMLFONT("[<I> 消去素質 </I>]", , 0x606060) + @") %"-" * (LINE_WIDTH - 56)%")
}
	IF FLAG:소질표시방법	;素質分類表示。ここでやる意味があるのかどうか…とりあえずユーザフレンドリ重点(謎)
		FOR LOCAL:0, 1, 8
			IF TALENT_SIZE:(LOCAL:0)
				PRINTFORM %TALENT_TYPENAME(LOCAL:0), 8, RIGHT%：
				LOCAL:2 = 10
				FOR LOCAL:1, 0, TALENT_SIZE:(LOCAL:0)
					LOCALS:0 = [%GET_TALENTNAME(TARGET, TALENT_LIST:(LOCAL:0):(LOCAL:1))%]%" "%
					LOCAL:3 = STRLENS(LOCALS:0)
					LOCAL:2 += LOCAL:3
					IF LOCAL:2 > LINE_WIDTH
						PRINTFORM \n          
						LOCAL:2 = 10 + LOCAL:3
					ENDIF
					SETCOLOR TALENT:TARGET:(TALENT_LIST:(LOCAL:0):(LOCAL:1)) ? 0x60FFFF # 0x606060
					PRINTS LOCALS:0
					SETCOLOR LOCAL:4
				NEXT
				PRINTL 
			ENDIF
		NEXT
	ELSE					;素質번호順表示
		LOCAL:2 = 0
		FOR LOCAL:0, 0, VARSIZE("TALENT_TBL")
			IF (TALENT:TARGET:(TALENT_TBL:(LOCAL:0)) ^^ CSVTALENT(NO:TARGET, TALENT_TBL:(LOCAL:0))) && TALENT_TYPE(TALENT_TBL:(LOCAL:0))
				LOCALS:0 = [%GET_TALENTNAME(TARGET, TALENT_TBL:(LOCAL:0))%]%" "%
				LOCAL:3 = STRLENS(LOCALS:0)
				LOCAL:2 += LOCAL:3
				IF LOCAL:2 > LINE_WIDTH
					PRINTL 
					LOCAL:2 = LOCAL:3
				ENDIF
				SETCOLOR TALENT:TARGET:(TALENT_TBL:(LOCAL:0)) ? 0x60FFFF # 0x606060
				PRINTS LOCALS:0
				SETCOLOR LOCAL:4
			ENDIF
		NEXT
		PRINTL 
	ENDIF
ENDIF

;────────────────────────────────────
;ボーナスメニューの表示
;名前の余白分
LOCAL = MAX(8 - STRLENSU(CALLNAME:TARGET), 0)
DRAWLINE

;一日一回のみ表示
IF DAYONCE != DAY
	;이벤트구상：마력使用前
	CALL KOJO_EVENT(110, 0)
	CALL VOIDLINE_IF(RESULT)
	DAYONCE = DAY
ENDIF

PRINTFORML %CALLNAME:TARGET%(은)는, %CALLNAME:MASTER%(으)로부터 정제 한 마력을 사용해…
PRINTL [ 0] 이제 되었다
;召喚は주인님限定
SIF TALENT:주인님
	PRINTFORML [ 1] 새로운 조교자를 소환한다　　　　　　　　　　　　 마력1000
SELECTCASE ABL:기교
	CASE 0
		PRINTFORML [ 2] 조교 기교의 재능을 개화시킨다　　　　　　　　　　 마력{RIZE_ABL_V(0), 4}
	CASE IS >= 99
		PRINTFORML [--] 조교 기교는 벌써 최고다
	CASEELSE
		PRINTFORML [ 2] 조교 기교를 높인다　　　　　　　　　　　　　　　 마력{RIZE_ABL_V(0), 4} 요LV{RIZE_ABL_V(1)}
	;試験的に마력で買える기교上限をなくしてみる
	;世の中마력だよ
	;PRINTFORML [--] 죠교기교は実践で高めるしかないだろう
ENDSELECT
PRINTL [ 3] 지식을 습득한다
PRINTL [ 4] 기능을 취득한다
IF FLAG:모드 != 1 || FLAG:디버그 || !(FLAG:AI자유도&1)
	PRINTL [ 5] 외계의 성 도구를 연성 한다
	PRINTL [ 6] 고문도구를 연성 한다
ENDIF
PRINTFORML \@ ITEM:9 ? [--] 도구는 이미 커스텀됬다 # [10] 도구를 커스터마이징한다　　　　　　　　　　　　 마력3000  要[%TALENTNAME:59%] \@
SIF !TALENT:남자
	PRINTFORML [11] \@ TALENT:후타나리 ? 양성화 의식을 취소　 # 양성화 의식을 한다 \@　　　　　　　　　　　　　　 마력7000
SIF !TALENT:남자
	PRINTFORML \@ TALENT:거유 ? [12] 가슴 크기는 최대치이다 # [12] 거유의 의식을 한다　　　　　 \@　　　　　　　　　　 마력5000
SIF !TALENT:남자
	PRINTFORML \@ TALENT:빈유 ? [13] 가슴 크기는 최소치이다 # [13] 빈유의 의식을 한다　　　　　 \@　　　　　　　　　　 마력1000
SIF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
	PRINTFORML \@ MAXBASE:MASTER:사정 < 2500 + CFLAG:MASTER:정력한계 * 500 ? [--] 노예의 정력은 아직 단련해서 늘릴 수 있다 # [16] 노예 정력의 한계를 올린다　　　　　　　　　　　 마력{(MAXBASE:MASTER:사정 - 2500 - CFLAG:MASTER:정력한계 * 500) / 500 * 1000 + 1000} \@
DRAWLINE

;────────────────────────────────────
;入力処理
$INPUT_LOOP
INPUT
SELECTCASE RESULT
	CASE 0
		RETURN 0
	CASE 1
		SIF !TALENT:주인님
			GOTO FAILED
		IF CFLAG:마력 < 1000
			$FAILED_BY_MP
			CLEARLINE 1
			REUSELASTLINE 마력이 부족한 것 같다…
			GOTO INPUT_LOOP
		ENDIF
		CALL NEW_TRAINER
		SIF RESULT
			RETURN 0
	CASE 2
		;기교上昇に挑戦
		CALL RIZE_ABL
		LOCAL = RESULT:1
		SELECTCASE RESULT
			;成功時
			CASE 0
				CALL PRINT_DIALOG(@"%CALLNAME:TARGET%의 죠교기교を<{ABL:기교+1}>に上げてよろしいですか？",1,1,1)
				CALL PRINT_CHOICE(,"はい",1,"いいえ",2,)
				IF RESULT == 1
					;마력を消費
					CFLAG:마력 -= LOCAL
					;기교を上昇
					ABL:기교++
					;이벤트구상：죠교기교アップ
					CALL KOJO_EVENT(110, 1)
					CALL VOIDLINE_IF(RESULT)
					;地の文表示
					PRINTFORMW %CALLNAME:TARGET%의 조교 기교는<{ABL:기교}>(이)가 되었습니다
				ENDIF
			;마력不발時
			CASE 1
				REUSELASTLINE 마력이 부족한 것 같다…(마력{LOCAL}が필요)
				GOTO INPUT_LOOP
			;죠교레벨不발時
			CASE 2
				REUSELASTLINE 기교を上げるには、もう少し경험を積む필요があるだろう…(죠교LV{RIZE_ABL_V(1)}が필요)
				GOTO INPUT_LOOP
			;기교上限時
			CASE 3
				REUSELASTLINE これ以上기교は上げられない
				GOTO INPUT_LOOP
			;その他の問題
			CASEELSE
				$FAILED
				CLEARLINE 1
				GOTO INPUT_LOOP
		ENDSELECT
	CASE 3
		;이벤트구상：지식を得る前
		CALL KOJO_EVENT(110, 2)
		PRINTFORML %CALLNAME%(은)는 마력을 사용해 새로운 지식을 얻기로 했다…
		PRINTL [ 0] - 그만둔다
		PRINTFORML \@ TALENT:배합지식   ? [--] [배합지식]을 가지고 있습니다           # [ 1] 약품의 [배합지식]을 습득　　　　　　　　　　　　 마력{GAIN_TALENT_V("배합지식"  , 0), 5} 요LV{GAIN_TALENT_V("배합지식"  , 1)} \@
		PRINTFORML \@ TALENT:매혹       ? [--] [매혹]을 가지고 있습니다               # [ 2] [매혹]능력을 체득　　　　　　　　　　　　　　　 마력{GAIN_TALENT_V("매혹"      , 0), 5} 요LV{GAIN_TALENT_V("매혹"      , 1)} \@
		PRINTFORML \@ TALENT:금단의지식 ? [--] [금단의지식]을 가지고 있습니다         # [ 3] [금단의지식]을 익힌다　　　　　　　　　　　 마력{GAIN_TALENT_V("금단의지식", 0), 5} 요LV{GAIN_TALENT_V("금단의지식", 1)} \@
		PRINTFORML \@ TALENT:도구능숙   ? [--] [도구능숙]을 가지고 있습니다           # [ 4] [도구능숙]의 지식을 습득　　　　　　　　　　　　 마력{GAIN_TALENT_V("도구능숙"  , 0), 5} 요LV{GAIN_TALENT_V("도구능숙"  , 1)} \@
		PRINTFORML \@ TALENT:촬영기능   ? [--] [촬영기능]을 가지고 있습니다           # [ 5] 사진의 [촬영기능]을 습득　　　　　　　　　　　　 마력{GAIN_TALENT_V("촬영기능"  , 0), 5} 요LV{GAIN_TALENT_V("촬영기능"  , 1)} \@
		$INPUT_LOOP_SKILL
		INPUT
		SELECTCASE RESULT
			CASE 0
				RESTART
			CASE 1 TO 5
				IF 0
				ELSEIF RESULT == 1
					LOCALS:0 = 배합지식
					LOCALS:1 = 薬を正しく扱うには、もう少し경험を積む필요があるだろう…
					LOCAL = 3
				ELSEIF RESULT == 2
					LOCALS:0 = 매혹
					LOCALS:1 = 매혹の能力を得るには、もう少し경험を積む필요があるだろう…
					LOCAL = 4
				ELSEIF RESULT == 3
					LOCALS:0 = 금단의지식
					LOCALS:1 = 금단의지식を紐解くには、もう少し경험を積む필요があるだろう…
					LOCAL = 5
				ELSEIF RESULT == 4
					LOCALS:0 = 도구능숙
					LOCALS:1 = 外界の性具を巧く使うには、まだ경험이 부족한 것 같다…
					LOCAL = 6
				ELSEIF RESULT == 5
					LOCALS:0 = 촬영기능
					LOCALS:1 = カメラを巧く使うには、もう少し경험を積む필요があるだろう…
					LOCAL = 37
				ENDIF
				IF TALENT:LOCALS
					CLEARLINE 1
					GOTO INPUT_LOOP_SKILL
				ENDIF
				;技能취득に挑戦
				CALL GAIN_TALENT(LOCALS)
				LOCAL:1 = RESULT:1
				SELECTCASE RESULT
					;成功時
					CASE 0
						CALL PRINT_DIALOG(@"%CALLNAME:TARGET%が素質[%LOCALS%]を취득します。よろしいですか？",1,1,1)
						CALL PRINT_CHOICE(,"はい",1,"いいえ",2,)
						IF RESULT == 1
							;마력を消費
							CFLAG:마력 -= LOCAL:1
							;技能を취득
							TALENT:LOCALS ^= 1
							;이벤트구상：지식취득
							CALL KOJO_EVENT(110, LOCAL)
							CALL VOIDLINE_IF(RESULT)
							;地の文表示
							PRINTFORMW %CALLNAME%(은)는 [%LOCALS%](을)를 취득했습니다
						ENDIF
					;마력不발時
					CASE 1
						CLEARLINE 1
						REUSELASTLINE 마력이 부족한 것 같다…(마력{LOCAL:1}が필요)
						GOTO INPUT_LOOP_SKILL
					;죠교레벨不발時
					CASE 2
						CLEARLINE 1
						REUSELASTLINE %LOCALS:1%(죠교LV{GAIN_TALENT_V(LOCALS, 1)}が필요)
						GOTO INPUT_LOOP_SKILL
					;その他の問題
					CASEELSE
						CLEARLINE 1
						GOTO INPUT_LOOP_SKILL
				ENDSELECT
			CASEELSE
				CLEARLINE 1
				GOTO INPUT_LOOP_SKILL
		ENDSELECT
	CASE 4
		;이벤트구상：技術を得る前
		CALL KOJO_EVENT(110, 7)
		PRINTFORML %CALLNAME%(은)는 마력을 사용해 새로운 기술을 얻기로 했다…
		PRINTL [ 0] - 그만둔다
		PRINTFORML \@ TALENT:혀기술          ? %"[--] [혀기술]을 가지고 있습니다"    , 51, LEFT% # %"[ 1] [혀기술]を特に巧みに"      , 51, LEFT   % 마력{GAIN_TALENT_V("혀기술"        , 0), 5} 요LV{GAIN_TALENT_V("혀기술"        , 1)} \@
		PRINTFORML \@ TALENT:손가락기술      ? %"[--] [손가락기술]을 가지고 있습니다", 51, LEFT% # %"[ 2] [손가락기술]を会得"        , 51, LEFT   % 마력{GAIN_TALENT_V("손가락기술"    , 0), 5} 요LV{GAIN_TALENT_V("손가락기술"    , 1)} \@
		PRINTFORML \@ TALENT:허리기술          ? %"[--] [허리기술]을 가지고 있습니다    ", 51, LEFT% # %"[ 3] [허리기술]をより厭らしく"    , 51, LEFT   % 마력{GAIN_TALENT_V("허리기술"        , 0), 5} 요LV{GAIN_TALENT_V("허리기술"        , 1)} \@
		PRINTFORML \@ TALENT:긴박능숙        ? %"[--] [긴박능숙]을 가지고 있습니다"  , 51, LEFT% # %"[ 4] [긴박능숙]に손を染める"    , 51, LEFT   % 마력{GAIN_TALENT_V("긴박능숙"      , 0), 5} 요LV{GAIN_TALENT_V("긴박능숙"      , 1)} \@
		PRINTFORML \@ TALENT:새드            ? %"[ 5] [새드]の道から발を洗う"    , 51, LEFT% # %"[ 5] [새드]の悦びを知る"        , 51, LEFT% \@ 마력{GAIN_TALENT_V("새드"          , 0), 5} 요LV{GAIN_TALENT_V("새드"          , 1)}
		PRINTFORML \@ TALENT:착의플레이선호  ? %"[ 6] [착의플레이선호]をやめる"  , 51, LEFT% # %"[ 6] [착의플레이선호]に目覚める", 51, LEFT% \@ 마력{GAIN_TALENT_V("착의플레이선호", 0), 5} 요LV{GAIN_TALENT_V("착의플레이선호", 1)}
		PRINTFORML \@ TALENT:더러움무시        ? %"[--] [더러움무시]을 가지고 있습니다"  , 51, LEFT% # %"[ 7] [더러움무시]が出来るように"  , 51, LEFT   % 마력{GAIN_TALENT_V("더러움무시"      , 0), 5} 요LV{GAIN_TALENT_V("더러움무시"      , 1)} \@
		$INPUT_LOOP_SKILL2
		INPUT
		SELECTCASE RESULT
			CASE 0
				RESTART
			CASE 1 TO 7
				IF 0
				ELSEIF RESULT == 1
					LOCALS:0 = 혀기술
					LOCALS:1 = 人外の혀기술に至るには、まだ경험이 부족한 것 같다…
					LOCAL = 8
				ELSEIF RESULT == 2
					LOCALS:0 = 손가락기술
					LOCALS:1 = 指先を繊細に使うには、まだ경험이 부족한 것 같다…
					LOCAL = 9
				ELSEIF RESULT == 3
					LOCALS:0 = 허리기술
					LOCALS:1 = 허리기술を極めるには、まだ경험이 부족한 것 같다…
					LOCAL = 39
				ELSEIF RESULT == 4
					LOCALS:0 = 긴박능숙
					LOCALS:1 = 구속術の極意に至るには、まだ경험이 부족한 것 같다…
					LOCAL = 10
				ELSEIF RESULT == 5
					LOCALS:0 = 새드
					IF TALENT:새드
						LOCALS:1 = 가학趣味をやめるにしても、まだ少々遊び발りない…
					ELSE
						LOCALS:1 = サディストとなるには、もっと虐げる悦びを知る필요があるだろう…
					ENDIF
					LOCAL = 11
				ELSEIF RESULT == 6
					LOCALS:0 = 착의플레이선호
					IF TALENT:착의플레이선호
						LOCALS:1 = 着衣플레이をやめるにも、少し物발りない気がする…
					ELSE
						LOCALS:1 = 着衣플레이に走る前に、통상の行為をもっと重ねたほうが良いだろう…
					ENDIF
					LOCAL = 38
				ELSEIF RESULT == 7
					LOCALS:0 = 더러움무시
					LOCALS:1 = 汚れを無視できるようになるには、もっと行為に慣れなくてはいけないだろう…
					LOCAL = 12
