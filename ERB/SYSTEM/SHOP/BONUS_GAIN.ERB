;기교上昇処理
;返り値 = 0:成功 1:마력不발 2:죠교레벨不발 3:기교上限
;────────────────────────────────────
@RIZE_ABL

;마력消費量を返す
RESULT:1 = RIZE_ABL_V(0)
;기교上限を判定
SIF ABL:기교 >= 99
	RETURN 3
;마력不발を判定
SIF CFLAG:마력 < RIZE_ABL_V(0)
	RETURN 1
;죠교레벨を判定
SIF CFLAG:죠교레벨 < RIZE_ABL_V(1)
	RETURN 2
RETURN 0

;-------------------------------------
;기교上昇所要마력・죠교레벨
;どちらの条件も満た했다場合のみ기교上昇が可能
;ARG = 0:마력 1:죠교레벨
;-------------------------------------
@RIZE_ABL_V(ARG)
#FUNCTION
IF ARG == 0
	;所要마력
	;0→1:100 1→2:300 2→3:1000 3→4:3000 4→5:10000 ……
	LOCAL = POWER(10, 2 + ABL:기교 / 2)
	SIF ABL:기교 % 2
		LOCAL *= 3
	RETURNF LOCAL
ELSE
	;所要レベル
	;0→1:0 1→2:1 2→3:4 3→4:9 4→5:16 ……
	RETURNF ABL:기교 * ABL:기교
ENDIF

