		IF TALENT:거유
			CALL PRINT_DIALOG(@"%CALLNAME:TARGET%의 가슴を人並みのサイズにします。よろしいですか？",1,1,1)
		ELSE
			CALL PRINT_DIALOG(@"%CALLNAME:TARGET%を[빈유]にします。よろしいですか？",1,1,1)
		ENDIF
		CALL PRINT_CHOICE(,"はい",1,"いいえ",2,)
		IF RESULT == 1
			CFLAG:마력 -= 1000
			TALENT:빈유 = !TALENT:거유
			TALENT:거유 = 0
			CALL KOJO_EVENT(110, 35)
			CALL VOIDLINE_IF(RESULT)
			PRINTFORMW %CALLNAME:TARGET%(은)는 \@ TALENT:빈유 ? 빈유 # 普通のおっぱい \@(이)가 되었습니다
		ENDIF
	CASE 16
		SIF MAXBASE:MASTER:사정 < 2500 + CFLAG:MASTER:정력한계 * 500
			GOTO FAILED
		SIF CFLAG:마력 < (MAXBASE:MASTER:사정 - 2500 - CFLAG:MASTER:정력한계 * 500) / 500 * 1000 + 1000
			GOTO FAILED_BY_MP
		CALL PRINT_DIALOG(@"%CALLNAME:MASTER%의 정력한계を少し強化します。よろしいですか？",1,1,0)
		CALL PRINT_DIALOG(@"※精力最大値の伸びしろが増大します。",0,1,0)
		CALL PRINT_DIALOG(@"  精力最大値自体は今後の죠교で鍛えてください。",0,1,1)
		CALL PRINT_CHOICE(,"はい",1,"いいえ",2,)
		IF RESULT == 1
			CFLAG:마력 -= (MAXBASE:MASTER:사정 - 2500 - CFLAG:MASTER:정력한계 * 500) / 500 * 1000 + 1000
			CFLAG:MASTER:정력한계 += (MAXBASE:MASTER:사정 - 2500 - CFLAG:MASTER:정력한계 * 500) / 500 + 1
			CALL KOJO_EVENT(110, 36)
			CALL VOIDLINE_IF(RESULT)
			PRINTFORMW %CALLNAME:MASTER%의 정력의 한계치가 상승했습니다
		ENDIF
	CASEELSE
		GOTO FAILED
ENDSELECT
RESTART


