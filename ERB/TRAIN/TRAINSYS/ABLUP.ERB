
;※죠교자のABLUP処理は存在しないパラメータがあるためところどころ妥協している

;────────────────────────────────────
;能力上昇処理
;────────────────────────────────────
;-------------------------------------------------
;순종ABL
;-------------------------------------------------
@ABLUP_0M
#DIM 굴복難易度
#DIM 굴복EXP
#DIM 굴복보정
#DIM 굴복달성
#DIM 공포難易度
#DIM 공포EXP
#DIM 공포보정
#DIM 공포달성

;순종、굴복ルート
;굴복難易度設定
굴복難易度 = POWER(2, ABL:MASTER:순종) * (100 - TALENT:MASTER:프라이드낮음 * 20 + TALENT:MASTER:프라이드높음 * 20)
;굴복EXP취득
굴복EXP = EXP:MASTER:굴복경험
;굴복각인が不발しているとEXP보정を減らす(1Lvにつき1/2)
LOCAL = (MARK:굴복각인 >= 10) ? 0 # (ABL:MASTER:순종 * 2 - 1 - MARK:굴복각인)
굴복보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)
;굴복달성判定
LOCAL = CUP:MASTER:굴복 + CUP:MASTER:공순
굴복달성 = (LOCAL / 2 + 굴복EXP * 20 / 굴복보정) > (굴복難易度 * ABLUP_DIFFICULTY(0))

;공포ルート
;공포難易度設定
공포難易度 = POWER(2, ABL:MASTER:순종) * (100 - TALENT:MASTER:겁쟁이 * 20 + TALENT:MASTER:굳셈 * 20)
;공포EXP취득
공포EXP = EXP:MASTER:공포경험
;트라우마が不발しているとEXP보정を減らす(1Lvにつき1/2)
LOCAL = (MARK:트라우마 >= 10) ? 0 # (ABL:MASTER:순종 * 2 - 1 - MARK:트라우마)
공포보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)
;공포달성判定
공포달성 = (CUP:MASTER:공포 + 공포EXP * 20 / 공포보정) > (공포難易度 * ABLUP_DIFFICULTY(0))

;굴복系・공포系で条件を満たせばABLUP
SIF 굴복달성 || 공포달성
	CALL ABLUP_RISE(MASTER, 0)


;-------------------------------------------------
;욕망ABL
;-------------------------------------------------
@ABLUP_1M
CALL ABLUP_1(MASTER)
@ABLUP_1T
CALL ABLUP_1(TARGET)

@ABLUP_1(ARG)
#DIM 욕망難易度
#DIM 욕망EXP
#DIM 욕망보정

;욕망難易度設定
욕망難易度 = POWER(2, ABL:ARG:욕망) * (100 - TALENT:ARG:쾌감에솔직 * 20 + TALENT:ARG:쾌감을부정 * 20)
;욕망EXP취득(高度なEXPほど보정値が大きい)
욕망EXP = EXP:ARG:Ｃ경험 + EXP:ARG:Ｂ경험 + EXP:ARG:성지식
욕망EXP += EXP:ARG:Ｖ경험 * 2 + EXP:ARG:Ａ경험 * 2 + EXP:ARG:절정경험 * 2 + EXP:ARG:사정경험 * 2
욕망EXP += EXP:ARG:자위경험 * 3 + EXP:ARG:분유경험 * 4 + EXP:ARG:이상경험 * 4 + EXP:ARG:Ｖ확장경험 * 5 + EXP:ARG:Ａ확장경험 * 5
;죠교대상
IF ARG == MASTER
	;쾌락각인が不발しているとEXP보정を減らす(1Lvにつき1/2)
	LOCAL = (MARK:쾌락각인 >= 10) ? 0 # (ABL:ARG:욕망 * 2 - 1 - MARK:쾌락각인)
;죠교자(妥協)
ELSE
	;죠교레벨が不발しているとEXP보정を減らす(1Lvにつき1/2)
	LOCAL = (CFLAG:MASTER:죠교레벨 >= 10) ? 0 # (ABL:ARG:욕망 * 2 - 1 - CFLAG:MASTER:죠교레벨)
ENDIF
욕망보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;욕망달성判定
;죠교대상
IF ARG == MASTER
	SIF (CUP:ARG:욕정 + 욕망EXP * 10 / 욕망보정) > (욕망難易度 * ABLUP_DIFFICULTY(1))
		CALL ABLUP_RISE(MASTER, 1)
;죠교자(妥協)
ELSE
	SIF (CUP:ARG:쾌Ｃ + CUP:ARG:쾌Ｖ + CUP:ARG:쾌Ａ + CUP:ARG:쾌Ｂ + 욕망EXP * 10 / 욕망보정) > (욕망難易度 * ABLUP_DIFFICULTY(1))
		CALL ABLUP_RISE(TARGET, 1)
ENDIF


;-------------------------------------------------
;기교ABL
;-------------------------------------------------
@ABLUP_2M
CALL ABLUP_2(MASTER)
@ABLUP_2T
CALL ABLUP_2(TARGET)

@ABLUP_2(ARG)
#DIM 기교難易度
#DIM 기교EXP
#DIM 기교보정
#DIM 相손

;봉사相손を代入
IF ARG == MASTER
	相손 = TARGET
ELSE
	相손 = MASTER
ENDIF

;기교難易度設定
기교難易度 = POWER(2, ABL:ARG:기교) * (100 - TALENT:ARG:습득빠름 * 20 + TALENT:ARG:습득느림 * 20)
;기교EXP취득
기교EXP = EXP:ARG:성지식 + EXP:ARG:구음경험 * 2 + EXP:ARG:수음경험 * 2 + EXP:ARG:성교경험 * 2 + EXP:ARG:절정시킨경험 * 4
;죠교레벨が不발しているとEXP보정を減らす(1Lvにつき1/2)
LOCAL = (CFLAG:ARG:죠교레벨 >= 10) ? 0 # (ABL:ARG:기교 * 2 - 1 - CFLAG:ARG:죠교레벨)
기교보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;기교달성判定
LOCAL = CUP:相손:쾌Ｃ + CUP:相손:쾌Ｖ + CUP:相손:쾌Ａ + CUP:相손:쾌Ｂ
SIF (LOCAL / 3 + 기교EXP * 10 / 기교보정) > (기교難易度 * ABLUP_DIFFICULTY(2))
	CALL ABLUP_RISE(ARG, 2)


;-------------------------------------------------
;Ｘ감각ABL
;-------------------------------------------------
@ABLUP_3M
CALL ABLUP_SENSITIVITY(MASTER, 3)
@ABLUP_4M
CALL ABLUP_SENSITIVITY(MASTER, 4)
@ABLUP_5M
CALL ABLUP_SENSITIVITY(MASTER, 5)
@ABLUP_6M
CALL ABLUP_SENSITIVITY(MASTER, 6)
@ABLUP_3T
CALL ABLUP_SENSITIVITY(TARGET, 3)
@ABLUP_4T
CALL ABLUP_SENSITIVITY(TARGET, 4)
@ABLUP_5T
CALL ABLUP_SENSITIVITY(TARGET, 5)
@ABLUP_6T
CALL ABLUP_SENSITIVITY(TARGET, 6)

@ABLUP_SENSITIVITY(대상, ARG)
#DIM 部位
#DIM 감각難易度
#DIM 감각EXP
#DIM 감각보정
#DIM 대상

部位 = ARG - 3
SPLIT "Ｃ/Ｖ/Ａ/Ｂ", "/", LOCALS

;감각難易度設定
감각難易度 = POWER(2, ABL:대상:@"%LOCALS:部位%감각") * (100 - TALENT:대상:@"%LOCALS:部位%민감" * 20 + TALENT:대상:@"%LOCALS:部位%둔감" * 20)
;감각EXP취득
감각EXP = EXP:대상:@"%LOCALS:部位%경험" * 2 + EXP:대상:절정경험
SIF LOCALS:部位 == "Ｃ"
	감각EXP += EXP:대상:절정경험
SIF LOCALS:部位 == "Ｖ"
	감각EXP += EXP:대상:Ｖ확장경험 * 4 + EXP:대상:성교경험 * 2
SIF LOCALS:部位 == "Ａ"
	감각EXP += EXP:대상:Ａ확장경험 * 4 + EXP:대상:성교경험 + EXP:대상:이상경험
SIF LOCALS:部位 == "Ｂ"
	감각EXP += EXP:대상:분유경험 * 3
;죠교대상
IF ARG == MASTER
	;쾌락각인が不발しているとEXP보정を減らす(1Lvにつき1/2)
	LOCAL = (MARK:쾌락각인 >= 10) ? 0 # (ABL:대상:@"%LOCALS:部位%감각" * 2 - 1 - MARK:쾌락각인)
;죠교자(妥協)
ELSE
	;욕망ABLが不발しているとEXP보정を減らす(1Lvにつき1/2)
	LOCAL = (ABL:대상:욕망 >= 10) ? 0 # (ABL:대상:@"%LOCALS:部位%감각" * 2 - 1 - ABL:대상:욕망)
ENDIF
감각보정 = POWER(2, LOCAL < 0 ? 0 # LOCAL)

;감각달성判定
SIF (CUP:대상:@"쾌%LOCALS:部位%" + 감각EXP * 10 / 감각보정) > (감각難易度 * ABLUP_DIFFICULTY(ARG) * 4)
	CALL ABLUP_RISE(대상, ARG)


