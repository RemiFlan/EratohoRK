
;-----------------------------------------------------------
;ACTABLE一括관리
;-----------------------------------------------------------
@GLOBAL_ACTABLE(ARG)
#FUNCTION
#LOCALSIZE 200
#DIM 参照ターン, 200
#DIM DAYTIME관리, 2

;日時更新
IF DAYTIME관리 != TIME || DAYTIME관리:1 != DAY
	VARSET 参照ターン, -1
	DAYTIME관리 = TIME
	DAYTIME관리:1 = DAY
ENDIF

;ターン内初回
IF 参照ターン:ARG != TFLAG:죠교시간
	参照ターン:ARG = TFLAG:죠교시간
	IF PERCENT(CFLAG:(1000+ARG))
		LOCAL:ARG = 1
	ELSE
		LOCAL:ARG = 0
	ENDIF
ENDIF

;CFLAG:1000～1199によるACT봉인判定
ACTABLE_REASON:ARG = 봉인_ACT봉인
IF LOCAL:ARG == 1
	;採用不可
	RESULT = 0
	RETURNF 1
ENDIF

;휴식系ACT봉인
;SANDBOX限定処理
ACTABLE_REASON:ARG = 봉인_SANDBOX휴식봉인
IF FLAG:모드 == 1 && GROUPMATCH(ARG, 90, 91, 92)
	RESULT = 0
	RETURNF 1
ENDIF

