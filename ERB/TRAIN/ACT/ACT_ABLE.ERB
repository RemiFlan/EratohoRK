;一括관리
SIF GLOBAL_ACTABLE(34)
	RETURN RESULT
;죠교자발기所要判定
;죠교자페니스が存在し、かつ발기不발であれば
ACTABLE_REASON:34 = 物理障害_발기不발(攻)
SIF PENIS(TARGET) && TCVAR:발기 < 2
	RETURN 0
;죠교자に페니스がなく、페니스밴드がない
ACTABLE_REASON:34 = 未所持_Ｐバンド
SIF !PENIS(TARGET) && (TEQUIP:페니스밴드 || !ITEM:페니스밴드)
	RETURN 0
;Ｖ사용중
ACTABLE_REASON:34 = 物理障害_장착(受)(Ｖ)
SIF TEQUIP:삼각목마 || TEQUIP:페니스밴드 || TEQUIP:바이브
	RETURN 0
;징계・모드
ACTABLE_REASON:34 = 状況不適合_징계・中
SIF TFLAG:징계・플래그
	RETURN 0
;방침が소프트/이상
ACTABLE_REASON:34 = 방침不適合_턴방침(%POLICY("今")%)
SIF IS_POLICY("今", "소프트/이상")
	RETURN 0
;남자
ACTABLE_REASON:34 = 物理障害_受男性
SIF TALENT:MASTER:남자
	RETURN 0
;体位変更不可
ACTABLE_REASON:34 = 物理障害_성교(攻→受)(体位)
SIF TEQUIP:성교중 == 1 || TEQUIP:성교중 == 4
	RETURN 0
;삽입중(사정するまで抜かないように)
ACTABLE_REASON:34 = 解除規制_성교(受→攻)(全)
SIF (TEQUIP:성교봉사중)
	RETURN 0
ACTABLE_REASON:34 = 解除規制_성교(攻→受)(Ａ)
SIF TEQUIP:성교중 == 6 && !TEQUIP:죠교자페니스밴드
	RETURN 0
;죠교자に벽통과がない（ペニバンも不可）
IF !TALENT:벽통과 || !PENIS(TARGET)
	;죠교대상の하반신겉옷で妨害される
	ACTABLE_REASON:34 = 物理障害_着衣(受)(하반신겉옷)
	SIF CLOTHESTYPE(1, 1) == 2
		RETURN 0
	;죠교대상の하반신속옷で妨害される
	ACTABLE_REASON:34 = 物理障害_着衣(受)(하반신속옷)
	SIF CLOTHESTYPE(1, 0) == 2
		RETURN 0
ENDIF
;죠교자の하반신겉옷で妨害される
ACTABLE_REASON:34 = 物理障害_着衣(攻)(하반신겉옷)
SIF CLOTHESTYPE(0, 1) == 2
	RETURN 0
;촉수
ACTABLE_REASON:34 = 物理障害_촉수
SIF TEQUIP:촉수조교중
	RETURN 0
;죠교대상が처녀で죠교자が주인님でない@ぽ0422test+勝손に調整3
;今後の条件考慮のためこういう案もあるということを기록@これみ
;SIF TALENT:MASTER:처녀 && !TALENT:TARGET:주인님
;	RETURN 0
;実行可能
ACTABLE_REASON:34 = 
RETURN 1

