;────────────────────────────────────
;050,수음/애무強制
;────────────────────────────────────
;リアクション前（Ｓ/Ｎ）（불결）
@ACTION_APPLY_50
;봉사系の행동は거부できるので、反応が出るまで身体の접촉がないようにしておきます
;탈의の処理
;죠교대상が악세서리장착中、1/3の確率か죠교자Ｃ둔감時악세서리ー강제탈의
SIF TEQUIP:MASTER:악세서리 && (!RAND:3 || TALENT:Ｃ둔감)
	TFLAG:강제탈의 = 6
;죠교자착의플레이선호素質がある場合
IF TALENT:착의플레이선호
	;속옷だけ자주탈의
	TFLAG:자주탈의 = 9
;해방素質、または確率(죠교자죠교레벨、욕망、죠교대상매혹で増大)で
ELSEIF TALENT:해방 || (GET_TRAINLV(TARGET) + GET_ABL(TARGET, "욕망") / 10 + TALENT:MASTER:매혹 * 5 > 4 + RAND:5)
	;半々の確率で악세서리－まで、または전라まで자주탈의
	TFLAG:자주탈의 = 7 + RAND:2
ENDIF
;수음派生時
IF TFLAG:ACT파생 == 1
	;불결SOURCE増加
	SOURCE:MASTER:불결 = 50
	;죠교자의 페니스に정액/항문汚れが存在すると불결SOURCE中増加
	SIF STAIN:페니스 & (4 + 8)
		SOURCE:MASTER:불결 += 150
	;죠교자의 페니스に血/粘液汚れが存在すると불결SOURCE大増加
	SIF STAIN:페니스 & (32 + 64)
		SOURCE:MASTER:불결 += 400
	;자주탈의を行わない判定であれば、하반신全部자주탈의に変更
	SIF TFLAG:자주탈의 == 0
		TFLAG:자주탈의 = 5
;普通の애무時
ELSE
	;불결SOURCE増加
	SOURCE:MASTER:불결 = 25
	;죠교자の바기나に정액/항문汚れが存在すると불결SOURCE中増加
	SIF STAIN:바기나 & (4 + 8)
		SOURCE:MASTER:불결 += 150
	;죠교자の바기나に血/粘液汚れが存在すると불결SOURCE大増加
	SIF STAIN:바기나 & (32 + 64)
		SOURCE:MASTER:불결 += 400
	;자주탈의を行わない判定であれば、上＋下겉옷자주탈의に変更
	SIF TFLAG:자주탈의 == 0
		TFLAG:자주탈의 = 3
ENDIF
;안면승마系解除
CALL FACESITTING_SET(0)
;죠교대상자위を中断
TEQUIP:자위중 &= ~1
;죠교자욕망に応じて발기도上昇
TCVAR:MASTER:금회발기 += 100 + GET_ABL(TARGET, "욕망")
TFLAG:경과시간 += 2

;リアクション後（Ｓ/Ｎ）（불결）
@ACTION_APPLY2_50
;REACT분류が要求に応じないの場合(拒絶)
IF IS_COMGRONAME("거부/날뛴다/逃げる")
	;リアクションが자위를 시작한다の場合
	IF IS_NOWCOMNAME("자위를 시작한다")
		;트라우마、굴종SOURCE減少
		TIMES SOURCE:MASTER:트라우마, 0.01
		TIMES SOURCE:MASTER:굴종, 0.10
	;치정先行
	ELSEIF PALAM:MASTER:치정 > PALAM:MASTER:반항
		;트라우마、굴종SOURCE減少
		TIMES SOURCE:MASTER:트라우마, 0.03
		TIMES SOURCE:MASTER:굴종, 0.20
	;반항先行
	ELSE
		;트라우마、굴종SOURCE減少
		TIMES SOURCE:MASTER:트라우마, 0.05
		TIMES SOURCE:MASTER:굴종, 0.15
	ENDIF
;その他の場合(受容)
ELSE
	;快ｘ用보정値(LOCAL:1～4)：애무보정、죠교대상순종、손가락기술素質に応じて上昇(쾌Ｖは計算するが使わない)
	FOR LOCAL, 1, 5
		LOCAL:LOCAL = 100 + ACT_TO(TARGET, LOCAL) * GET_ABL(MASTER, "순종") * (2 + TALENT:MASTER:손가락기술) / 600
	NEXT
	;汚れが相互に移る
	STAIN:MASTER:손 |= STAIN:(PENIS(TARGET) ? 2 # 3)
	STAIN:(PENIS(TARGET) ? 2 # 3) |= STAIN:MASTER:손
	;죠교대상죠교레벨、쾌Ｃ用보정値に応じて접촉SOURCE増加
	SOURCE:MASTER:접촉 += CFLAG:MASTER:죠교레벨 * 10 + LOCAL:1 / 10
	;쾌Ｃ用보정値に応じて애정、쾌ＣSOURCE増加
	SOURCE:MASTER:애정 += LOCAL:1 / 5
	SOURCE:쾌Ｃ += LOCAL:1 * 2
	;애무時
	IF !TFLAG:ACT파생
		;쾌Ｂ用보정値に応じて쾌ＢSOURCE増加
		SOURCE:쾌Ｂ += LOCAL:4 * 2
		;죠교대상기교が高い場合
		SIF ABL:MASTER:기교 > 2
			;쾌Ａ用보정値に応じて쾌ＡSOURCE増加
			SOURCE:쾌Ａ += LOCAL:3 * 2
	ENDIF
	;불결SOURCE増加
	TIMES SOURCE:MASTER:불결 , 1.20
	;積極的・태연하게 한다の場合
	IF IS_COMGRONAME("적극적으로 한다/태연하게 한다")
		;2/3で座り자세が무릎으로 서있다に변화
		IF TCVAR:MASTER:자세 == 2 && RAND:3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:자세 = 10
		ENDIF
		;누움でなければ、1/2で後ろから애무するように
		IF TCVAR:MASTER:자세 != 1 && RAND:2 && TCVAR:MASTER:위치전후 != 3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:위치전후 = 3
		ENDIF
	ENDIF
ENDIF

;────────────────────────────────────
;051,펠라チオ/クンニ強制
;────────────────────────────────────
;リアクション前（Ｓ/Ｎ/Ｈ）（불결）
@ACTION_APPLY_51
;탈의の処理
;죠교자착의플레이선호素質がある場合
IF TALENT:착의플레이선호
	;속옷だけ자주탈의
	TFLAG:자주탈의 = 9
;억압/수줍음素質がある場合
ELSEIF TALENT:억압 || TALENT:수줍음
	;하반신겉옷자주탈의
	TFLAG:자주탈의 = 2
;その他の場合
ELSE
	;하반신全部자주탈의
	TFLAG:자주탈의 = 5
ENDIF
;펠라チオ系強制時
IF TFLAG:ACT파생 == 1 || TFLAG:ACT파생 == 4
	;불결SOURCE増加
	SOURCE:MASTER:불결 = 150
	;죠교자의 페니스に정액/항문汚れが存在すると불결SOURCE中増加
	SIF STAIN:페니스 & (4 + 8)
		SOURCE:MASTER:불결 += 280
	;죠교자의 페니스に血/粘液汚れが存在すると불결SOURCE大増加
	SIF STAIN:페니스 & (32 + 64)
		SOURCE:MASTER:불결 += 650
;안면승마항문強制時
ELSEIF TFLAG:ACT파생 == 3
	;불결SOURCE増加
	SOURCE:MASTER:불결 = 1000
;クンニ系強制時
ELSE
	;불결SOURCE増加
	SOURCE:MASTER:불결 = 100
	;죠교자の바기나に정액/항문汚れが存在すると불결SOURCE中増加
	SIF STAIN:바기나 & (4 + 8)
		SOURCE:MASTER:불결 += 230
	;죠교자の바기나に血/粘液汚れが存在すると불결SOURCE大増加
	SIF STAIN:바기나 & (32 + 64)
		SOURCE:MASTER:불결 += 620
ENDIF
;안면승마強制時
IF TFLAG:ACT파생 == 2
	;안면승마상태をセット
	CALL FACESITTING_SET(1)
;안면승마항문強制時
ELSEIF TFLAG:ACT파생 == 3
	;안면승마상태をセット
	CALL FACESITTING_SET(2)
;その他の場合
ELSE
	;안면승마系解除
	CALL FACESITTING_SET(0)
ENDIF
;죠교자자위を中断
TEQUIP:자위중 &= ~2
;죠교자욕망に応じて발기도上昇
TCVAR:MASTER:금회발기 += 100 + GET_ABL(TARGET, "욕망")
TFLAG:경과시간 += 2

;リアクション後（Ｓ/Ｎ/Ｈ）（불결）
@ACTION_APPLY2_51
;REACT분류が要求に応じないの場合(拒絶)
IF IS_COMGRONAME("거부/날뛴다/逃げる")
	;치정先行
	IF PALAM:MASTER:치정 > PALAM:MASTER:반항
		;트라우마、굴종SOURCE減少
		TIMES SOURCE:MASTER:트라우마, 0.03
		TIMES SOURCE:MASTER:굴종, 0.20
	;반항先行
	ELSE
		;트라우마、굴종SOURCE減少
		TIMES SOURCE:MASTER:트라우마, 0.05
		TIMES SOURCE:MASTER:굴종, 0.15
	ENDIF
;その他の場合(受容)
ELSE
	;コマンド派生によって分岐
	SELECTCASE TFLAG:ACT파생
		;펠라チオ強制
		CASE 1
			;애무보정、죠교대상순종、혀기술、거친혀素質によって보정値上昇
			LOCAL = 250 + ACT_TO(TARGET, 1) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀기술 + TALENT:MASTER:거친혀) / 900
			;汚れが相互に移る
			STAIN:MASTER:입 |= STAIN:페니스
			STAIN:페니스 |= STAIN:MASTER:입
			;보정値に応じて쾌ＣSOURCE増加
			SOURCE:쾌Ｃ = LOCAL * 2
		;안면승마항문
		CASE 3
			;애무보정、죠교대상순종、혀기술、거친혀素質によって보정値上昇
			LOCAL = 250 + ACT_TO(TARGET, 3) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀기술 + TALENT:MASTER:거친혀) / 900
			;汚れが相互に移る
			STAIN:MASTER:입 |= STAIN:항문
			STAIN:항문 |= STAIN:MASTER:입
			;보정値に応じて쾌ＡSOURCE増加
			SOURCE:쾌Ａ = LOCAL * 2
		;上記以外(クンニ強制/안면승마)
		CASEELSE
			;애무보정、죠교대상순종、혀기술、거친혀素質によって보정値上昇
			LOCAL = 250 + ACT_TO(TARGET, 1) * GET_ABL(MASTER, "순종") * (3 + TALENT:MASTER:혀기술 + TALENT:MASTER:거친혀) / 900
			;汚れが相互に移る
			STAIN:MASTER:입 |= STAIN:바기나
			STAIN:바기나 |= STAIN:MASTER:입
			;보정値に応じて쾌ＣSOURCE増加
			SOURCE:쾌Ｃ = LOCAL * 2
	ENDSELECT
	;죠교대상죠교레벨、보정値に応じて접촉SOURCE増加
	SOURCE:MASTER:접촉 += CFLAG:MASTER:죠교레벨 * 10 + LOCAL / 10
	;보정値に応じて애정SOURCE増加
	SOURCE:MASTER:애정 += LOCAL / 5
	;불결SOURCE増加
	TIMES SOURCE:MASTER:불결 , 1.20
	;積極的/태연하게 한다の場合
	IF IS_COMGRONAME("적극적으로 한다/태연하게 한다") && TFLAG:ACT파생 == 1
		;2/3で座り자세が무릎으로 서있다に변화
		IF TCVAR:MASTER:자세 == 2 && RAND:3
			TCVAR:MASTER:REACT자세변화 = 1
			TCVAR:MASTER:전회의자세 = TCVAR:MASTER:자세
			TCVAR:MASTER:자세 = 10
		ENDIF
	ENDIF
ENDIF

