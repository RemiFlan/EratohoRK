;────────────────────────────────────
;メイン죠교画面の表示、죠교자행동を呼び出し、そしてCOMABLE前の필요な処理
;────────────────────────────────────
@SHOW_STATUS
#DIM LINE
#DIM PREV_BASE_M, 9
#DIM PREV_BASE_T, 9
#DIM NOW_BASE_M, 9
#DIM NOW_BASE_T, 9
#DIM PREV_B_M
#DIM PREV_B_T
#DIM NOW_B_M
#DIM NOW_B_T
#DIM 시간
시간 = 6
;조수１をメイン조수としておく
ASSI = ASSI:1
IF IS_POLICY("금일", "야외")
	TEQUIP:야외플레이 = 1
	TFLAG:금일의방침 = 0
ENDIF

DRAWLINE
;-----------EDIT 004 START--------------;無駄にボタン化するのを回避@revkoishi(14/05/28)
PRINTPLAINFORM {DAY + 1}일째(%GET_TIME()%) 경과시간[{TFLAG:경과시간}] 
SIF TALENT:MASTER:독심능력 > 1
	PRINTPLAINFORM / 징계・\@ TFLAG:징계・플래그 ? 모드 # 포인트[{TFLAG:징계・포인트}]\@
PRINTL 
;-----------EDIT 004 END  --------------
FOR LOCAL, 0, 9
	NOW_BASE_M:LOCAL = BASE:MASTER:LOCAL
	NOW_BASE_T:LOCAL = BASE:TARGET:LOCAL
NEXT
NOW_B_M = TCVAR:MASTER:발기도
NOW_B_T = TCVAR:TARGET:발기도
REDRAW 0
LINE = LINECOUNT
FOR LOCAL:1, TFLAG:경과시간 ? 0 # 시간, 시간 + 1
	CLEARLINE LINECOUNT - LINE
	SIF MESSKIP() || FLAG:PALAM애니메 == 2
		LOCAL:1 = 시간
	IF LOCAL:1 == 시간
		FOR LOCAL, 0 ,9
			BASE:MASTER:LOCAL = NOW_BASE_M:LOCAL
			BASE:TARGET:LOCAL = NOW_BASE_T:LOCAL
		NEXT
		TCVAR:MASTER:발기도 = NOW_B_M 
		TCVAR:TARGET:발기도 = NOW_B_T
	ELSE
		FOR LOCAL, 0, 9
			BASE:MASTER:LOCAL = (NOW_BASE_M:LOCAL * LOCAL:1 + PREV_BASE_M:LOCAL * (시간 - LOCAL:1)) / 시간
			BASE:TARGET:LOCAL = (NOW_BASE_T:LOCAL * LOCAL:1 + PREV_BASE_T:LOCAL * (시간 - LOCAL:1)) / 시간
		NEXT
		TCVAR:MASTER:발기도 = (NOW_B_M * LOCAL:1 + PREV_B_M * (시간 - LOCAL:1)) / 시간
		TCVAR:TARGET:발기도 = (NOW_B_T * LOCAL:1 + PREV_B_T * (시간 - LOCAL:1)) / 시간
	ENDIF
	CALL INFO_GAUGE_TRAINER
	;CALL INFO_GAUGE_TENTACLE
	DRAWLINE

	;服装の表示
	CALL SHOW_EQUIP_3
	;修正前の자세を表示
	CALL POSE_PRINT
	;사용중の도구を表示
	CALL SHOW_EQUIP_1
	;状況の表示
	CALL SHOW_EQUIP_2
	DRAWLINE
	TWAIT 100, 1
NEXT
FOR LOCAL, 0, 9
	PREV_BASE_M:LOCAL = BASE:MASTER:LOCAL
	PREV_BASE_T:LOCAL = BASE:TARGET:LOCAL
NEXT
;-----------EDIT 003 START--------------
;-----------EDIT 001 START--------------
;조교중止の判定自体がACTを決定했다あとに
;そのあとで内容を全部潰すとかやってたので独立시킨다
;---------------------------------------
;조교중止の判定
;---------------------------------------
SIF TFLAG:경과시간 == 0
	CALL TRAIN_CANCEL
;-----------EDIT 001 END  --------------
;-----------EDIT 003 END  --------------
PREV_B_M = TCVAR:MASTER:발기도
PREV_B_T = TCVAR:TARGET:발기도
REDRAW 1
;-----------EDIT 001 START--------------
;조수関数に引数を設けつつ、TCVARに플래그を移して処理を簡易化し
;また、メッセージの表示タイミング及び구상の表示タイミングを
;すべてのコマンド決定後に変更
;---------------------------------------
;죠교자행동の表示
IF TFLAG:조교자행동제++ == 0 && !TFLAG:일회휴게
	;죠교자の행동を선택します。
	CALL PLAYER_ACT
	;-----------EDIT 001 START--------------
	;現在ACT名の취득を関数を通시킨다ように
	;---------------------------------------
	TSTR:죠교指令 = %NOWACTNAME()%
	;-----------EDIT 001 END  --------------
	;행동前判定
	CALL BEFORE_ACT
	;各죠교アクションの누적値
	CFLAG:TARGET:(2000 + TFLAG:ACT) += 1
	CFLAG:MASTER:(2000 + TFLAG:ACT) += 1
	;선택당했다행동によってMASTERの体勢と사용중の体の部位を기록します。
	CALL MASTER_POSE
	CALL PLAYER_ACT_EXTRA_LATER
	;전회의행동を表示して今回の행동を決定
	IF STRLENS(TSTR:前죠교指令) || (STRLENS(TSTR:죠교指令) && FLAG:디버그)
		IF FLAG:디버그
			SIF STRLENS(TSTR:前죠교指令)
				PRINTFORML 전회의 조교 지령：(%MENU(TFLAG:전ACT분류)%계)＜%TSTR:前죠교指令%＞
			PRINTFORML 이번 조교 지령：(%MENU(TFLAG:ACT분류)%계)＜%TSTR:죠교指令%＞
		ELSE
			PRINTFORML 전회의 조교 지령：＜%TSTR:前죠교指令%＞
		ENDIF
		DRAWLINE
	ENDIF
	CALL EQUIP_2USE
	CALL SP_CHECK
	;선택당했다행동が派生する場合、その플래그を立てます
	CALL TRAIN_VARIATION
	IF TFLAG:ＳＰ행동 < 200
		;PALAM,발기도などの計算
		CALL ACTION_APPLY
	ELSE
		CALL SP_ACTION
	ENDIF
	SIF TFLAG:강제탈의 > 0
		CALL KYOUSEI_DATUI
	SIF TFLAG:자주탈의 > 0
		CALL JISHU_DATUI
	;접촉情報の취득、地の文表示
	CALL TOUCH_SUCCESSION
	;----------------------------------------
	;조수の행동決定
	;----------------------------------------
	LOCAL = 1
	WHILE ASSI:LOCAL > 0
		;조수행동の決定
		IF TCVAR:ASSI:행동예약 >= 0
			TCVAR:(ASSI:LOCAL):조수ACT = TCVAR:ASSI:행동예약
			TCVAR:ASSI:행동예약 = -1
		ELSE
			CALL ASSI_ACT, ASSI:LOCAL
		ENDIF
		;MASTERの体勢の情報を上書き
		CALL MASTER_POSE_ASSI, ASSI:LOCAL
		CALL ASSI_ACT_APPLY, ASSI:LOCAL
		;派生情報の취득
		CALL ASSI_TRAIN_VARIATION, ASSI:LOCAL, TCVAR:(ASSI:LOCAL):조수ACT
		;횟수カウントの追加
		IF TCVAR:(ASSI:LOCAL):조수방침 >= 0 && GET_ASSIMENUNAME(TCVAR:(ASSI:LOCAL):조수방침) != "コンビネーション"
			CFLAG:(ASSI:LOCAL):(2200 + TCVAR:(ASSI:LOCAL):조수ACT) += 1
			CFLAG:MASTER:(2200 + TCVAR:(ASSI:LOCAL):조수ACT) += 1
		ENDIF
		LOCAL++
	WEND
	;강제탈의の成功判定
	SIF TFLAG:강제탈의 > 0
		CALL KYOUSEI_DATUI_CHECK
	;----------------------------------------------
	;メッセージ表示
	;----------------------------------------------
	;-----------EDIT 002 START--------------
	;탈의実行処理と地の文、구상表示処理を分離
	;---------------------------------------
	;強制・자주탈의の実行、また地の文表示とその구상
	SIF TFLAG:자주탈의 || TFLAG:강제탈의
		CALL DATUI_PROCESS
	;-----------EDIT 002 END  --------------
	;자세の変更の地の文とその구상
	CALL POSE_CHANGE
	;ACTの地の文
	CALL TRAIN_MESSAGE
	;죠교자アクションの表示とその구상
	CALL KOJO_ACT
	;追加コマンドのメッセージ
	CALL PLAYER_ACT_EXTRA_MESSAGE
	;----------------------------------
	;조수のテ키스ト表示
	;----------------------------------
	LOCAL = 1
	WHILE ASSI:LOCAL > 0
		ALIGNMENT RIGHT
		;구상用に互換処理
		ASSI = ASSI:LOCAL
		TFLAG:조수ACT = TCVAR:(ASSI:LOCAL):조수ACT
		TFLAG:조수ACT파생 = TCVAR:(ASSI:LOCAL):조수ACT파생
		CALL ASSI_TRAIN_MESSAGE, ASSI:LOCAL
		LOCAL++
		ALIGNMENT LEFT
	WEND
	ASSI = ASSI:1
	;미약・로션使用とその구상
	SIF TFLAG:미약등사용
		CALL USE_MESSAGE
	CALL EQUIP_CHECK
	CALL CLOTHES_CHECK
	DRAWLINE
	;ここでリアクション派生をリセットしておく
	TFLAG:REACT파생 = 0
ELSE
	SIF STRLENS(TSTR:前죠교指令)
		PRINTFORML 전회의 조교 지령＜%TSTR:2%＞
	PRINTFORML 이번 조교 지령＜%TSTR:1%＞
	DRAWLINE
ENDIF
;-----------EDIT 001 END  --------------
SIF FLAG:디버그
	CALL SHOW_TOUCH
;COMABLE処理
CALL COMABLE(-1)
IF FLAG:오토모드 > 0
	CALL AUTO_MODE
	LOCAL = RESULT
	DOTRAIN LOCAL
ENDIF
@AUTO_MODE, ARG
#DIMS AUTO, 100
;ARGが真の時、あるいはAUTOが空っぽの時、AUTOを更新する
IF AUTO == "" || ARG
	VARSET AUTO
	SPLIT GLOBALS:@"오토모드등록{FLAG:선택세트}", "_", RESULTS
	SPLIT RESULTS:1, "/", AUTO
	SIF ARG
		RETURN 0
ENDIF
FOR LOCAL, 0, 100
	SIF GETNUM(TRAINNAME,AUTO:LOCAL) == -1
		CONTINUE
	CALL COMABLE(GETNUM(TRAINNAME,AUTO:LOCAL))
	SIF !RESULT
		CONTINUE
	RETURN GETNUM(TRAINNAME,AUTO:LOCAL)
	BREAK
NEXT
RETURN GETNUM(TRAINNAME, "이루는대로")
