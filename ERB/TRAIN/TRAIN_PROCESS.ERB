;행동前判定（EVETRAINより移動）
;2011/10/14		휴식による終了を切り離して別関数にする
;────────────────────────────────────
@BEFORE_ACT
;죠교자행동前구상
CALL BEFORE_ACT_MESSAGE
CALL KOJO_EVENT(12)
IF RESULT
	DRAWLINE
ELSE
	WAIT
ENDIF
;────────────────────────────────────
;행동前判定（EVETRAINより移動）
;2011/10/14		BEFORE_ACTから切り出して独立했다関数に変更
;────────────────────────────────────
@TRAIN_CANCEL
IF IS_POLICY("금일", "휴식")
	;죠교자행동前구상・ご褒美による中止
	CALL KOJO_EVENT(12, 3)
	IF RESULT < 2
		SIF RESULT
			PRINTL 
		PRINTFORML %CALLNAME:TARGET%(은)는 오늘의 조교를 중지해,
		PRINTFORML %CALLNAME:MASTER%멈추거나로 한 시간을 보냈다…
	ENDIF
	CALL _BEFORE_ACT_SUB1
ELSEIF CFLAG:소모 > 5
	;죠교자행동前구상・죠교자の疲労による中止
	CALL KOJO_EVENT(12, 1)
	IF RESULT < 2
		SIF RESULT
			PRINTL 
		PRINTFORML 조교의 시간이 왔지만, 들어 왔다%CALLNAME:TARGET%(은)는 어쩐지 피곤한 모습으로,
		PRINTFORML 이번은 휴일이라고 말해 나와 갔다…
	ENDIF
	CALL _BEFORE_ACT_SUB1
;SANDBOX限定処理 체력不발で조교중止
ELSEIF CFLAG:MASTER:소모 > 5 || (FLAG:모드 == 1 && BASE:MASTER:체력 * 100 / MAXBASE:MASTER:체력 < 30)
	;죠교자행동前구상・죠교대상の疲労による中止
	CALL KOJO_EVENT(12, 2)
	IF RESULT < 2
		SIF RESULT
			PRINTL 
		PRINTFORML 조교의 시간이 왔지만,%CALLNAME:TARGET%(은)는 심한 모습의 %CALLNAME:MASTER%(을)를 봐 당분간 생각하면(자),
		PRINTFORML 이번은 쉬어도 좋다고 말해 나와 갔다…
	ENDIF
	CALL _BEFORE_ACT_SUB1
ENDIF

@_BEFORE_ACT_SUB1
DRAWLINE
CFLAG:TARGET:소모 = 0
CFLAG:MASTER:소모 = 0
TFLAG:조교자행동제 = 1
TFLAG:턴방침 = -1
TFLAG:ACT분류 = -1
TFLAG:ACT = -1
TFLAG:일회휴게 = 1


;────────────────────────────────────
;汚れ表示
;────────────────────────────────────
@STAIN_INFO
CALL _STAIN_INFO_SUB1(MASTER)
CALL _STAIN_INFO_SUB1(TARGET)
CALL _STAIN_INFO_SUB1(ASSI:1)
CALL _STAIN_INFO_SUB1(ASSI:2)
CALL _STAIN_INFO_SUB1(ASSI:3)
WAIT

@_STAIN_INFO_SUB1(ARG)
#LOCALSSIZE 20
SIF ARG < 0
	RETURN 0
SIF !STRLENS(LOCALS)
	SPLIT "입/손/Ｐ/Ｖ/Ａ/Ｂ/발/Ｖ/Ｐ/精/Ａ/乳/血/粘液", "/", LOCALS
FOR LOCAL, 0, 7
	;페니스が無い場合、Ｐは処理しない。남자の場合、ＶとＢは処理しない
	SIF (LOCAL == 2 && !PENIS(ARG)) || ((LOCAL == 3 || LOCAL == 5) && TALENT:ARG:남자)
		CONTINUE
	PRINTFORM %CALLNAME:ARG%의 %LOCALS:LOCAL%：
	FOR LOCAL:1, 0, 7
		PRINTFORM \@ GETBIT(STAIN:ARG:LOCAL, LOCAL:1) ? <%LOCALS:(7 + LOCAL:1)%> # \@
	NEXT
	PRINTL 
NEXT


